breakpoint.mdAndUp,\n        }\"\n      >\n        <v-card\n          :class=\"{\n            'px-8': $vuetify.breakpoint.smAndDown,\n            'py-4': $vuetify.breakpoint.smAndDown,\n          }\"\n          flat\n        >\n          <v-form @keyup.enter=\"lookupOrder\">\n            <o-text\n              :xl=\"$vuetify.breakpoint.smAndDown\"\n              :xl2=\"$vuetify.breakpoint.mdAndUp\"\n              bold\n              class=\"\"\n              :class=\"{\n                'pb-4 pt-6': $vuetify.breakpoint.smAndDown,\n                'pb-2 pt-2': $vuetify.breakpoint.mdAndUp,\n              }\"\n            >\n              {{ RETURN_AND_TRACK_ORDERS }}\n            </o-text>\n            <o-text-field\n              :title=\"ENTER_ORDER_ID\"\n              id=\"orderId\"\n              @onChange=\"onChange($event)\"\n              @onKeyEnter=\"lookupOrder\"\n              @onPaste=\"onPaste($event)\"\n            />\n            <action-button\n              @click.prevent=\"lookupOrder\"\n              class=\"mt-n2\"\n              primary-color\n              :disabled=\"isDisabled\"\n              block\n              x-large\n            >\n              {{ NEXT }}\n            </action-button>\n\n            <div\n              :class=\"{\n                'mt-5': $vuetify.breakpoint.smAndDown,\n                'mt-4': $vuetify.breakpoint.mdAndUp,\n                'd-flex justify-content-between': this.isGiftAllowed,\n                'd-flex justify-center': !this.isGiftAllowed,\n              }\"\n            >\n              <div>\n                <action-button\n                  @click.stop=\"onGiftReturnClick()\"\n                  tertiary-color\n                  v-show=\"this.isGiftAllowed\"\n                  text\n                >\n                  {{ RETURN_A_GIFT }}\n                </action-button>\n                <ReturnGiftView\n                  :visible=\"showReturnGift\"\n                  @close=\"showReturnGift = false\"\n                />\n              </div>\n              <div>\n                <action-button\n                  @click.stop=\"showHowItWorks = true\"\n                  tertiary-color\n                  text\n                >\n                  {{ HOW_IT_WORKS }}\n                </action-button>\n                <HowItWorksView\n                  :visible=\"showHowItWorks\"\n                  @close=\"showHowItWorks = false\"\n                />\n              </div>\n            </div>\n          </v-form>\n        </v-card>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport ReturnGiftView from '@/components/pages/ReturnGiftView.vue'\nimport HowItWorksView from '@/components/pages/HowItWorksView.vue'\nimport heroImage from 'branding/assets/images/hero.png'\nimport HeroImage from '@/components/molecules/HeroImage.vue'\nimport OTextField from '@/components/molecules/OTextField.vue'\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport OText from '@/components/atoms/OText.vue'\nimport { validateOrderId } from '@/utils'\nimport { ORDER_LOOKUP } from 'branding/strings.json'\nimport { GET_CONFIG } from '@/store/constants.js'\n\nexport default {\n  name: 'Home',\n  components: {\n    ReturnGiftView,\n    HowItWorksView,\n    HeroImage,\n    OTextField,\n    ActionButton,\n    OText\n  },\n  data () {\n    return {\n      orderId: '',\n      isDisabled: true,\n      showReturnGift: false,\n      showHowItWorks: false,\n      heroImage,\n      RETURN_AND_TRACK_ORDERS: ORDER_LOOKUP.RETURN_AND_TRACK_ORDERS,\n      ENTER_ORDER_ID: ORDER_LOOKUP.ENTER_ORDER_ID,\n      RETURN_A_GIFT: ORDER_LOOKUP.RETURN_A_GIFT,\n      HOW_IT_WORKS: ORDER_LOOKUP.HOW_IT_WORKS,\n      NEXT: ORDER_LOOKUP.NEXT,\n      isGiftAllowed: false\n    }\n  },\n  mounted () {\n    this.orderId = ''\n    this.$store.commit('ui/updateOrderId', '')\n    this.$store\n      .dispatch('order/' + GET_CONFIG, null)\n      .then((response) => {\n        if (response) {\n          this.isGiftAllowed = true // response?.gift_returns_allowed\n        }\n      })\n      .catch((err) => {\n        console.error({ err })\n      })\n  },\n  methods: {\n    validate (value) {\n      this.orderId = value\n      this.isDisabled = !validateOrderId(value)\n    },\n    onChange (event) {\n      this.validate(event.target.value)\n    },\n    onPaste (event) {\n      this.validate(event.clipboardData.getData('text/plain'))\n    },\n    async lookupOrder () {\n      this.$store.commit('ui/updateOrderId', this.orderId)\n      this.$router.push('/zipcode')\n      this.$store.commit('ui/updateIsGiftReturn', false)\n    },\n    onGiftReturnClick () {\n      if (validateOrderId(this.orderId)) {\n        this.$store.commit('ui/updateOrderId', this.orderId)\n      }\n      this.$router.push('/email')\n      this.$store.commit('ui/updateIsGiftReturn', true)\n    }\n  }\n}\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components/pages"}]}