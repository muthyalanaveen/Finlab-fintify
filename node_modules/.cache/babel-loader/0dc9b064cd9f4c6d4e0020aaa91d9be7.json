CA,KADA,EACA;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,MAAA,CAAA,KAAA;AACA,WAAA,KAAA,IAAA,KAAA;AACA,KAJA;AAKA,IAAA,QAAA,EAAA,kBAAA,CAAA,EAAA;AACA,WAAA,MAAA,GAAA,EAAA;;AAEA,UAAA,CAAA,KAAA,QAAA,EAAA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,UAAA,CAAA,4BAAA;AACA;;AACA,UAAA,CAAA,KAAA,UAAA,EAAA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,UAAA,CAAA,4BAAA;AACA;;AAEA,UAAA,CAAA,KAAA,QAAA,IAAA,CAAA,KAAA,UAAA,EAAA;AACA;AACA;;AAEA,UACA,KAAA,QAAA,CAAA,IAAA,GAAA,WAAA,OAAA,QAAA,IACA,KAAA,UAAA,KAAA,aAFA,EAGA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,iBAAA,EAAA,MAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,eAAA,IAAA;AACA,OAPA,MAOA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,UAAA,CAAA,6BAAA;AACA;;AAEA,MAAA,CAAA,CAAA,cAAA;AACA;AA/BA,GAXA;AA4CA,EAAA,OA5CA,qBA4CA;AACA;AACA,QAAA,eAAA,GAAA,YAAA,CAAA,OAAA,CAAA,iBAAA,CAAA;;AACA,QAAA,eAAA,KAAA,MAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA;AAlDA,CAAA","sourcesContent":["<template>\n  <div class=\"d-flex justify-center align-center\" style=\"min-height: 100vh; \">\n    <v-container>\n      <header-title style=\"text-align: center\" class=\"mt-12\">\n        {{ LOGIN_PAGE.PAGE_HEADING }}\n      </header-title>\n      <v-form @keyup.enter=\"onSubmit\">\n        <v-row class=\"justify-center\">\n          <v-col\n            cols=\"12\"\n            sm=\"6\"\n            md=\"3\"\n          >\n            <o-text-field\n              label=\"username\"\n              id=\"username\"\n              v-model=\"username\"\n              @onChange=\"onChange($event, 'username')\"\n              @onKeyEnter=\"onSubmit\"\n            />\n          </v-col>\n        </v-row>\n        <v-row class=\"justify-center\">\n          <v-col\n            cols=\"12\"\n            sm=\"6\"\n            md=\"3\"\n          >\n            <o-text-field\n              label=\"password\"\n              id=\"password\"\n              v-model=\"secureText\"\n              type=\"password\"\n              @onChange=\"onChange($event, 'secureText')\"\n              @onKeyEnter=\"onSubmit\"\n            />\n          </v-col>\n        </v-row>\n        <v-row class=\"justify-center\">\n          <v-col\n            cols=\"12\"\n            sm=\"6\"\n            md=\"3\"\n          >\n            <div v-if=\"errors.length\">\n              <o-text\n                v-for=\"error in errors\"\n                :key=\"error\"\n                text-red\n              >\n                {{ error }}\n              </o-text>\n            </div>\n          </v-col>\n        </v-row>\n        <v-row class=\"justify-center\">\n          <v-col\n            cols=\"12\"\n            sm=\"6\"\n            md=\"3\"\n          >\n            <div class=\"d-flex justify-center\">\n              <action-button\n                @click.prevent=\"onSubmit\"\n                primary-color\n                x-large\n                block\n              >\n                {{ LOGIN_PAGE.ACTION_BUTTON }}\n              </action-button>\n            </div>\n          </v-col>\n        </v-row>\n      </v-form>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\nimport OTextField from '@/components/molecules/OTextField.vue'\nimport OText from '@/components/atoms/OText.vue'\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport { appSecureText } from '@/common/config'\nimport { LOGIN_PAGE } from 'branding/strings.json'\nexport default {\n  name: 'Login',\n  components: { HeaderTitle, OTextField, ActionButton, OText },\n  data () {\n    return {\n      username: '',\n      secureText: '',\n      errors: [],\n      LOGIN_PAGE\n    }\n  },\n  methods: {\n    onChange (e, field) {\n      const value = e.target.value\n      this[field] = value\n    },\n    onSubmit: function (e) {\n      this.errors = []\n\n      if (!this.username) {\n        this.errors.push(LOGIN_PAGE.USERNAME_REQUIRED_VALIDATION)\n      }\n      if (!this.secureText) {\n        this.errors.push(LOGIN_PAGE.PASSWORD_REQUIRED_VALIDATION)\n      }\n\n      if (!this.username || !this.secureText) {\n        return\n      }\n\n      if (\n        this.username.trim().toLowerCase() === 'optoro' &&\n        this.secureText === appSecureText\n      ) {\n        localStorage.setItem('isAuthenticated', 'true')\n        this.$router.push('/')\n        return true\n      } else {\n        this.errors.push(LOGIN_PAGE.INCORRECT_PASSWORD_VALIDATION)\n      }\n\n      e.preventDefault()\n    }\n  },\n  created () {\n    // Check if user has already logged in\n    const isAuthenticated = localStorage.getItem('isAuthenticated')\n    if (isAuthenticated === 'true') {\n      this.$router.push('/')\n    }\n  }\n}\n</script>\n\n<style></style>\n"],"sourceRoot":"src/components/pages"}]}