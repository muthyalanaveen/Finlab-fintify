AZA;AAaA;AACA,GAvFA;AAwFA,EAAA,gBAxFA,4BAwFA,EAxFA,EAwFA,IAxFA,EAwFA,IAxFA,EAwFA;AACA,QAAA,CAAA,EAAA,CAAA,IAAA,EAAA;AACA,MAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA;AACA;AACA;;AACA,IAAA,IAAA;AACA,GA9FA;AA+FA,EAAA,OA/FA,qBA+FA,CAAA;AA/FA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"mt-8\">\n      <div style=\"max-width: 664px\" class=\"mx-auto\">\n        <v-row\n          justify=\"center\"\n          class=\"mb-4 mt-0 mx-auto\"\n          :class=\"{}\"\n        >\n          <div\n            class=\"bg-well text-center justify-center align-center mt-4 py-8\"\n          >\n            <div class=\"d-flex justify-center\">\n              <v-img\n                :src=\"require('@/assets/images/confirmed.png')\"\n                max-width=\"56\"\n                max-height=\"56\"\n              />\n            </div>\n            <o-text\n              lg\n              text-green\n              bold\n              class=\"mt-2\"\n            >\n              {{ RETURN_CONFIRMED }}\n            </o-text>\n            <o-text sm class=\"mt-1\">\n              {{ CONFIRMATION_EMAIL_MESSAGE + order.shopper_email }}\n            </o-text>\n            <div class=\"d-flex mt-4 align-center justify-center\">\n              <action-button\n                @click.stop=\"openReturnLabelLink\"\n                secondary-color\n                outlined\n                :loading=\"isLoading\"\n                :disabled=\"isLoading\"\n                v-if=\"this.selectedReturnMethod.type === MAIL_BACK || this.selectedReturnMethod.type === EXPRESS_RETURNS\"\n              >\n                {{\n                  this.selectedReturnMethod.type === MAIL_BACK\n                    ? PRINT_RETURN_LABEL\n                    : VIEW_RETURN_CODES_BUTTON\n                }}\n              </action-button>\n              <o-dialog\n                :visible=\"showReturnCodes\"\n                is-full-screen\n                @close=\"showReturnCodes = false\"\n              >\n                <QRCodeView\n                  v-if=\"this.selectedReturnMethod.type === EXPRESS_RETURNS\"\n                  :qr-data=\"this.qrData\"\n                  from-return-methods\n                />\n              </o-dialog>\n              <action-button\n                tertiary-color\n                text\n                class=\"ml-4\"\n                @click.stop=\"showReturnDetails = true\"\n              >\n                {{ VIEW_RETURN_DETAILS_BUTTON }}\n              </action-button>\n              <o-dialog\n                :visible=\"showReturnDetails\"\n                is-full-screen\n                @close=\"showReturnDetails = false\"\n              >\n                <ReviewView\n                  is-modal\n                  :show-confirm-action-view=\"false\"\n                  :title=\"getReturnTitle()\"\n                  show-order-details\n                  :return-date=\"getReturnDate()\"\n                />\n              </o-dialog>\n            </div>\n          </div>\n          <!-- <div\n            class=\"text-center mt-6\"\n          >\n            <action-button\n              tertiary-color\n              text\n              class=\"mx-auto\"\n            >\n              {{ CANCEL_RETURN_TITLE }}\n            </action-button>\n            <o-text\n              sm\n              class=\"mt-2\"\n            >\n              {{ CANCEL_RETURN_DESCRIPTION }}\n            </o-text>\n          </div> -->\n        </v-row>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport OText from '@/components/atoms/OText.vue'\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport { CONFIRMATION_VIEW } from 'branding/strings.json'\nimport ODialog from '@/components/atoms/ODialog.vue'\nimport ReviewView from '@/components/pages/ReviewView.vue'\nimport QRCodeView from '@/components/pages/QRCodeView.vue'\nimport { mapGetters } from 'vuex'\nimport { getFormattedDate } from '@/utils'\nimport { GET_SHIPPING_LABEL, GET_QR_CODE_LABEL } from '@/store/constants.js'\nimport { CONSTANTS } from '@/constants/index.js'\n\nexport default {\n  name: 'ConfirmationView',\n\n  components: {\n    OText,\n    ActionButton,\n    ODialog,\n    ReviewView,\n    QRCodeView\n  },\n\n  computed: {\n    ...mapGetters({\n      order: 'order/order',\n      selectedReturnMethod: 'returns/getSelectedReturnMethod'\n    })\n  },\n  data () {\n    return {\n      RETURN_CONFIRMED: CONFIRMATION_VIEW.RETURN_CONFIRMED,\n      CONFIRMATION_EMAIL_MESSAGE: CONFIRMATION_VIEW.CONFIRMATION_EMAIL_MESSAGE,\n      VIEW_RETURN_CODES_BUTTON: CONFIRMATION_VIEW.VIEW_RETURN_CODES_BUTTON,\n      PRINT_RETURN_LABEL: CONFIRMATION_VIEW.PRINT_RETURN_LABEL,\n      VIEW_RETURN_DETAILS_BUTTON: CONFIRMATION_VIEW.VIEW_RETURN_DETAILS_BUTTON,\n      CANCEL_RETURN_TITLE: CONFIRMATION_VIEW.CANCEL_RETURN_TITLE,\n      CANCEL_RETURN_DESCRIPTION: CONFIRMATION_VIEW.CANCEL_RETURN_DESCRIPTION,\n      MAIL_BACK: CONSTANTS.RETURN_METHODS.MAIL_BACK,\n      EXPRESS_RETURNS: CONSTANTS.RETURN_METHODS.EXPRESS_RETURNS,\n      IN_STORE: CONSTANTS.RETURN_METHODS.IN_STORE,\n      showReturnDetails: false,\n      showReturnCodes: false,\n      returnLabelLink: '',\n      isLoading: false,\n      qrData: null\n    }\n  },\n\n  props: {\n  },\n\n  methods: {\n    getReturnTitle () {\n      return 'Order #' + this.order.order_id\n    },\n    getReturnDate () {\n      return 'Date ' + getFormattedDate(this.order?.order?.created_at)\n    },\n    openReturnLabelLink () {\n      this.selectedReturnMethod.type === this.MAIL_BACK ? window.open(this.returnLabelLink) : this.showReturnCodes = true\n    }\n  },\n\n  mounted () {\n    const identifier = this.$route.params.identifier\n    const params = {\n      identifier\n    }\n    this.isLoading = true\n    if (this.selectedReturnMethod.type === this.MAIL_BACK) {\n      this.$store\n        .dispatch('order/' + GET_SHIPPING_LABEL, params)\n        .then((response) => {\n          if (response) {\n            this.returnLabelLink = response.label_url\n          }\n        })\n        .catch((err) => {\n          console.error({ err })\n        })\n        .finally(() => {\n          this.isLoading = false\n        })\n    } else if (this.selectedReturnMethod.type === this.EXPRESS_RETURNS || this.selectedReturnMethod.type === this.IN_STORE) {\n      this.$store\n        .dispatch('order/' + GET_QR_CODE_LABEL, params)\n        .then((response) => {\n          if (response) {\n            this.qrData = response\n          }\n        })\n        .catch((err) => {\n          console.error({ err })\n        })\n        .finally(() => {\n          this.isLoading = false\n        })\n    }\n  },\n  beforeRouteLeave (to, from, next) {\n    if (!to.hash) {\n      window.location.replace('/')\n      return\n    }\n    next()\n  },\n  created () {}\n}\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components/pages"}]}