ts/organism/ProductCard.vue')\n  },\n  props: {\n    filter: {\n      type: Function,\n      default: (v) => !!v\n    },\n    height: {\n      type: [Number, String],\n      default: 125\n    },\n    width: {\n      type: [Number, String],\n      default: 175\n    },\n    cards: {\n      type: Array,\n      required: true\n    },\n    subtitleShow: {\n      type: Boolean,\n      default: true\n    },\n    img: {\n      type: String,\n      default: ''\n    },\n    enableText: {\n      type: Boolean,\n      default: true\n    },\n    onCardSelected: {\n      type: Function,\n      required: false,\n      default: () => {}\n    },\n    previouslySelectedOption: {\n      type: Object,\n      required: false,\n      default: () => {}\n    }\n  },\n\n  data: () => ({\n    reset: false,\n    selectedOption: null\n  }),\n\n  methods: {\n    onObserve (x, y, isVisible) {\n      if (this.reset) {\n        if (this.previouslySelectedOption) {\n          this.selectedOption = this.previouslySelectedOption.sku\n          this.$store.commit('returns/updateReturnReasonSelected', true)\n        } else {\n          this.selectedOption = null\n        }\n        return\n      }\n\n      setTimeout(() => {\n        this.reset = !this.reset\n      }, 100)\n    },\n    onClick (card) {\n      this.$store.commit('returns/updateReturnReasonSelected', true)\n      this.selectedOption = card.identifier\n      this.onCardSelected(card)\n    }\n  }\n}\n</script>\n<style scoped></style>\n"],"sourceRoot":"src/components/organism"}]}