CAAA,SAAA,EAAA;AACA,QAAA,KAAA,GAAA,KAAA,SAAA;AACA,QAAA,IAAA,GAAA,MAAA;AACA,OAHA,MAGA,IAAA,YAAA,KAAA,KAAA,aAAA,CAAA,QAAA,EAAA;AACA,QAAA,KAAA,GAAA,KAAA,QAAA;AACA,QAAA,IAAA,GAAA,OAAA;AACA;;AACA,aAAA;AAAA,QAAA,KAAA,EAAA,KAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA;AAvDA,IA1BA;AAmFA,EAAA,OAnFA,qBAmFA;AACA,SAAA,eAAA;AACA;AArFA,CAAA","sourcesContent":["<template>\n  <div>\n    <template v-for=\"(returnItem, returnItemIndex) in this.rmaList.rmaListItems\">\n      <v-row :key=\"returnItemIndex\">\n        <div class=\"bg-well my-2\">\n          <div class=\"mx-5\">\n            <div class=\"d-flex justify-space-between align-center my-5\">\n              <div>\n                <o-text sm bold>\n                  {{ getReturnDate(returnItem.created_at) }}\n                </o-text>\n              </div>\n              <div class=\"d-flex align-center\">\n                <v-icon small bold>\n                  {{ getStepperlabel(returnItem.return_status).icon }}\n                </v-icon>\n                <o-text\n                  sm\n                  bold\n                  class=\"ml-2\"\n                >\n                  {{ getStepperlabel(returnItem.return_status).title }}\n                </o-text>\n              </div>\n            </div>\n\n            <v-divider />\n\n            <template v-for=\"(item, index) in returnItem.rma_items\">\n              <div :key=\"index\">\n                <o-text\n                  sm\n                  bold\n                  v-if=\"isExchange(item)\"\n                >\n                  {{ EXCHANGE }}\n                </o-text>\n                <o-text\n                  sm\n                  bold\n                  v-else\n                >\n                  {{ REFUND }}\n                </o-text>\n                <v-row>\n                  <div v-if=\"isExchange(item)\" class=\"d-flex mb-4\">\n                    <v-row>\n                      <v-row>\n                        <v-col cols=\"10\">\n                          <product-thumbnail\n                            :key=\"index\"\n                            :item=\"item.exchange_item\"\n                            :index=\"index\"\n                            :title=\"ORIGINAL_PURCHASE\"\n                            :sub-title1=\"item.exchange_item.original_item.title\"\n                            :sub-title2=\"\n                              item.exchange_item.original_item.color +\n                                ' - ' +\n                                item.exchange_item.original_item.size\n                            \"\n                            :image-url=\"\n                              item.exchange_item.original_item.image_url\n                            \"\n                            is-small-thumbnail\n                            from-return-item\n                          />\n                          <div class=\"ml-6\">\n                            <v-img\n                              :src=\"require('@/assets/images/down_arrow.png')\"\n                              max-width=\"22\"\n                              max-height=\"22\"\n                              style=\"z-index: 1\"\n                              contain\n                            />\n                          </div>\n                        </v-col>\n                      </v-row>\n                      <v-row class=\"mb-1 mt-n3\">\n                        <v-col cols=\"10\">\n                          <product-thumbnail\n                            :key=\"index\"\n                            :item=\"item.exchange_item\"\n                            :index=\"index\"\n                            :title=\"INCOMING_ITEM\"\n                            :sub-title1=\"item.exchange_item.title\"\n                            :sub-title2=\"\n                              item.exchange_item.color +\n                                ' - ' +\n                                item.exchange_item.size\n                            \"\n                            :image-url=\"item.exchange_item.image_url\"\n                            is-small-thumbnail\n                            from-return-item\n                          />\n                        </v-col>\n                      </v-row>\n                    </v-row>\n                  </div>\n                  <div v-else class=\"d-flex mb-4\">\n                    <v-row>\n                      <v-col>\n                        <product-thumbnail\n                          :key=\"index\"\n                          :item=\"item\"\n                          :index=\"index\"\n                          :title=\"item.order_item.title\"\n                          :sub-title1=\"item.order_item.subtitle\"\n                          :image-url=\"item.order_item.image_url\"\n                          is-small-thumbnail\n                          from-return-item\n                        />\n                      </v-col>\n                      <v-col style=\"max-width: 50px\">\n                        <o-text\n                          sm\n                          :key=\"index\"\n                          class=\"d-flex justify-end\"\n                        >\n                          {{ item.order_item.formattedRefundPrice }}\n                        </o-text>\n                      </v-col>\n                    </v-row>\n                  </div>\n                </v-row>\n              </div>\n            </template>\n\n            <v-divider />\n\n            <div\n              class=\"d-flex justify-content-between align-center justify-center my-5\"\n            >\n              <action-button\n                @click.stop=\"trackMyReturn(returnItem)\"\n                primary-color\n                :block=\"$vuetify.breakpoint.smAndDown\"\n              >\n                {{\n                  isExchangeOrder(returnItem.rma_items)\n                    ? TRACK_MY_EXCHANGE\n                    : TRACK_MY_RETURN\n                }}\n              </action-button>\n            </div>\n          </div>\n        </div>\n      </v-row>\n    </template>\n  </div>\n</template>\n\n<script>\nimport ProductThumbnail from '@/components/organism/ProductThumbnail.vue'\nimport { mapGetters, mapMutations } from 'vuex'\nimport OText from '@/components/atoms/OText.vue'\nimport { getFormattedDate } from '@/utils'\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport {\n  UPDATE_REFUND_LIST_FOR_RETURN_USER,\n  UPDATE_EXCHANGE_LIST_FOR_RETURN_USER\n} from '@/store/constants.js'\nimport { TRACKING_VIEW, RETURN_REVIEW } from 'branding/strings.json'\nimport { CONSTANTS } from '@/constants'\nexport default {\n  name: 'ReturnUserView',\n  components: {\n    ProductThumbnail,\n    OText,\n    ActionButton\n  },\n  data () {\n    return {\n      COMPLETED: TRACKING_VIEW.COMPLETED,\n      ACTIVE: TRACKING_VIEW.ACTIVE,\n      CANCELED: TRACKING_VIEW.CANCELED,\n      REFUND: TRACKING_VIEW.REFUND,\n      EXCHANGE: TRACKING_VIEW.EXCHANGE,\n      TRACK_MY_RETURN: TRACKING_VIEW.TRACK_MY_RETURN,\n      TRACK_MY_EXCHANGE: TRACKING_VIEW.TRACK_MY_EXCHANGE,\n      ORIGINAL_PURCHASE: RETURN_REVIEW.ORIGINAL_PURCHASE,\n      INCOMING_ITEM: RETURN_REVIEW.INCOMING_ITEM,\n      RETURN_STATUS: CONSTANTS.RETURN_STATUS,\n      rmaStatus: ''\n    }\n  },\n  computed: {\n    ...mapGetters({ rmaList: 'order/rmaList', order: 'order/order' })\n  },\n  props: {},\n  methods: {\n    ...mapMutations({\n      setSelectedReturnMethod: 'returns/setSelectedReturnMethod'\n    }),\n    getReturnDate (createdAt) {\n      return 'Requested on ' + getFormattedDate(createdAt)\n    },\n    isExchangeOrder (rmaItems) {\n      return rmaItems?.filter((rmaItem) => rmaItem?.is_exchange)?.length > 0\n    },\n    async trackMyReturn (returnItem) {\n      this.$store.commit('returns/updateSelectedReturnItem', returnItem)\n      const refundList = []\n      const exchangeList = []\n      returnItem.rma_items.map((item) => {\n        if (!item?.is_exchange) {\n          refundList.push(item.order_item)\n        } else {\n          exchangeList.push(item.exchange_item)\n        }\n      })\n      this.$store.commit(\n        'order/' + UPDATE_REFUND_LIST_FOR_RETURN_USER,\n        refundList\n      )\n      this.$store.commit(\n        'order/' + UPDATE_EXCHANGE_LIST_FOR_RETURN_USER,\n        exchangeList\n      )\n      this.setSelectedReturnMethod(returnItem.return_method)\n      this.$router.push(\n        '/orders/' + this.order.order_hash + '/tracker/' + returnItem.identifier\n      )\n    },\n    isExchange (item) {\n      return item.is_exchange && item.exchange_item\n    },\n    getStepperlabel (returnStatus) {\n      var title = ''\n      var icon = ''\n      if (\n        returnStatus === this.RETURN_STATUS.STARTED ||\n        returnStatus === this.RETURN_STATUS.IN_TRANSIT ||\n        returnStatus === this.RETURN_STATUS.RECEIVED\n      ) {\n        title = this.ACTIVE\n        icon = 'hourglass_top'\n      } else if (returnStatus === this.RETURN_STATUS.COMPLETED) {\n        title = this.COMPLETED\n        icon = 'done'\n      } else if (returnStatus === this.RETURN_STATUS.CANCELED) {\n        title = this.CANCELED\n        icon = 'block'\n      }\n      return { title, icon }\n    }\n  },\n  mounted () {\n    this.getStepperlabel()\n  }\n}\n</script>\n<style scoped></style>\n"],"sourceRoot":"src/components/pages"}]}