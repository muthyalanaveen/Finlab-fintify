AAW,CAAC0B;AADf;AAJR,CA7Ia,EAqJb;AACEpB,EAAAA,IAAI,EAAE,QADR;AAEEC,EAAAA,IAAI,EAAE,OAFR;AAGEC,EAAAA,SAAS,EAAEb,KAHb;AAIEc,EAAAA,IAAI,EAAE;AACJC,IAAAA,KAAK,EAAEV,WAAW,CAAC2B;AADf;AAJR,CArJa,EA6Jb;AACErB,EAAAA,IAAI,EAAE,iBADR;AAEEC,EAAAA,IAAI,EAAE,gBAFR;AAGEC,EAAAA,SAAS,EAAEZ,aAHb;AAIEa,EAAAA,IAAI,EAAE;AACJC,IAAAA,KAAK,EAAEV,WAAW,CAAC4B;AADf;AAJR,CA7Ja,EAqKb;AACEtB,EAAAA,IAAI,EAAE,mBADR;AAEEC,EAAAA,IAAI,EAAE,kBAFR;AAGEC,EAAAA,SAAS,EAAEX,cAHb;AAIEY,EAAAA,IAAI,EAAE;AACJC,IAAAA,KAAK,EAAEV,WAAW,CAAC6B;AADf;AAJR,CArKa,EA6Kb;AACEvB,EAAAA,IAAI,EAAE,GADR;AAEEE,EAAAA,SAAS,EAAEf,YAFb;AAGEgB,EAAAA,IAAI,EAAE;AACJC,IAAAA,KAAK,EAAEV,WAAW,CAAC8B;AADf;AAHR,CA7Ka,CAAf;AAsLA,IAAMC,MAAM,GAAG,IAAIvD,SAAJ,CAAc;AAC3BwD,EAAAA,IAAI,EAAE,SADqB;AAE3B3B,EAAAA,MAAM,EAANA,MAF2B;AAG3B4B,EAAAA,cAAc,EAAE,wBAACC,EAAD,EAAKC,IAAL,EAAWC,aAAX,EAA6B;AAC3C,QAAIF,EAAE,CAACG,IAAP,EAAa,OAAO;AAAEC,MAAAA,QAAQ,EAAEJ,EAAE,CAACG;AAAf,KAAP;AACb,QAAID,aAAJ,EAAmB,OAAOA,aAAP;AACnB,WAAO;AAAEG,MAAAA,CAAC,EAAE,CAAL;AAAQC,MAAAA,CAAC,EAAE;AAAX,KAAP;AACD;AAP0B,CAAd,CAAf;AAUAT,MAAM,CAACU,UAAP,CAAkB,UAACP,EAAD,EAAKC,IAAL,EAAWO,IAAX,EAAoB;AACpC,MAAMC,eAAe,GAAGC,YAAY,CAACC,OAAb,CAAqB,iBAArB,CAAxB;AACA,MAAMpC,IAAI,GAAGqC,QAAQ,CAACC,aAAT,CAAuB,MAAvB,CAAb;AACAtC,EAAAA,IAAI,CAACF,IAAL,GAAY,aAAZ;AACAE,EAAAA,IAAI,CAACa,OAAL,GAAevB,MAAM,CAACiD,WAAtB;AACAF,EAAAA,QAAQ,CAACG,oBAAT,CAA8B,MAA9B,EAAsC,CAAtC,EAAyCC,WAAzC,CAAqDzC,IAArD;AACAqC,EAAAA,QAAQ,CAACpC,KAAT,GAAiBwB,EAAE,CAACzB,IAAH,CAAQC,KAAzB;;AAEA,MAAIwB,EAAE,CAAC3B,IAAH,KAAY,MAAhB,EAAwB;AAAE;AACxBL,IAAAA,YAAY;AACb,GAFD,MAEO,IAAIgC,EAAE,CAAC3B,IAAH,KAAY,iBAAhB,EAAmC;AAAE;AAC1CN,IAAAA,cAAc;AACf;;AAED,MACEE,gBAAgB,KAAK,MAArB,IACAwC,eAAe,KAAK,MADpB,IAEAT,EAAE,CAAC3B,IAAH,KAAY,OAHd,EAIE;AACAmC,IAAAA,IAAI,CAAC;AAAEnC,MAAAA,IAAI,EAAE;AAAR,KAAD,CAAJ;AACD,GAND,MAMOmC,IAAI;AACZ,CArBD;AAuBA,eAAeX,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport Home from '@/components/pages/Home.vue'\nimport CustomerKeepsView from '@/components/pages/CustomerKeepsView.vue'\nimport ZipCodeView from '@/components/pages/ZipCodeView.vue'\nimport EmailView from '@/components/pages/EmailView.vue'\nimport OrderIdView from '@/components/pages/OrderIdView.vue'\nimport OrderandZipCodeView from '@/components/pages/OrderandZipCodeView.vue'\nimport ReturnListView from '@/components/pages/ReturnListView.vue'\nimport HowItWorksView from '@/components/pages/HowItWorksView.vue'\nimport ReturnGiftView from '@/components/pages/ReturnGiftView.vue'\nimport ReturnReasonWrapper from '@/components/pages/ReturnReasonWrapper.vue'\nimport ReturnMethods from '@/components/pages/ReturnMethods.vue'\nimport OrdersView from '@/components/pages/orders.vue'\nimport Typography from '@/components/pages/Typography.vue'\nimport PDPPage from '@/components/pages/PDPPage.vue'\nimport ReviewView from '@/components/pages/ReviewView.vue'\nimport ConfirmationView from '@/components/pages/ConfirmationView.vue'\nimport PageNotFound from '@/components/pages/PageNotFound.vue'\nimport TrackerView from '@/components/pages/TrackerView.vue'\nimport Login from '@/components/pages/Login.vue'\nimport PrivacyPolicy from '@/components/pages/PrivacyPolicy.vue'\nimport TermsOfService from '@/components/pages/TermsOfService.vue'\nimport ReturnMethodsMap from '@/components/pages/ReturnMethodsMap.vue'\nimport { SEARCH, PAGE_TITLES } from 'branding/strings.json'\nimport { clearGiftEmail, clearSession } from '@/store/sessionStorage.js'\nimport { appLoginRequired } from '@/common/config'\n\nVue.use(VueRouter)\n\nconst routes = [\n  {\n    path: '/',\n    name: 'Home',\n    component: Home,\n    meta: {\n      title: PAGE_TITLES.ORDER_LOOKUP\n    }\n  },\n  {\n    path: '/zipcode',\n    name: 'zipcode',\n    component: ZipCodeView,\n    meta: {\n      title: PAGE_TITLES.SHIPPING_ZIP_CODE\n    }\n  },\n  {\n    path: '/email',\n    name: 'email',\n    component: EmailView\n  },\n  {\n    path: '/order',\n    name: 'order',\n    component: OrderIdView\n  },\n  {\n    path: '/orderandzipcode',\n    name: 'orderandzipcode',\n    component: OrderandZipCodeView,\n    meta: {\n      title: PAGE_TITLES.ORDER_NOT_FOUND\n    }\n  },\n  {\n    path: '/orders/:id',\n    component: OrdersView,\n    children: [\n      {\n        path: '',\n        component: ReturnListView,\n        meta: {\n          title: PAGE_TITLES.ITEM_SELECTION\n        }\n      },\n      {\n        path: 'reasons',\n        name: 'reasons',\n        component: ReturnReasonWrapper,\n        meta: {\n          title: PAGE_TITLES.RETURN_REASON\n        }\n      },\n      {\n        path: 'customer-keeps',\n        name: 'customer-keeps',\n        component: CustomerKeepsView\n      },\n      {\n        path: 'refund-amount',\n        name: 'refund-amount',\n        component: ReturnReasonWrapper,\n        meta: {\n          title: '' // switches based off tabs\n        }\n      },\n      {\n        path: 'shipping',\n        component: ReturnMethods,\n        meta: {\n          title: PAGE_TITLES.RETURN_METHOD\n        }\n      },\n      {\n        path: 'mapshipping',\n        name: 'mapshipping',\n        component: ReturnMethodsMap,\n        meta: {\n          title: PAGE_TITLES.RETURN_METHOD\n        }\n      },\n      {\n        path: 'review',\n        component: ReviewView,\n        meta: {\n          title: PAGE_TITLES.REVIEW\n        },\n        props: true\n      },\n      {\n        path: 'confirm/:identifier',\n        component: ConfirmationView,\n        meta: {\n          title: PAGE_TITLES.CONFIRMATION\n        },\n        props: {\n          header: true,\n          content: true\n        }\n      },\n      {\n        path: 'tracker',\n        component: TrackerView,\n        meta: {\n          title: PAGE_TITLES.TRACKER\n        }\n      },\n      {\n        path: 'tracker/:identifier',\n        component: TrackerView,\n        meta: {\n          title: PAGE_TITLES.TRACKER\n        }\n      }\n    ]\n  },\n  {\n    path: '/howitworks',\n    name: 'howitworks',\n    component: HowItWorksView,\n    meta: {\n      title: PAGE_TITLES.HOW_IT_WORKS\n    }\n  },\n  {\n    path: '/returngift',\n    name: 'returngift',\n    component: ReturnGiftView,\n    meta: {\n      title: '' // TODO: update with new return gift flow\n    }\n  },\n  {\n    path: '/pdppage',\n    name: 'pdppage',\n    component: PDPPage,\n    meta: {\n      title: PAGE_TITLES.PDP_PAGE\n    }\n  },\n  {\n    path: '/Typography',\n    name: 'Typography',\n    component: Typography,\n    meta: {\n      title: PAGE_TITLES.TYPOGRAPHY\n    }\n  },\n  {\n    path: '/login',\n    name: 'login',\n    component: Login,\n    meta: {\n      title: PAGE_TITLES.LOG_IN\n    }\n  },\n  {\n    path: '/privacy-policy',\n    name: 'privacy-policy',\n    component: PrivacyPolicy,\n    meta: {\n      title: PAGE_TITLES.PRIVACY_POLICY\n    }\n  },\n  {\n    path: '/terms-of-service',\n    name: 'terms-of-service',\n    component: TermsOfService,\n    meta: {\n      title: PAGE_TITLES.TERMS_OF_SERVICE\n    }\n  },\n  {\n    path: '*',\n    component: PageNotFound,\n    meta: {\n      title: PAGE_TITLES.PAGE_NOT_FOUND\n    }\n  }\n]\n\nconst router = new VueRouter({\n  mode: 'history',\n  routes,\n  scrollBehavior: (to, from, savedPosition) => {\n    if (to.hash) return { selector: to.hash }\n    if (savedPosition) return savedPosition\n    return { x: 0, y: 0 }\n  }\n})\n\nrouter.beforeEach((to, from, next) => {\n  const isAuthenticated = localStorage.getItem('isAuthenticated')\n  const meta = document.createElement('meta')\n  meta.name = 'description'\n  meta.content = SEARCH.DESCRIPTION\n  document.getElementsByTagName('head')[0].appendChild(meta)\n  document.title = to.meta.title\n\n  if (to.name === 'Home') { // clear is_gift and email on home page\n    clearSession()\n  } else if (to.name === 'orderandzipcode') { // clear email\n    clearGiftEmail()\n  }\n\n  if (\n    appLoginRequired === 'true' &&\n    isAuthenticated !== 'true' &&\n    to.name !== 'login'\n  ) {\n    next({ name: 'login' })\n  } else next()\n})\n\nexport default router\n"]}]}