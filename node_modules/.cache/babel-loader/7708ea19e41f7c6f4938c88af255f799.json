 (commit, dispatch, data) {\n  commit(FETCH_ORDER_END, data)\n  commit('returns/updatePolicies', data.policies, { root: true })\n  commit('returns/updatePolicyReturnMethods', data.policies.return_methods, {\n    root: true\n  })\n  commit('returns/setAddress', data.order.shipping_address, {\n    root: true\n  })\n  dispatch(\n    'returns/fetchAddressInfo',\n    {\n      shippingAddress: data.order.shipping_address,\n      returnMethods: formatReturnMethods(data.policies.return_methods)\n    },\n    { root: true }\n  )\n}\n\nfunction onFetchConfigSuccess (commit, data) {\n  commit('returns/updateConfig', data, { root: true })\n}\n\nexport default {\n  async [FETCH_ORDER] ({ commit, dispatch }, params) {\n    try {\n      commit(FETCH_START)\n      const { data: { access_token: accessToken } } = await AuthService.getAccessToken(params)\n      jwtService.saveAccessToken(accessToken)\n\n      const { data } = await FetchOrderService.getOrder()\n      onFetchOrderSuccess(commit, dispatch, data)\n      return data\n    } catch (error) {\n      commit(SET_ERROR)\n      throw new Error(error)\n    }\n  },\n  [CREATE_RMA] ({ commit }, params) {\n    commit(FETCH_START)\n    return CreateRMAService.createRMA(params)\n      .then(({ data }) => {\n        return data\n      })\n      .catch((error) => {\n        commit(SET_ERROR)\n        throw new Error(error)\n      })\n  },\n  [GET_SHIPPING_LABEL] ({ commit }, params) {\n    commit(FETCH_START)\n    return GetShippingLabelService.getShippingLabel(params)\n      .then(({ data }) => {\n        return data\n      })\n      .catch((error) => {\n        commit(SET_ERROR)\n        throw new Error(error)\n      })\n  },\n  [GET_QR_CODE_LABEL] ({ commit }, params) {\n    commit(FETCH_START)\n    return GetQRCodeLabelService.getQRCodeLabel(params)\n      .then(({ data }) => {\n        return data\n      })\n      .catch((error) => {\n        commit(SET_ERROR)\n        throw new Error(error)\n      })\n  },\n  [GET_CONFIG] ({ commit }, params) {\n    return GetConfigService.getConfig(params)\n      .then(({ data }) => {\n        onFetchConfigSuccess(commit, data)\n        return data\n      })\n      .catch((error) => {\n        commit(SET_ERROR)\n        throw new Error(error)\n      })\n  },\n  [IS_TRUST_WORTHY_USER] ({ commit }, params) {\n    return IsTrustWorthyUserService.isTrustWorthyUser(params)\n      .then(({ data }) => {\n        return data\n      })\n      .catch((error) => {\n        commit(SET_ERROR)\n        throw new Error(error)\n      })\n  }\n}\n"]}]}