AA,EAAA,MAAA,CAAA;AAFA,iBALA;;AAUA,oBAAA,MAAA,CAAA,YAAA,EAAA;AACA,kBAAA,MAAA,mCAAA,MAAA;AAAA,oBAAA,KAAA,EAAA,MAAA,CAAA;AAAA,oBAAA;AACA;;AACA,gBAAA,MAAA,CAAA,MAAA,CACA,QADA,CACA,WAAA,WADA,EACA,MADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,sBAAA,QAAA,EAAA;AACA,wBAAA,SAAA,GACA,QAAA,IAAA,QAAA,IAAA,IAAA,GACA,QAAA,CAAA,UADA,GAEA,MAAA,CAAA,KAAA,CAAA,OAHA;;AAIA,oBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,aAAA,SAAA;AACA,mBANA,MAMA;AACA,oBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,kBAAA;AACA;AACA,iBAZA,EAaA,KAbA,CAaA,UAAA,GAAA,EAAA;AACA,kBAAA,OAAA,CAAA,KAAA,CAAA;AAAA,oBAAA,GAAA,EAAA;AAAA,mBAAA;AACA,iBAfA;;AAbA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA6BA;AApDA;AAvDA,CAAA","sourcesContent":["<template>\n  <v-container>\n    <v-row class=\"d-flex justify-center\">\n      <v-col\n        md=\"4\"\n        sm=\"8\"\n        xs=\"12\"\n        :class=\"{\n          'px-7': $vuetify.breakpoint.smAndDown,\n          'py-5': $vuetify.breakpoint.smAndDown,\n        }\"\n      >\n        <v-form>\n          <div class=\"d-flex align-center\">\n            <v-icon style=\"color: #d83933;\" small>\n              mdi-alert-circle\n            </v-icon>\n            <o-text sm semi-bold text-red class=\"pl-2\">\n              {{ NO_ORDER_FOUND }}\n            </o-text>\n          </div>\n\n          <header-title\n            :class=\"{\n              'pb-3': $vuetify.breakpoint.smAndDown,\n              'pb-5': $vuetify.breakpoint.mdAndUp,\n            }\"\n          >\n            {{ PLEASE_TRY_AGAIN }}\n          </header-title>\n          <o-text-field\n            :title=\"ENTER_ORDER_ID\"\n            id=\"orderId\"\n            v-model=\"orderId\"\n            @onChange=\"onOrderChange($event)\"\n            @onKeyEnter=\"lookupOrder\"\n            @onPaste=\"validate()\"\n            :class=\"{ 'pb-3': $vuetify.breakpoint.mdAndUp }\"\n          />\n          <o-text-field\n            :title=\"EMAIL_TEXT_FIELD_TITLE\"\n            id=\"email\"\n            v-if=\"isGiftReturn\"\n            v-model=\"email\"\n            @onChange=\"onEmailChange($event)\"\n            @onKeyEnter=\"lookupOrder\"\n            @onPaste=\"validate()\"\n            :class=\"{ 'pb-3': $vuetify.breakpoint.mdAndUp }\"\n          />\n          <o-text-field\n            :title=\"ZIP_CODE_TEXT_FIELD_TITLE\"\n            id=\"zipCode\"\n            v-model=\"zipCode\"\n            @onChange=\"onZipChange($event)\"\n            @onKeyEnter=\"lookupOrder\"\n            @onPaste=\"validate()\"\n          />\n          <action-button\n            @click=\"lookupOrder\"\n            :class=\"{\n              'mt-2': $vuetify.breakpoint.smAndDown,\n              'mt-0': $vuetify.breakpoint.mdAndUp,\n            }\"\n            :loading=\"isLoading\"\n            :disabled=\"isDisabled\"\n            primary-color\n            block\n            x-large\n          >\n            {{ LOOKUP_ORDER }}\n          </action-button>\n        </v-form>\n        <v-col class=\"mt-8 mb-2\">\n          <o-text sm text-blue class=\"d-flex justify-center mb-1\">\n            Have a question? Give us a call.\n          </o-text>\n          <o-text sm text-blue class=\"d-flex justify-center\">\n            {{ this.customerSupportContact }}\n          </o-text>\n        </v-col>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport { FETCH_ORDER, GET_CONFIG } from \"@/store/constants.js\";\nimport OTextField from \"@/components/molecules/OTextField.vue\";\nimport ActionButton from \"@/components/atoms/ActionButton.vue\";\nimport { validateOrderId, validateZipcode, validateEmail } from \"@/utils\";\nimport {\n  ORDER_LOOKUP,\n  ORDER_LOOKUP_TRY_AGAIN,\n  ZIP_CODE_LOOKUP,\n  EMAIL_LOOKUP,\n} from \"branding/strings.json\";\nimport OText from \"@/components/atoms/OText.vue\";\nimport HeaderTitle from \"@/components/molecules/HeaderTitle.vue\";\n\nexport default {\n  components: {\n    OTextField,\n    ActionButton,\n    OText,\n    HeaderTitle,\n  },\n  data() {\n    return {\n      orderId: \"\",\n      zipCode: \"\",\n      email: \"\",\n      isDisabled: true,\n      customerSupportContact: \"\",\n      NO_ORDER_FOUND: ORDER_LOOKUP_TRY_AGAIN.NO_ORDER_FOUND,\n      PLEASE_TRY_AGAIN: ORDER_LOOKUP_TRY_AGAIN.PLEASE_TRY_AGAIN,\n      ENTER_ORDER_ID: ORDER_LOOKUP.ENTER_ORDER_ID,\n      ZIP_CODE_TEXT_FIELD_TITLE: ZIP_CODE_LOOKUP.ZIP_CODE_TEXT_FIELD_TITLE,\n      LOOKUP_ORDER: ZIP_CODE_LOOKUP.LOOKUP_ORDER,\n      EMAIL_TEXT_FIELD_TITLE: EMAIL_LOOKUP.EMAIL_TEXT_FIELD_TITLE,\n    };\n  },\n  computed: {\n    ...mapGetters({\n      order: \"order/order\",\n      isLoading: \"order/isLoading\",\n      state_orderId: \"ui/orderId\",\n      state_zipCode: \"ui/zipCode\",\n      config: \"returns/config\",\n      isGiftReturn: \"ui/isGiftReturn\",\n      state_Email: \"ui/email\",\n    }),\n  },\n  mounted() {\n    this.orderId = this.state_orderId;\n    this.zipCode = this.state_zipCode;\n    this.email = this.state_Email;\n    this.validate();\n\n    if (!this.config) {\n      this.$store\n        .dispatch(\"order/\" + GET_CONFIG, null)\n        .then((response) => {\n          if (response) {\n            this.customerSupportContact =\n              response.retailer_info?.customer_support_contact;\n          }\n        })\n        .catch((err) => {\n          console.error({ err });\n        });\n    } else {\n      this.customerSupportContact = this.config?.retailer_info?.customer_support_contact;\n    }\n  },\n  methods: {\n    validate() {\n      this.isDisabled = !(\n        validateOrderId(this.orderId) &&\n        validateZipcode(this.zipCode) &&\n        validateEmail(this.email)\n      );\n    },\n    onZipChange(event) {\n      const value = event.target.value;\n      this.zipCode = value;\n      this.validate();\n    },\n    onEmailChange(event) {\n      const value = event.target.value;\n      this.email = value;\n      this.validate();\n    },\n    onOrderChange(event) {\n      const value = event.target.value;\n      this.orderId = value;\n      this.validate();\n    },\n    async lookupOrder() {\n      this.$store.commit(\"ui/updateOrderId\", this.orderId);\n      this.$store.commit(\"ui/updateZipCode\", this.zipCode);\n      this.$store.commit(\"ui/updateEmail\", this.email);\n\n      let params = {\n        order_id: this.orderId,\n        zip_code: this.zipCode,\n      };\n\n      if (this.isGiftReturn) {\n        params = { ...params, email: this.email };\n      }\n      this.$store\n        .dispatch(\"order/\" + FETCH_ORDER, params)\n        .then((response) => {\n          if (response) {\n            const orderHash =\n              response && response != null\n                ? response.order_hash\n                : this.order.orderId;\n            this.$router.push(\"/orders/\" + orderHash);\n          } else {\n            this.$router.push(\"/orderandzipcode\");\n          }\n        })\n        .catch((err) => {\n          console.error({ err });\n        });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  padding-top: 60px;\n}\n</style>\n"],"sourceRoot":"src/components/pages"}]}