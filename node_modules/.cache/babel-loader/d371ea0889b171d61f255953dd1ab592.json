AkB,CAACkD,WAAnB,CAA+BhB,MAA/B,EACJS,IADI,CACC,kBAAc;AAAA,QAAXzB,IAAW,UAAXA,IAAW;AAClB,WAAOA,IAAP;AACD,GAHI,EAIJ0B,KAJI,CAIE,UAACC,KAAD,EAAW;AAChB7B,IAAAA,MAAM,CAACX,SAAD,CAAN;AACA,UAAM,IAAIoC,KAAJ,CAAUI,KAAV,CAAN;AACD,GAPI,CAAP;AAQD,CAjFH","sourcesContent":["import {\n  FetchOrderService,\n  CreateRMAService,\n  GetShippingLabelService,\n  GetQRCodeLabelService,\n  GetConfigService,\n  IsTrustWorthyUserService,\n  DestinationService,\n  AuthService\n} from '@/common/api.service'\nimport {\n  FETCH_ORDER,\n  FETCH_START,\n  FETCH_ORDER_END,\n  SET_ERROR,\n  CREATE_RMA,\n  GET_SHIPPING_LABEL,\n  GET_QR_CODE_LABEL,\n  GET_CONFIG,\n  IS_TRUST_WORTHY_USER,\n  DESTINATION\n} from '@/store/constants'\nimport { formatReturnMethods } from '@/controllers/ReturnMethodsController.js'\nimport jwtService from '@/common/jwt.service'\nimport { getIsGift } from '@/store/sessionStorage.js'\n\nfunction onFetchOrderSuccess (commit, dispatch, data, state) {\n  commit(FETCH_ORDER_END, data)\n\n  const isGiftReturn = getIsGift()\n  const giftReturnsAllowed = data.policies?.gift_returns_allowed\n\n  let policies = data.policies\n\n  if (giftReturnsAllowed && isGiftReturn) {\n    policies = {\n      ...policies,\n      return_methods: data.policies.return_methods.filter(\n        (method) => method.type !== 'express-returns'\n      )\n    }\n  }\n  commit('returns/updatePolicies', policies, { root: true })\n  commit('returns/updatePolicyReturnMethods', policies.return_methods, {\n    root: true\n  })\n  commit('returns/setAddress', data.order.shipping_address, {\n    root: true\n  })\n  dispatch(\n    'returns/fetchAddressInfo',\n    {\n      shippingAddress: data.order.shipping_address,\n      returnMethods: formatReturnMethods(policies.return_methods)\n    },\n    { root: true }\n  )\n}\n\nfunction onFetchConfigSuccess (commit, data) {\n  commit('returns/updateConfig', data, { root: true })\n}\n\nexport default {\n  async [FETCH_ORDER] ({ commit, dispatch, rootState }, params) {\n    try {\n      commit(FETCH_START)\n      const {\n        data: { access_token: accessToken }\n      } = await AuthService.getAccessToken(params)\n      jwtService.saveAccessToken(accessToken)\n\n      const { data } = await FetchOrderService.getOrder()\n\n      onFetchOrderSuccess(commit, dispatch, data, rootState)\n      return data\n    } catch (error) {\n      commit(SET_ERROR)\n      throw new Error(error)\n    }\n  },\n  [CREATE_RMA] ({ commit }, params) {\n    commit(FETCH_START)\n    return CreateRMAService.createRMA(params)\n      .then(({ data }) => {\n        return data\n      })\n      .catch((error) => {\n        commit(SET_ERROR)\n        throw new Error(error)\n      })\n  },\n  [GET_SHIPPING_LABEL] ({ commit }, params) {\n    commit(FETCH_START)\n    return GetShippingLabelService.getShippingLabel(params)\n      .then(({ data }) => {\n        return data\n      })\n      .catch((error) => {\n        commit(SET_ERROR)\n        throw new Error(error)\n      })\n  },\n  [GET_QR_CODE_LABEL] ({ commit }, params) {\n    commit(FETCH_START)\n    return GetQRCodeLabelService.getQRCodeLabel(params)\n      .then(({ data }) => {\n        return data\n      })\n      .catch((error) => {\n        commit(SET_ERROR)\n        throw new Error(error)\n      })\n  },\n  [GET_CONFIG] ({ commit }, params) {\n    return GetConfigService.getConfig(params)\n      .then(({ data }) => {\n        onFetchConfigSuccess(commit, data)\n        return data\n      })\n      .catch((error) => {\n        commit(SET_ERROR)\n        throw new Error(error)\n      })\n  },\n  [IS_TRUST_WORTHY_USER] ({ commit }, params) {\n    return IsTrustWorthyUserService.isTrustWorthyUser(params)\n      .then(({ data }) => {\n        return data\n      })\n      .catch((error) => {\n        commit(SET_ERROR)\n        throw new Error(error)\n      })\n  },\n  [DESTINATION] ({ commit }, params) {\n    return DestinationService.destination(params)\n      .then(({ data }) => {\n        return data\n      })\n      .catch((error) => {\n        commit(SET_ERROR)\n        throw new Error(error)\n      })\n  }\n}\n"]}]}