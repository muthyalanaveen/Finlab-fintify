dAndUp,\n            }\"\n            :disabled=\"isDisabled\"\n            :loading=\"isLoading\"\n            primary-color\n            block\n            x-large\n          >\n            {{ NEXT_BUTTON }}\n          </action-button>\n        </v-form>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\nimport OTextField from '@/components/molecules/OTextField.vue'\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport { EMAIL_LOOKUP } from 'branding/strings.json'\nimport OText from '@/components/atoms/OText.vue'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\nimport { validateEmail, validateOrderId } from '@/utils'\n\nexport default {\n  components: {\n    OTextField,\n    ActionButton,\n    OText,\n    HeaderTitle\n  },\n  data () {\n    return {\n      email: '',\n      isDisabled: true,\n      ONE_MORE_THING: EMAIL_LOOKUP.ONE_MORE_THING,\n      EMAIL_TITLE: EMAIL_LOOKUP.EMAIL_TITLE,\n      EMAIL_TEXT_FIELD_TITLE: EMAIL_LOOKUP.EMAIL_TEXT_FIELD_TITLE,\n      NEXT_BUTTON: EMAIL_LOOKUP.NEXT_BUTTON\n    }\n  },\n  computed: {\n    ...mapGetters({\n      order: 'order/order',\n      isLoading: 'order/isLoading',\n      state_orderId: 'ui/orderId',\n      state_Email: 'ui/email'\n    })\n  },\n  mounted () {\n    this.email = ''\n    this.validate()\n  },\n  methods: {\n    onChange (event) {\n      this.validate(event.target.value)\n    },\n    onPaste (event) {\n      this.validate(event.clipboardData.getData('text/plain'))\n    },\n    validate (value) {\n      this.email = value\n      this.isDisabled = !validateEmail(value)\n    },\n    async onNextClicked () {\n      if (this.isDisabled) {\n        return\n      }\n      this.$store.commit('ui/updateEmail', this.email)\n      if (this.state_orderId && validateOrderId(this.state_orderId)) {\n        this.$router.push('/zipcode')\n      } else {\n        this.$router.push('/order')\n      }\n    }\n  }\n}\n</script>\n\n