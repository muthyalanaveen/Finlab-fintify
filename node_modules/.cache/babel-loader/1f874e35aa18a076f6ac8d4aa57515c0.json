ASC,aAAT,CAAwBC,OAAxB,EAAiCC,cAAjC,EAAiD;AACtD;AACA,MAAMC,sBAAsB,GAAGD,cAAc,CAACvC,KAAf,CAAqB,GAArB,CAA/B;AACA,MAAMyC,QAAQ,GAAGH,OAAO,CAACI,MAAR,CAAe,UAACC,SAAD,EAAe;AAC7C,QAAMC,oBAAoB,GAAGD,SAAS,CAAC3C,KAAV,CAAgB,GAAhB,CAA7B;;AACA,QAAIwC,sBAAsB,CAAC7G,MAAvB,KAAkC,CAAlC,IAAuC6G,sBAAsB,CAAC,CAAD,CAAtB,KAA8BI,oBAAoB,CAAC,CAAD,CAAzF,IAAgGJ,sBAAsB,CAAC,CAAD,CAAtB,KAA8BI,oBAAoB,CAAC,CAAD,CAAlJ,IAAyJJ,sBAAsB,CAAC,CAAD,CAAtB,KAA8BI,oBAAoB,CAAC,CAAD,CAA3M,IAAkNJ,sBAAsB,CAAC,CAAD,CAAtB,KAA8BI,oBAAoB,CAAC,CAAD,CAAxQ,EAA6Q;AAC3Q,aAAO,IAAP;AACD;;AACD,WAAO,KAAP;AACD,GANgB,CAAjB;AAOA,SAAOH,QAAP;AACD;AAED,OAAO,IAAMI,KAAK,GAAG,SAARA,KAAQ,CAAAC,EAAE,EAAI;AACzB,SAAO,IAAIC,OAAJ,CAAY,UAAAC,OAAO;AAAA,WAAIC,UAAU,CAACD,OAAD,EAAUF,EAAV,CAAd;AAAA,GAAnB,CAAP;AACD,CAFM","sourcesContent":["import store from '@/store'\nimport { whiteListedStaplesStores, whiteListedUSPSStores } from '@/constants'\n\nexport function pad (num, size) {\n  num = num.toString()\n  while (num.length < size) num = '0' + num\n  return num\n}\n\nexport function validateOrderId (str) {\n  const orderIdRex = /[a-zA-Z0-9]/g\n  if (str !== undefined && str !== null) {\n    if (str.length >= 4 && orderIdRex.test(str)) {\n      return true\n    }\n  }\n  return false\n}\n\nexport function validateZipcode (str) {\n  const zipcodeRex = /[0-9]/g\n  if (str !== undefined && str !== null) {\n    if (str.length === 5 && zipcodeRex.test(str)) {\n      return true\n    }\n  }\n  return false\n}\n\nexport function validateEmail (str) {\n  const emailRegex = /.+@.+\\..+/\n  if (str !== undefined && str !== null) {\n    if (emailRegex.test(str)) {\n      return true\n    }\n  }\n  return false\n}\n\nexport function getFormattedDate (date) {\n  date = new Date(date)\n  if (date) {\n    const year = date.getFullYear()\n    const month = (1 + date.getMonth()).toString().padStart(2, '0')\n    const day = date\n      .getDate()\n      .toString()\n      .padStart(2, '0')\n\n    return month + '/' + day + '/' + year\n  }\n  return date\n}\n\nexport function formatReturnMethods (returnMethods) {\n  const methods = []\n  returnMethods.forEach((method) => {\n    const type = method.type\n    const requirePackageAndPrint = method.package_and_print\n    const isSustainable = method.sustainable\n    const returnMethodRank = method.return_method_rank\n    const distanceThreshold = method.distance_threshold\n\n    method.affiliates.map((affiliate) => {\n      methods.push({\n        id: `${type}_${affiliate.name}`,\n        type,\n        requirePackageAndPrint,\n        isSustainable,\n        returnMethodRank,\n        distanceThreshold,\n        ...affiliate\n      })\n    })\n  })\n  return methods\n}\n\nexport function dayOfWeekAsString (dayIndex) {\n  return (['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'][dayIndex] || '')\n}\n\nexport function toRegularTime ({ dayIndex = '', hours, minutes }) {\n  return hours === null || hours === null\n    ? ''\n    : `${hours > 12 ? hours - 12 : hours}:${\n        minutes < 10 ? '0' + minutes : minutes\n      } ${hours >= 12 ? 'PM' : 'AM'} ${dayIndex && 'on ' + dayOfWeekAsString(dayIndex)}`\n}\n\nexport function validateImageURI (uri) {\n  const validExtensions = ['jpg', 'png', 'jpeg']\n  return (\n    // ends with one of image extensions or image is encoded in base 64 format\n    uri && (validExtensions.some((suffix) => uri.toLowerCase().endsWith(suffix)) || uri.substring(0, 5) === 'data:')\n  )\n}\n\nexport function flattenVariant (variant) {\n  const flattenedAttributes = {}\n  variant.attributes.forEach((a) => { flattenedAttributes[a.name.toLowerCase()] = a.value })\n  return {\n    ...flattenedAttributes,\n    ...variant\n  }\n}\n\nexport function flattenVariants (rawVariants) {\n  return rawVariants.map((v) => flattenVariant(v))\n}\n\nexport function doFormatVariantSizes (variants, item) {\n  const sizeOrder = ['XXXL', 'XXL', 'XL', 'L', 'M', 'S', 'XS', 'XXS', 'XXXS']\n  const sizes = []\n  const selectedSize = item.subtitle.split(' - ')[1]\n  variants.forEach((v) => {\n    const { attributes } = v\n    return attributes?.forEach((a) => {\n      if (a?.name?.toLowerCase()?.indexOf('size') >= 0) {\n        const disabled = selectedSize === a.value\n        sizes.push({\n          title: a?.value,\n          subtitle: selectedSize === a?.value ? 'Original Purchase' : '',\n          disabled,\n          sku: v.sku\n        })\n      }\n    })\n  })\n  if (sizeOrder.includes(sizes[0].title)) {\n    return sizes.sort((a, b) => {\n      return sizeOrder.indexOf(a.title) - sizeOrder.indexOf(b.title)\n    })\n  } else if (Number(sizes[0].title)) {\n    return sizes.sort((a, b) => Number(a.title) - Number(b.title))\n  } else {\n    return sizes.sort((a, b) => a.title < b.title)\n  }\n}\n\nexport function getReturnsHelperText (returnType) {\n  return returnType.indexOf('mail') >= 0 ? 'Mail back via' : 'Drop off at'\n}\n\nexport function getRefundPrice (orderItem) {\n  const quantity = orderItem.quantity && orderItem.quantity > 0 ? orderItem.quantity : 1\n  return (orderItem.product_amount_cents\n    ? (orderItem.product_amount_cents / 100 - orderItem.discount_amount_cents / 100 + orderItem.tax_amount_cents / 100) / quantity\n    : 0\n  ).toFixed(2)\n}\n\nexport function validStoreName (storeName, storeType) {\n  const returnMethods = store.getters['returns/policyReturnMethods']\n  // format returnMethods if not already formatted\n  const formattedReturnMethods = returnMethods.length && returnMethods[0].affiliates ? formatReturnMethods(returnMethods) : returnMethods\n  const groupedReturnMethods = formattedReturnMethods.reduce(function (storage, item) {\n    var group = item.type\n    storage[group] = storage[group] || []\n    storage[group].push(item)\n    return storage\n  }, {}) // group by type\n\n  const storeNamesArr = groupedReturnMethods[storeType] && groupedReturnMethods[storeType].map((type) => type?.customer_facing_carrier.toLowerCase())\n  if (storeNamesArr) {\n    return storeNamesArr.some((name) => {\n      var found = false\n      if (name === 'staples') {\n        found = whiteListedStaplesStores.some((store) => store === storeName.toLowerCase())\n      } else if (name === 'usps') {\n        found = whiteListedUSPSStores.some((store) => {\n          return store === storeName.toLowerCase()\n        })\n      } else {\n        found = storeName.toLowerCase().includes(name)\n      }\n      return found\n    })\n  }\n  return true\n}\n\nexport function getDistanceValue (measure) {\n  return parseFloat(measure?.replace(/,/g, '').split(' ')[0]) // separate unit part and return only value as a number\n}\n\nexport function isStoreWithinDistance (distance, maxDistance) {\n  return getDistanceValue(distance) <= maxDistance\n}\n\nexport function getLookupKeys (keysArr, checkingString) {\n  // ${state}_${zipCode}_${city}_${name}_${type}_info\n  const splitCheckingStringArr = checkingString.split('_')\n  const reqdKeys = keysArr.filter((lookupKey) => {\n    const splitLookupStringArr = lookupKey.split('_')\n    if (splitCheckingStringArr.length === 6 && splitCheckingStringArr[0] === splitLookupStringArr[0] && splitCheckingStringArr[1] === splitLookupStringArr[1] && splitCheckingStringArr[2] === splitLookupStringArr[2] && splitCheckingStringArr[5] === splitLookupStringArr[5]) {\n      return true\n    }\n    return false\n  })\n  return reqdKeys\n}\n\nexport const delay = ms => {\n  return new Promise(resolve => setTimeout(resolve, ms))\n}\n"]}]}