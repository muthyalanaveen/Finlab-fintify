          :last-item=\"index === exchangeSizeOptions.length-1\"\n          :size=\"option\"\n          :selected=\"selectedItem.SKU === option.SKU\"\n          :on-click=\"onSizeSelected\"\n        />\n      </template>\n    </v-radio-group>\n    <!-- </template> -->\n    <v-row\n      :class=\"{\n        'mt-4': $vuetify.breakpoint.smAndDown,\n      }\"\n    >\n      <v-col\n        :class=\"{\n          'half-width': $vuetify.breakpoint.mdAndUp\n        }\"\n        order-md=\"2\"\n        class=\"d-flex justify-end\"\n      >\n        <action-button\n          class=\"pr-0\"\n          @click.stop=\"togglePdpPage(true)\"\n          tertiary-color\n          :loading=\"isLoading\"\n          :block=\"$vuetify.breakpoint.smAndDown\"\n          large\n          text\n        >\n          {{ action2_Text }}\n        </action-button>\n        <PDPPage\n          :visible=\"showPDPPage\"\n          @close=\"togglePdpPage(false)\"\n          :return-reason-clicked=\"returnReasonClicked\"\n        />\n      </v-col>\n      <v-col\n        :class=\"{\n          'half-width': $vuetify.breakpoint.mdAndUp,\n        }\"\n      >\n        <action-button\n          @click.stop=\"onClickMethod\"\n          primary-color\n          :loading=\"isLoading || isMapLoading\"\n          :block=\"$vuetify.breakpoint.smAndDown\"\n          large\n          :disabled=\"isDisabled\"\n        >\n          {{ isDisabled ? action1_Text : action1_Selected_Text }}\n        </action-button>\n      </v-col>\n    </v-row>\n  </div>\n  <div v-else>\n    <v-skeleton-loader type=\"list-item\" />\n  </div>\n</template>\n\n<script>\nimport {\n  getSizeOptionsForSelectedVariant,\n  saveSelectedItemToStore,\n  getDefaultValue\n} from '@/controllers/ExchangesController'\nimport { mapGetters } from 'vuex'\nimport { EXCHANGE_REFUND, PAGE_TITLES } from 'branding/strings.json'\n\nexport default {\n  name: 'ExchangeTextView',\n  components: {\n    RadioItemCard: () => import('@/components/molecules/RadioItemCard.vue'),\n    HeaderTitle: () => import('@/components/molecules/HeaderTitle.vue'),\n    HeaderSmallTitle: () =>\n      import('@/components/molecules/HeaderSmallTitle.vue'),\n    PDPPage: () => import('@/components/pages/PDPPage.vue'),\n    ActionButton: () => import('@/components/atoms/ActionButton.vue')\n  },\n  props: {\n    returnReasonClicked: {\n      type: Function,\n      required: true\n    },\n    isMapLoading: {\n      type: Boolean,\n      required: true\n    }\n  },\n  computed: {\n    exchangeSizeOptions () {\n      return getSizeOptionsForSelectedVariant(this.item?.color)\n    },\n    defaultValue () {\n      return getDefaultValue()\n    },\n    item () {\n      return this.selectedItems[this.currentItemIndex]\n    },\n    ...mapGetters({\n      isLoading: 'exchange/isLoading',\n      currentItemIndex: 'returns/currentItemIndex',\n      selectedItems: 'order/selectedItems'\n    })\n  },\n  data: () => ({\n    showPDPPage: false,\n    title1_text: EXCHANGE_REFUND.EXCHANGE_SIZE_TITLE_1,\n    title2_text: EXCHANGE_REFUND.EXCHANGE_SIZE_TITLE_1,\n    action1_Text: EXCHANGE_REFUND.EXCHANGE_SIZE_ACTION_TEXT_1,\n    action1_Selected_Text: EXCHANGE_REFUND.EXCHANGE_SIZE_ACTION_TEXT_1_SELECTED,\n    action2_Text: EXCHANGE_REFUND.EXCHANGE_SIZE_ACTION_TEXT_2,\n    selectedItem: {},\n    isDisabled: true\n  }),\n  mounted () {\n    this.selectedItem = this.defaultValue\n    this.updateIsDisabled()\n  },\n  methods: {\n    onSizeSelected (selectedItem) {\n      if (!selectedItem.disabled) {\n        this.selectedItem = selectedItem\n        this.updateIsDisabled()\n      }\n    },\n    togglePdpPage (opening) {\n      if (opening) {\n        this.showPDPPage = true\n        document.title = PAGE_TITLES.PDP_PAGE\n      } else {\n        this.showPDPPage = false\n        document.title = PAGE_TITLES.EXCHANGE\n      }\n    },\n    updateIsDisabled () {\n      this.isDisabled = !(\n        this.selectedItem &&\n        !this.selectedItem.disabled &&\n        !this.isLoading\n      )\n    },\n    onClickMethod (event) {\n      saveSelectedItemToStore(this.selectedItem)\n      this.returnReasonClicked()\n      this.$emit('click', event)\n    }\n  }\n}\n</script>\n\n<style scoped>\n</style>\n"],"sourceRoot":"src/components/pages"}]}