AAA,2BAAA;AACA;AAnFA,CAAA","sourcesContent":["<template>\n  <div class=\"mt-12\">\n    <back-button\n      v-show=\"!disableModalGoBack && !disableBackButton\"\n      override-go-back\n      :override-on-click=\"() => openModal({ type: DIALOG_CARRIER_LOCATIONS })\"\n      aria-label=\"back\"\n    />\n    <div style=\"max-width: 664px; margin: auto;\">\n      <div class=\"d-flex justify-content-between align-center\">\n        <header-title>\n          {{ this.getSelectedReturnMethodDropOffDescription() }}\n        </header-title>\n        <v-btn\n          icon\n          style=\"background-color: #ffffff; color: #000000;\"\n          @click.stop=\"closeAction\"\n        >\n          <CloseIcon />\n        </v-btn>\n      </div>\n      <o-text bold class=\"mb-4\">\n        {{ this.getShippingAmt(selectedCarrier) }}\n      </o-text>\n      <div v-if=\"!isMailBack\">\n        <o-text>\n          {{ selectedCarrier.distance || \"\" }}  -\n          {{ selectedCarrier.formattedAddress || \"\" }}\n        </o-text>\n\n        <o-text>{{ selectedCarrier.formattedPhoneNumber || \"\" }}</o-text>\n      </div>\n      <div v-if=\"isRmaSubmitted\">\n        <a :href=\"'https://www.google.com/maps/dir/?api=1&destination=' + selectedCarrier.formattedAddress\" aria-label=\"Get directions\">Get directions</a>\n      </div>\n      <div class=\"mt-6\">\n        <div v-if=\"weekdayText && !isMailBack\">\n          <o-text\n            text-primary\n            lg\n          >\n            Opening hours\n          </o-text>\n          <div>\n            <li\n              v-for=\"({day, hours}, idx) in weekdayText\"\n              :key=\"idx\"\n              class=\"d-flex justify-content-between align-center mt-2\"\n            >\n              <o-text text-secondary md>\n                {{ day }}\n              </o-text>\n              <o-text\n                v-if=\"day.closed\"\n                text-secondary\n                md\n              >\n                CLOSED\n              </o-text>\n              <o-text\n                v-else\n                text-secondary\n                md\n              >\n                {{ hours }}\n              </o-text>\n            </li>\n          </div>\n        </div>\n        <v-divider\n          class=\"mt-6 mb-8\"\n          v-if=\"weekdayText\"\n        />\n        <!--          HOW IT WORKS SECTION             -->\n        <div class=\"d-flex flex-column\">\n          <o-text\n            text-primary\n            lg\n          >\n            {{ HOW_IT_WORKS_TITLE }}\n          </o-text>\n          <!-- in-store -->\n          <div\n            v-if=\"selectedCarrier.type === IN_STORE\"\n            class=\"d-flex flex-column align-center mt-4\"\n          >\n            <li class=\"d-flex align-center justify-between\">\n              <o-text>\n                {{ SHOW_RECEIPT_STAFF }}\n              </o-text>\n            </li>\n          </div>\n          <div\n            v-else-if=\"selectedCarrier.type === EXPRESS_RETURNS\"\n            class=\"d-flex flex-column align-center mt-4\"\n          >\n            <li class=\"d-flex align-center justify-between\">\n              <o-text>\n                {{ QR_CODE_IN_STORE }}\n              </o-text>\n            </li>\n          </div>\n          <!-- mail-back -->\n          <div\n            v-else-if=\"selectedCarrier.type === MAIL_BACK\"\n            class=\"d-flex flex-column mt-4\"\n          >\n            <li class=\"d-flex align-center justify-between\">\n              <o-text>\n                {{ this.mailBackHowItWorksDesc }}\n              </o-text>\n            </li>\n          </div>\n          <div\n            v-if=\"(selectedCarrier.type === MAIL_BACK || selectedCarrier.type === EXPRESS_RETURNS) && isExchange\"\n            class=\"d-flex flex-column align-center mt-4\"\n          >\n            <li class=\"d-flex align-center justify-between\">\n              <o-text>\n                {{ EXCHANGE_HOW_IT_WORKS }}\n              </o-text>\n            </li>\n          </div>\n        </div>\n        <v-divider\n          class=\"mt-8 mb-6\"\n        />\n        <!--        BENEFITS SECTION          -->\n        <div\n          v-if=\"selectedCarrier.type !== MAIL_BACK\"\n          class=\"d-flex flex-column\"\n        >\n          <o-text text-primary lg>\n            {{ BENEFITS_TITLE }}\n          </o-text>\n          <!-- in-store -->\n          <div\n            v-if=\"selectedCarrier.type === IN_STORE\"\n            class=\"d-flex flex-column mt-4\"\n          >\n            <li class=\"d-flex justify-between\">\n              <o-text>\n                {{ BENEFITS_NO_PRINT_DESC }}\n              </o-text>\n            </li>\n            <li class=\"d-flex align-center justify-between mt-6\">\n              <o-text>\n                {{ BENEFITS_SUSTAINABLE_DESC }}\n              </o-text>\n            </li>\n          </div>\n          <!-- express-return -->\n          <div\n            v-else-if=\"selectedCarrier.type === EXPRESS_RETURNS\"\n            class=\"d-flex flex-column mt-4\"\n          >\n            <li class=\"d-flex justify-between\">\n              <o-text>\n                {{ BENEFITS_NO_PRINT_DESC }}\n              </o-text>\n            </li>\n          </div>\n          <!-- mail-back -->\n        </div>\n        <!--        REQUIREMENTS SECTION          -->\n        <div\n          v-if=\"selectedCarrier.type === MAIL_BACK\"\n          class=\"d-flex flex-column\"\n        >\n          <o-text text-primary lg>\n            {{ REQUIREMENTS_TITLE }}\n          </o-text>\n          <!-- mail-back -->\n          <div\n            v-if=\"selectedCarrier.type === MAIL_BACK\"\n            class=\"d-flex flex-column mt-4\"\n          >\n            <li class=\"d-flex justify-between mt-6\">\n              <o-text>\n                {{ REQUIREMENTS_SHIPPING_PACKAGE }}\n              </o-text>\n            </li>\n            <li class=\"d-flex justify-between mt-6\">\n              <o-text>\n                {{ REQUIREMENTS_PRINTING_LABEL }}\n              </o-text>\n            </li>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport OText from '@/components/atoms/OText.vue'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\nimport { RETURN_METHOD_DIALOGS } from 'branding/strings.json'\nimport BackButton from '@/components/atoms/BackButton.vue'\nimport { mapGetters, mapMutations } from 'vuex'\nimport { DIALOG_CARRIER_LOCATIONS } from '@/store/constants'\nimport { CONSTANTS } from '@/constants/index.js'\nimport { isExchangeOrder, isMailBackReturn, isUSPSCarrier, getSelectedReturnMethodDropOffDescription, getShippingAmt } from '@/controllers/ReturnMethodsController.js'\nimport CloseIcon from 'vue-material-design-icons/Close.vue'\n\nexport default {\n  components: { OText, HeaderTitle, BackButton, CloseIcon },\n  computed: {\n    ...mapGetters({\n      selectedCarrier: 'returns/getSelectedCarrier',\n      disableModalGoBack: 'returns/getDisableModalGoBack',\n      selectedReturnMethod: 'returns/getSelectedReturnMethod',\n      exchangeList: 'order/exchangeList'\n    }),\n    weekdayText () {\n      const weekdayText = this.selectedCarrier?.openingHours?.weekday_text\n      if (weekdayText) {\n        return weekdayText.map((day) => {\n          const arr = day.split(/:(.+)/)\n          return { day: arr[0], hours: arr[1] }\n        })\n      } else {\n        return ''\n      }\n    },\n    isExchange () {\n      return isExchangeOrder(this.exchangeList)\n    },\n    isUSPS () {\n      return isUSPSCarrier(this.selectedCarrier)\n    },\n    isMailBack () {\n      return isMailBackReturn(this.selectedReturnMethod)\n    }\n  },\n  props: {\n    disableBackButton: {\n      required: false,\n      type: Boolean,\n      default: false\n    },\n    isRmaSubmitted: {\n      required: true,\n      type: Boolean\n    }\n  },\n  methods: {\n    ...mapMutations({\n      openModal: 'returns/openModal'\n    }),\n    getSelectedReturnMethodDropOffDescription () {\n      return getSelectedReturnMethodDropOffDescription(this.selectedCarrier)\n    },\n    getShippingAmt () {\n      return getShippingAmt(this.selectedCarrier)\n    },\n    closeAction (event) {\n      this.$emit('closeClicked', event)\n    }\n  },\n  data () {\n    return {\n      HOW_IT_WORKS_TITLE: RETURN_METHOD_DIALOGS.HOW_IT_WORKS_TITLE,\n      QR_CODE_IN_STORE: RETURN_METHOD_DIALOGS.QR_CODE_IN_STORE,\n      BENEFITS_NO_PRINT_DESC: RETURN_METHOD_DIALOGS.BENEFITS_NO_PRINT_DESC,\n      BENEFITS_CASHBACK_DESC: RETURN_METHOD_DIALOGS.BENEFITS_CASHBACK_DESC,\n      BENEFITS_SUSTAINABLE_DESC:\n        RETURN_METHOD_DIALOGS.BENEFITS_SUSTAINABLE_DESC,\n      MAIL_BACK: CONSTANTS.RETURN_METHODS.MAIL_BACK,\n      IN_STORE: CONSTANTS.RETURN_METHODS.IN_STORE,\n      EXPRESS_RETURNS: CONSTANTS.RETURN_METHODS.EXPRESS_RETURNS,\n      BENEFITS_TITLE: RETURN_METHOD_DIALOGS.BENEFITS_TITLE,\n      BENEFITS_PRINT_DESC: RETURN_METHOD_DIALOGS.BENEFITS_PRINT_DESC,\n      REQUIREMENTS_TITLE: RETURN_METHOD_DIALOGS.REQUIREMENTS_TITLE,\n      REQUIREMENTS_SHIPPING_PACKAGE: RETURN_METHOD_DIALOGS.REQUIREMENTS_SHIPPING_PACKAGE,\n      REQUIREMENTS_PRINTING_LABEL: RETURN_METHOD_DIALOGS.REQUIREMENTS_PRINTING_LABEL,\n      SHOW_RECEIPT_STAFF: RETURN_METHOD_DIALOGS.SHOW_RECEIPT_STAFF,\n      MAIL_BACK_HOW_IT_WORKS_DESC: RETURN_METHOD_DIALOGS.MAIL_BACK_HOW_IT_WORKS_DESC,\n      EXCHANGE_HOW_IT_WORKS: RETURN_METHOD_DIALOGS.EXCHANGE_HOW_IT_WORKS,\n      MAIL_BACK_HOW_IT_WORKS_DESC_MAIL_INNOVATIONS: RETURN_METHOD_DIALOGS.MAIL_BACK_HOW_IT_WORKS_DESC_MAIL_INNOVATIONS,\n      EXCHANGE_ITEM_RETURN_SHIP:\n        RETURN_METHOD_DIALOGS.EXCHANGE_ITEM_RETURN_SHIP,\n      DIALOG_CARRIER_LOCATIONS,\n      mailBackHowItWorksDesc: ''\n    }\n  },\n  mounted () {\n    this.mailBackHowItWorksDesc = this.isUSPS ? this.MAIL_BACK_HOW_IT_WORKS_DESC_MAIL_INNOVATIONS : this.MAIL_BACK_HOW_IT_WORKS_DESC\n  }\n}\n</script>\n\n<style></style>\n"],"sourceRoot":"src/components/organism"}]}