AEA;AAD+B,KAAlC,CAAP;AAGD;AALoC,CAAhC;AAQP,OAAO,IAAMgB,gBAAgB,GAAG;AAC9BC,EAAAA,SAD8B,qBACnBjB,MADmB,EACX;AACjB,WAAOb,UAAU,CAACmB,IAAX,CAAgB,SAAhB,oBACFN,MADE,EAAP;AAGD;AAL6B,CAAzB;AAQP,OAAO,IAAMkB,uBAAuB,GAAG;AACrCC,EAAAA,gBADqC,4BACnBnB,MADmB,EACX;AACxB,WAAOb,UAAU,CAACmB,IAAX,CAAgB,oBAAhB,oBACFN,MADE,EAAP;AAGD;AALoC,CAAhC;AAQP,OAAO,IAAMoB,qBAAqB,GAAG;AACnCC,EAAAA,cADmC,0BACnBrB,MADmB,EACX;AACtB,WAAOb,UAAU,CAACmB,IAAX,CAAgB,0BAAhB,oBACFN,MADE,EAAP;AAGD;AALkC,CAA9B;AAQP,OAAO,IAAMsB,gBAAgB,GAAG;AAC9BC,EAAAA,SAD8B,qBACnBvB,MADmB,EACX;AACjB,WAAOb,UAAU,CAACW,KAAX,CAAiB,YAAjB,oBACFE,MADE,EAAP;AAGD;AAL6B,CAAzB;AAQP,OAAO,IAAMwB,wBAAwB,GAAG;AACtCC,EAAAA,iBADsC,6BACnBzB,MADmB,EACX;AACzB,WAAOb,UAAU,CAACmB,IAAX,CAAgB,0BAAhB,oBACFN,MADE,EAAP;AAGD;AALqC,CAAjC","sourcesContent":["import Vue from 'vue'\nimport axios from 'axios'\nimport VueAxios from 'vue-axios'\nimport JwtService from './jwt.service'\nimport { apiUrl } from './config'\nimport { authToken, optiturnId } from '@/common/config'\n\nconst ApiService = {\n  init () {\n    Vue.use(VueAxios, axios)\n    Vue.axios.defaults.baseURL = apiUrl\n    JwtService.saveToken(authToken)\n    ApiService.setHeader()\n  },\n\n  setHeader () {\n    Vue.axios.defaults.headers.common.Authorization = `Token ${JwtService.getToken()}`\n    Vue.axios.defaults.headers['X-OPTITURN-ID'] = optiturnId\n  },\n\n  query (resource, params) {\n    return Vue.axios.get(resource, params).catch((error) => {\n      throw new Error(`[Rousseau] ApiService ${error}`)\n    })\n  },\n\n  get (resource, slug = '') {\n    return Vue.axios.get(`${resource}/${slug}`).catch((error) => {\n      throw new Error(`[Rousseau] ApiService ${error}`)\n    })\n  },\n\n  post (resource, params) {\n    return Vue.axios.post(`${resource}`, params)\n  },\n\n  update (resource, slug, params) {\n    return Vue.axios.put(`${resource}/${slug}`, params)\n  },\n\n  put (resource, params) {\n    return Vue.axios.put(`${resource}`, params)\n  },\n\n  delete (resource) {\n    return Vue.axios.delete(resource).catch((error) => {\n      throw new Error(`[Rousseau] ApiService ${error}`)\n    })\n  }\n}\n\nexport default ApiService\n\nexport const FetchOrderService = {\n  getOrderById (params) {\n    return ApiService.query('v1/orders/?', {\n      params: params\n    })\n  },\n  getOrderByHash (hash) {\n    return ApiService.query(`v1/orders/${hash}`)\n  }\n}\n\nexport const VariantInventoryService = {\n  getVariantsBySKU (params) {\n    return ApiService.query('v1/variants/?', {\n      params: params\n    })\n  }\n}\n\nexport const CreateRMAService = {\n  createRMA (params) {\n    return ApiService.post('v1/rma/', {\n      ...params\n    })\n  }\n}\n\nexport const GetShippingLabelService = {\n  getShippingLabel (params) {\n    return ApiService.post('v1/shipping_label/', {\n      ...params\n    })\n  }\n}\n\nexport const GetQRCodeLabelService = {\n  getQRCodeLabel (params) {\n    return ApiService.post('v1/express_return_label/', {\n      ...params\n    })\n  }\n}\n\nexport const GetPolicyService = {\n  getPolicy (params) {\n    return ApiService.query('v1/policy/', {\n      ...params\n    })\n  }\n}\n\nexport const IsTrustWorthyUserService = {\n  isTrustWorthyUser (params) {\n    return ApiService.post('v1/exchange_eligibility/', {\n      ...params\n    })\n  }\n}\n"]}]}