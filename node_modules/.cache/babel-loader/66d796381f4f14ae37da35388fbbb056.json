{ trimExtraSpacesFromStringsInObject } from '@/utils'\nimport router from '@/router'\nimport { CONSTANTS } from '@/constants'\n\nconst protectRoute = async (cb, protect = true) => {\n  if (protect) {\n    Vue.axios.defaults.headers.common.Authorization = `Bearer ${JwtService.getAccessToken()}`\n  }\n\n  return cb().catch((error) => {\n    if (protect && error?.response?.status === CONSTANTS.ERROR_CODES.FORBIDDEN) {\n      router.push('/orderandzipcode')\n    } else {\n      throw error\n    }\n  })\n}\n\nconst ApiService = {\n  init () {\n    Vue.use(VueAxios, axios)\n    Vue.axios.defaults.baseURL = apiUrl\n    ApiService.setHeader()\n  },\n\n  setHeader () {\n    Vue.axios.defaults.headers['X-OPTITURN-ID'] = optiturnId\n  },\n\n  query: (resource, params, protect = true) =>\n    protectRoute(\n      () => Vue.axios.get(resource, trimExtraSpacesFromStringsInObject(params)),\n      protect\n    ),\n\n  get: (resource, slug = '', protect = true) =>\n    protectRoute(() => Vue.axios.get(`${resource}/${slug}`), protect),\n\n  post: (resource, params, protect = true) =>\n    protectRoute(() => Vue.axios.post(`${resource}`, params), protect),\n\n  update: (resource, slug, params, protect = true) =>\n    protectRoute(() => Vue.axios.put(`${resource}/${slug}`, params), protect),\n\n  put: (resource, params, protect = true) =>\n    protectRoute(() => Vue.axios.put(`${resource}`, params), protect),\n\n  delete: (resource, protect = true) =>\n    protectRoute(() => Vue.axios.delete(resource), protect)\n}\n\nexport default ApiService\n\nexport const FetchOrderService = {\n  getOrder () {\n    return ApiService.query(`${CONSTANTS.API_VERSION}/orders/`)\n  }\n}\n\nexport const AuthService = {\n  getAccessToken (params) {\n    return ApiService.query(\n      `${CONSTANTS.API_VERSION}/auth/?`,\n      {\n        params: trimExtraSpacesFromStringsInObject(params)\n      },\n      false\n    )\n  }\n}\n\nexport const VariantInventoryService = {\n  getVariantsBySKU (params) {\n    return ApiService.query(`${CONSTANTS.API_VERSION}/variants/?`, {\n      params\n    })\n  }\n}\n\nexport const CreateRMAService = {\n  createRMA (params) {\n    return ApiService.post(`${CONSTANTS.API_VERSION}/rma/`, {\n      ...params\n    })\n  }\n}\n\nexport const GetShippingLabelService = {\n  getShippingLabel (params) {\n    return ApiService.post(`${CONSTANTS.API_VERSION}/shipping_label/`, {\n      ...params\n    })\n  }\n}\n\nexport const GetQRCodeLabelService = {\n  getQRCodeLabel (params) {\n    return ApiService.post(`${CONSTANTS.API_VERSION}/express_return_label/`, {\n      ...params\n    })\n  }\n}\n\nexport const GetConfigService = {\n  getConfig (params) {\n    return ApiService.query(`${CONSTANTS.API_VERSION}/retailer_config/`, {\n      ...params\n    })\n  }\n}\n\nexport const IsTrustWorthyUserService = {\n  isTrustWorthyUser (params) {\n    return ApiService.post(`${CONSTANTS.API_VERSION}/exchange_eligibility/`, {\n      ...params\n    })\n  }\n}\n\nexport const DestinationService = {\n  destination (params) {\n    return ApiService.post(`${CONSTANTS.API_VERSION}/destination/`, {\n      ...params\n    })\n  }\n}\n"]}]}