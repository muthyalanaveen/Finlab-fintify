AA;AAMA,GAdA;AAeA,EAAA,QAAA,EAAA;AACA,IAAA,WADA,yBACA;AAAA;;AACA,aAAA,sBAAA,OAAA,wDAAA,aAAA,GAAA,qBAAA,CAAA,WAAA,GAAA,SAAA;AACA,KAHA;AAIA,IAAA,YAJA,0BAIA;AAAA;;AACA,UAAA,YAAA,GAAA,uBAAA,OAAA,0DAAA,gBAAA,qBAAA,KAAA,OAAA,mDAAA,eAAA,gBAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA;;AACA,UAAA,YAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,SAAA;AACA;;AACA,aAAA,YAAA,CAAA,CAAA,CAAA,GAAA,IAAA,GAAA,YAAA,CAAA,CAAA,CAAA;AACA,KAVA;AAWA,IAAA,UAXA,wBAWA;AAAA;;AACA,UAAA,YAAA,GAAA,uBAAA,OAAA,0DAAA,gBAAA,qBAAA,KAAA,OAAA,mDAAA,eAAA,gBAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA;;AACA,UAAA,YAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,UAAA,WAAA,GAAA,YAAA,CAAA,CAAA,CAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,aAAA,WAAA,CAAA,MAAA,CAAA,UAAA,CAAA,EAAA,KAAA;AAAA,eAAA,KAAA,KAAA,CAAA;AAAA,OAAA,EAAA,IAAA,CAAA,GAAA,CAAA;AACA;AAlBA,GAfA;AAmCA,EAAA,OAAA,EAAA;AACA,IAAA,kBADA,gCACA;AACA,aAAA,cAAA,CAAA,KAAA,OAAA,CAAA;AACA;AAHA,GAnCA;AAwCA,EAAA,KAAA,EAAA;AACA,IAAA,OAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,QAAA,EAAA;AAFA,KADA;AAKA,IAAA,sBAAA,EAAA;AACA,MAAA,IAAA,EAAA,QADA;AAEA,MAAA,QAAA,EAAA;AAFA,KALA;AASA,IAAA,KAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,QAAA,EAAA;AAFA,KATA;AAaA,IAAA,WAAA,EAAA;AACA,MAAA,IAAA,EAAA,QADA;AAEA,MAAA,QAAA,EAAA;AAFA;AAbA;AAxCA,CAAA","sourcesContent":["<template>\n  <div\n    class=\"px-4\"\n    :class=\"{\n      'bg-well': index === 0 && $vuetify.breakpoint.mdAndUp,\n      'white': index !== 0,\n      'mr-2 ml-2 map-tile-sm': $vuetify.breakpoint.smAndDown,\n      'mr-1 map-tile-lg': $vuetify.breakpoint.mdAndUp,\n    }\"\n    @click=\"tileClicked(index, true)\"\n  >\n    <div class=\"d-flex align-center justify-content-between pt-2\">\n      <o-text\n        text-primary\n        bold\n        lg\n        class=\"pt-2\"\n        :aria-label=\"carrier.customer_facing_carrier + ', ' + streetName\"\n      >\n        {{ carrier.customer_facing_carrier || 'N/A' }}\n      </o-text>\n      <o-text\n        text-primary\n        bold\n        lg\n        class=\"pt-2\"\n      >\n        {{ this.getShippingAmtText() }}\n      </o-text>\n    </div>\n    <div class=\"mt-1\">\n      <section class=\"d-flex pb-4\">\n        <o-text\n          text-secondary\n          class=\"mb-0\"\n        >\n          {{ addressShown || 'N/A' }}\n        </o-text>\n      </section>\n      <section class=\"d-flex justify-space-between pb-4 mb-2\">\n        <div>\n          <div v-if=\"carrier.openingHours.isOpen !== undefined\" class=\"d-flex align-center\">\n            <v-icon\n              small\n              class=\"mr-2\"\n              aria-label=\"schedule\"\n              role=\"img\"\n            >\n              schedule\n            </v-icon>\n            <o-text sm>\n              Today: {{ carrier.openingHours.todayOpeningTime || 'N/A' }} -\n              {{ carrier.openingHours.todayClosingTime || 'N/A' }}\n            </o-text>\n          </div>\n          <div v-if=\"SUSTAINABLE\" class=\"d-flex align-center\">\n            <v-icon\n              small\n              class=\"mr-2\"\n              aria-hidden=\"true\"\n              role=\"img\"\n            >\n              spa\n            </v-icon>\n            <o-text sm>\n              {{ SUSTAINABLE }}\n            </o-text>\n          </div>\n        </div>\n        <action-button\n          @click=\"handleCarrierSelection(carrier)\"\n          primary-color\n          :aria-label=\"'Choose Staples, ' + addressShown\"\n          large\n        >\n          Choose\n        </action-button>\n      </section>\n    </div>\n    <v-divider\n      class=\"my-0 map-tile-divider\"\n      v-if=\"$vuetify.breakpoint.mdAndUp\"\n    />\n  </div>\n</template>\n\n<script>\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport OText from '@/components/atoms/OText.vue'\nimport { RETURN_METHOD_SCREENS } from 'branding/strings.json'\nimport { DIALOG_CARRIER_INFORMATION, DIALOG_CARRIER_LOCATIONS } from '@/store/constants'\nimport { getShippingAmt } from '@/controllers/ReturnMethodsController.js'\n\nexport default {\n  name: 'ShippingLocationCard',\n  components: {\n    ActionButton,\n    OText\n  },\n  mounted () {},\n  data () {\n    return ({\n      PACKAGE_AND_PRINT: RETURN_METHOD_SCREENS.PACKAGE_AND_PRINT,\n      NO_PACKAGE_AND_PRINT: RETURN_METHOD_SCREENS.NO_PACKAGE_AND_PRINT,\n      DIALOG_CARRIER_INFORMATION,\n      DIALOG_CARRIER_LOCATIONS\n    })\n  },\n  computed: {\n    SUSTAINABLE () {\n      return this.carrier?.isSustainable ? RETURN_METHOD_SCREENS.SUSTAINABLE : undefined\n    },\n    addressShown () {\n      const splitAddress = this.carrier?.formattedAddress ? this.carrier?.formattedAddress.split(',') : []\n      if (splitAddress.length < 2) {\n        return undefined\n      }\n      return splitAddress[0] + ', ' + splitAddress[1]\n    },\n    streetName () {\n      const splitAddress = this.carrier?.formattedAddress ? this.carrier?.formattedAddress.split(',') : []\n      if (splitAddress.length < 2) {\n        return 'N/A'\n      }\n      const secondSplit = splitAddress[0].split(' ')\n      return secondSplit.filter((x, index) => index !== 0).join(' ')\n    }\n  },\n  methods: {\n    getShippingAmtText () {\n      return getShippingAmt(this.carrier)\n    }\n  },\n  props: {\n    carrier: {\n      type: Object,\n      required: true\n    },\n    handleCarrierSelection: {\n      type: Function,\n      required: true\n    },\n    index: {\n      type: Number,\n      required: true\n    },\n    tileClicked: {\n      type: Function,\n      required: true\n    }\n  }\n}\n</script>\n\n<style scoped>\n  .map-tile-divider {\n    border-color: grey; width: 100%;\n  }\n  .map-tile-lg {\n    width: 100%;\n  }\n  .map-tile-sm {\n    width: 375px;\n    box-shadow: 2px 2px 4px #d8d8d8;\n  }\n</style>\n"],"sourceRoot":"src/components/organism"}]}