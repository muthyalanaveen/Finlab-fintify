ADA,EACA,MADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,gBAAA,QAAA,EAAA;AACA,cAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CACA,iCADA,EAEA,QAFA;AAIA;AACA,WATA,EAUA,KAVA,CAUA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,CAAA,KAAA,CAAA;AAAA,cAAA,GAAA,EAAA;AAAA,aAAA;;AACA,YAAA,MAAA,CAAA,iCAAA;AACA,WAbA,EAcA,OAdA,CAcA,YAAA;AACA,YAAA,MAAA,CAAA,iCAAA;AACA,WAhBA;AAiBA,SAlBA,MAkBA;AACA,eAAA,iCAAA;AACA;AACA,OAnCA,MAmCA;AACA,aAAA,iCAAA;AACA;AACA,KA5FA;AA6FA,IAAA,iCA7FA,+CA6FA;AACA,WAAA,KAAA,GAAA,KAAA,wBAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,aAAA,KAAA,KAAA,CAAA,UAAA,GAAA,gBAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,oBAAA,EAAA,KAAA,kBAAA,CAAA,MAAA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,KAlGA;AAmGA,IAAA,iBAnGA,6BAmGA,KAnGA,EAmGA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MAAA,CAAA,kBAAA,GAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,oBAAA,EAAA,MAAA,CAAA,kBAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,EAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KAvGA;AAwGA,IAAA,iBAxGA,+BAwGA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,+BACA,MAAA,CAAA,IADA;AAAA,kDAEA,MAAA,CAAA,kBAAA,CAAA,OAFA,wBAcA,MAAA,CAAA,kBAAA,CAAA,MAdA;AAAA;;AAAA;AAGA,oBAAA,MAAA,CAAA,gBAAA,KAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,aAAA,MAAA,CAAA,KAAA,CAAA,UAAA;AACA,iBAFA,MAEA;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,wBAAA;;AACA,kBAAA,MAAA,CAAA,KAAA,GAAA,EAAA;;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CACA,oBADA,EAEA,MAAA,CAAA,kBAAA,CAAA,MAFA;AAIA;;AAZA;;AAAA;AAeA,gBAAA,MAAA,CAAA,KAAA,GAAA,MAAA,CAAA,qBAAA;;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CACA,oBADA,EAEA,MAAA,CAAA,kBAAA,CAAA,OAFA;;AAIA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,aAAA,MAAA,CAAA,KAAA,CAAA,UAAA,GAAA,UAAA;;AApBA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAyBA;AAjIA;AAjFA,CAAA","sourcesContent":["<template>\n  <div class=\"mb-16\">\n    <v-row\n      :class=\"{\n        'mt-4 mb-16': $vuetify.breakpoint.smAndDown,\n        'mx-2 my-6': $vuetify.breakpoint.mdAndUp,\n      }\"\n    >\n      <o-toast\n        color=\"snackbarGreen\"\n        :timeout=\"timeout\"\n        left-icon=\"check_circle\"\n        :visible=\"show\"\n        @close=\"show = false\"\n        :title=\"this.TOAST_MESSAGE\"\n      />\n      <v-col\n        fixed\n        top\n        v-if=\"$vuetify.breakpoint.mdAndUp\"\n        md=\"3\"\n        class=\"mr-8 justify-md-start\"\n      >\n        <v-col style=\"position: fixed\" md=\"3\">\n          <return-reason-header\n            :count=\"selectedItems.length\"\n            :index=\"currentItemIndex\"\n            :title=\"title\"\n            :show-feedback=\"this.step === RETURN_REASON_STEP.PRIMARY\"\n            @click.stop=\"backButtonClicked\"\n            :item=\"selectedItems[currentItemIndex]\"\n            @feedbackClicked=\"showReturnFeedback = true\"\n          />\n        </v-col>\n      </v-col>\n    </v-row>\n    <v-row class=\"justify-center ml-4 mt-n12\">\n      <v-col\n        md=\"5\"\n        lg=\"5\"\n        sm=\"8\"\n        xs=\"12\"\n        class=\"px-4\"\n      >\n        <div v-if=\"$vuetify.breakpoint.smAndDown\">\n          <return-reason-header\n            :count=\"selectedItems.length\"\n            :index=\"currentItemIndex\"\n            :title=\"title\"\n            :show-feedback=\"this.step === RETURN_REASON_STEP.PRIMARY\"\n            @click.stop=\"backButtonClicked\"\n            :item=\"selectedItems[currentItemIndex]\"\n            class=\"mt-n4\"\n            :class=\"{ 'ml-n3': $vuetify.breakpoint.smAndDown }\"\n            @feedbackClicked=\"showReturnFeedback = true\"\n          />\n        </div>\n        <v-list\n          class=\"mx-n3 mb-2\"\n          :class=\"{\n            'mt-5': $vuetify.breakpoint.smAndDown,\n            'mt-1': $vuetify.breakpoint.mdAndUp,\n          }\"\n          md=\"3\"\n          v-if=\"this.step === RETURN_REASON_STEP.PRIMARY\"\n        >\n          <div v-if=\"exchangesLoading\">\n            <list-item-skeleton :list=\"this.reasons\" />\n          </div>\n          <template v-for=\"(reasonToDisplay, index) in this.reasons\" v-else>\n            <list-item\n              :key=\"index\"\n              :index=\"index\"\n              :item=\"reasonToDisplay\"\n              @onClick.stop=\"returnReasonClicked\"\n              class=\"ma-0 pa-0\"\n              :show-loading=\"isLoading && reason.id === reasonToDisplay.id\"\n            />\n          </template>\n        </v-list>\n        <div class=\"d-flex justify-center mb-8\">\n          <action-button\n            v-if=\"this.showFeedbackButton\"\n            text\n            color=\"primary\"\n            @click.stop=\"showReturnFeedback = true\"\n          >\n            {{ LEAVE_FEEDBACK }}\n          </action-button>\n        </div>\n        <refund-or-exchange\n          v-if=\"this.step === RETURN_REASON_STEP.ACTION\"\n          :return-reason-clicked=\"returnReasonClicked\"\n          :is-map-loading=\"isMapLoading\"\n        />\n        <return-reason-feedback\n          :visible=\"showReturnFeedback\"\n          @close=\"showReturnFeedback = false\"\n          @click.stop=\"feedbackSubmitted\"\n        />\n        <v-overlay :value=\"loading\">\n          <v-progress-circular\n            v-if=\"loading\"\n            :width=\"3\"\n            :active=\"loading\"\n            color=\"gray\"\n            :indeterminate=\"loading\"\n            class=\"justify-center align-center\"\n          />\n        </v-overlay>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport ListItem from '@/components/molecules/ListItem.vue'\nimport ListItemSkeleton from '@/components/molecules/ListItemSkeleton.vue'\nimport ReturnReasonHeader from '@/components/organism/ReturnReasonHeader.vue'\nimport ReturnReasonFeedback from './ReturnReasonFeedback.vue'\nimport { EXCHANGE_REFUND, RETURN_REASON } from 'branding/strings.json'\nimport OToast from '@/components/atoms/OToast.vue'\nimport { mapGetters } from 'vuex'\nimport RefundOrExchange from '@/components/organism/RefundOrExchange.vue'\nimport { checkForExpressDropoffOptions } from '@/controllers/map.js'\nimport { CONSTANTS } from '@/constants'\nimport { IS_TRUST_WORTHY_USER } from '@/store/constants.js'\nimport { isExchangesEnabled } from '@/controllers/exchangesController'\n\nexport default {\n  components: {\n    ListItem,\n    ListItemSkeleton,\n    ReturnReasonHeader,\n    ReturnReasonFeedback,\n    ActionButton,\n    OToast,\n    RefundOrExchange\n  },\n  data: () => ({\n    LETS_MAKE_IT_RIGHT_TITLE: EXCHANGE_REFUND.LETS_MAKE_IT_RIGHT_TITLE,\n    IT_DID_NOT_WORK_OUT_TITLE: EXCHANGE_REFUND.IT_DID_NOT_WORK_OUT_TITLE,\n    WHY_ARE_YOU_RETURNING: RETURN_REASON.WHY_ARE_YOU_RETURNING,\n    TELL_US_MORE: RETURN_REASON.TELL_US_MORE,\n    PLEASE_EXPLAIN_FURTHER: RETURN_REASON.PLEASE_EXPLAIN_FURTHER,\n    LEAVE_FEEDBACK: RETURN_REASON.LEAVE_FEEDBACK,\n    TOAST_MESSAGE: RETURN_REASON.TOAST_MESSAGE,\n    loading: false,\n    showReturnFeedback: false,\n    title: RETURN_REASON.WHY_ARE_YOU_RETURNING,\n    show: false,\n    timeout: 5000,\n    isMapLoading: false,\n    RETURN_REASON_STEP: CONSTANTS.RETURN_REASON_STEP,\n    isLoading: false\n  }),\n  watch: {\n    loading (val) {\n      if (!val) return\n      setTimeout(() => this.refresh(), 1000)\n    },\n    $route (val) {\n      if (val.name === 'reasons') {\n        this.$store.commit(\n          'returns/updateStep',\n          this.RETURN_REASON_STEP.PRIMARY\n        )\n      } else {\n        this.$store.commit(\n          'returns/updateStep',\n          this.RETURN_REASON_STEP.ACTION\n        )\n      }\n    }\n  },\n  beforeUnmount () {},\n  computed: {\n    ...mapGetters({\n      order: 'order/order',\n      exchangeEligibility: 'exchange/getExchangeEligibility',\n      reasons: 'returns/reasons',\n      step: 'returns/step',\n      currentItemIndex: 'returns/currentItemIndex',\n      selectedItems: 'order/selectedItems',\n      reason: 'returns/reason',\n      policies: 'returns/policies',\n      exchangesLoading: 'exchange/isLoading',\n      policyReturnMethods: 'returns/policyReturnMethods',\n      returnMethods: 'returns/returnMethods'\n    }),\n    item () {\n      return this.selectedItems[this.currentItemIndex]\n    },\n    showFeedbackButton () {\n      return (\n        this.step === this.RETURN_REASON_STEP.PRIMARY &&\n        this.policies.return_feedback_allowed\n      )\n    }\n  },\n  mounted () {\n    if (this.order.order_hash === '') {\n      this.$router.push('/orders/' + this.$route.params.id)\n    }\n    if (this.$route.name === 'reasons') {\n      this.$store.commit('returns/updateStep', this.RETURN_REASON_STEP.PRIMARY)\n    } else {\n      this.$store.commit('returns/updateStep', this.RETURN_REASON_STEP.ACTION)\n    }\n  },\n  methods: {\n    refresh () {\n      this.loading = false\n    },\n    async checkForMap () {\n      this.isMapLoading = true\n      const willHaveMap = await checkForExpressDropoffOptions(\n        this.returnMethods,\n        this.policyReturnMethods\n      )\n      this.isMapLoading = false\n      return willHaveMap\n    },\n    async returnReasonClicked (event) {\n      switch (this.step) {\n        case this.RETURN_REASON_STEP.PRIMARY:\n          this.returnReasonStepPrimary(\n            this.reasons[event.target.id],\n            this.selectedItems[this.currentItemIndex]\n          )\n          break\n        case this.RETURN_REASON_STEP.ACTION:\n          this.returnReasonStepAction()\n          break\n        default:\n          break\n      }\n    },\n    async returnReasonStepAction () {\n      if (this.currentItemIndex === this.selectedItems.length - 1) {\n        const hasMap = await this.checkForMap()\n        if (hasMap) {\n          this.$router.push(\n            '/orders/' + this.order.order_hash + '/mapshipping'\n          )\n        } else {\n          this.$router.push('/orders/' + this.order.order_hash + '/shipping')\n        }\n      } else {\n        this.title = this.WHY_ARE_YOU_RETURNING\n        this.$store.commit(\n          'returns/updateStep',\n          this.RETURN_REASON_STEP.PRIMARY\n        )\n        this.$store.commit('returns/incrementIndex', { root: true })\n        this.show = true\n      }\n    },\n    returnReasonStepPrimary (reason, selectedItem) {\n      this.$store.commit('returns/updateReason', {\n        reason: reason,\n        sku: selectedItem.sku\n      })\n\n      if (isExchangesEnabled()) {\n        const params = {\n          order_id: this.order.order_id,\n          momentary_rma: [\n            {\n              sku: selectedItem.sku,\n              return_cause: reason.name,\n              total_refund_amount_cents: selectedItem.total_refund_amount_cents,\n              units: selectedItem.quantity,\n              event_occurred_at: new Date()\n            }\n          ]\n        }\n        this.isLoading = true\n        if (this.exchangeEligibility === null) {\n          this.$store\n            .dispatch('order/' + IS_TRUST_WORTHY_USER, params)\n            .then((response) => {\n              if (response) {\n                this.$store.commit(\n                  'exchange/setExchangeEligibility',\n                  response\n                )\n              }\n            })\n            .catch((err) => {\n              console.error({ err })\n              this.isTrustWorthyUserResponseCallback()\n            })\n            .finally(() => {\n              this.isTrustWorthyUserResponseCallback()\n            })\n        } else {\n          this.isTrustWorthyUserResponseCallback()\n        }\n      } else {\n        this.isTrustWorthyUserResponseCallback()\n      }\n    },\n    isTrustWorthyUserResponseCallback () {\n      this.title = this.LETS_MAKE_IT_RIGHT_TITLE\n      this.$router.push('/orders/' + this.order.order_hash + '/refund-amount')\n      this.$store.commit('returns/updateStep', this.RETURN_REASON_STEP.ACTION)\n      this.isLoading = false\n    },\n    async feedbackSubmitted (event) {\n      this.showReturnFeedback = false\n      this.$store.commit('returns/updateStep', this.RETURN_REASON_STEP.ACTION)\n      this.title = ''\n    },\n    async backButtonClicked () {\n      switch (this.step) {\n        case this.RETURN_REASON_STEP.PRIMARY:\n          if (this.currentItemIndex === 0) {\n            this.$router.push('/orders/' + this.order.order_hash)\n          } else {\n            this.$store.commit('returns/decrementIndex')\n            this.title = ''\n            this.$store.commit(\n              'returns/updateStep',\n              this.RETURN_REASON_STEP.ACTION\n            )\n          }\n          break\n        case this.RETURN_REASON_STEP.ACTION:\n          this.title = this.WHY_ARE_YOU_RETURNING\n          this.$store.commit(\n            'returns/updateStep',\n            this.RETURN_REASON_STEP.PRIMARY\n          )\n          this.$router.push('/orders/' + this.order.order_hash + '/reasons')\n          break\n        default:\n          break\n      }\n    }\n  }\n}\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components/pages"}]}