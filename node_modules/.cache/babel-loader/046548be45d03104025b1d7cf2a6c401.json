    {{ customerKeepSubtitle }}\n          </header-small-title>\n        </v-row>\n\n        <div\n          :class=\"{\n            'd-flex align-center justify-center flex-wrap': this.$vuetify.breakpoint\n              .smAndDown,\n            'd-flex align-start justify-center flex-wrap': this.$vuetify.breakpoint.mdAndUp,\n          }\"\n        >\n          <product-card\n            class=\"keeps-card mt-4\"\n            v-for=\"(item, i) in selectedItems\"\n            :key=\"i\"\n            :height=\"determineImageSize()\"\n            :width=\"determineImageSize()\"\n            v-bind=\"item\"\n            :src=\"item.image_url\"\n            :selected=\"false\"\n            enable-text\n            display-pricing\n            no-border\n            basic\n          />\n        </div>\n        <v-col>\n          <action-button\n            @click.stop=\"handleNextClick\"\n            primary-color\n            :loading=\"isLoading\"\n            aria-label=\"Go to Review Return Page\"\n            :padding=\"'0px'\"\n            block\n            large\n          >\n            {{ \"Next\" }}\n          </action-button>\n        </v-col>\n      </div>\n    </div>\n  </div>\n  <div v-else>\n    <v-skeleton-loader type=\"card\" />\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\nimport { setSelectedReturnMethod } from '@/controllers/MapController.js'\nimport { CUSTOMER_KEEPS } from 'branding/strings.json'\nimport HeaderSmallTitle from '@/components/molecules/HeaderSmallTitle.vue'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\nimport BackButton from '@/components/atoms/BackButton.vue'\n\nexport default {\n  name: 'CustomerKeepsView',\n  components: {\n    ActionButton: () => import('@/components/atoms/ActionButton.vue'),\n    ProductCard: () => import('@/components/organism/ProductCard.vue'),\n    HeaderSmallTitle,\n    HeaderTitle,\n    BackButton\n  },\n  data: () => ({\n    CUSTOMER_KEEPS_TITLE: CUSTOMER_KEEPS.TITLE,\n    CUSTOMER_KEEPS_SUB_TITLE: CUSTOMER_KEEPS.SUB_TITLE,\n    customerKeepSubtitle: '',\n    imageSize: {\n      xs: '358px',\n      sm: '358px',\n      md: '358px',\n      lg: '520px',\n      xl: '358px',\n      desktopMultiple: '316px'\n    }\n  }),\n  computed: {\n    ...mapGetters({\n      order: 'order/order',\n      currentItemIndex: 'returns/currentItemIndex',\n      selectedItems: 'order/selectedItems',\n      isLoading: 'exchange/isLoading',\n      policies: 'returns/policies'\n    }),\n    items () {\n      return this.selectedItems\n    }\n  },\n  methods: {\n    determineImageSize () {\n      const currentSize = this.$vuetify.breakpoint.name\n      const largerSizes = ['md', 'lg', 'xl']\n\n      if (largerSizes.includes(currentSize) && this.selectedItems.length > 1) {\n        return '316px'\n      }\n\n      return this.imageSize[this.$vuetify.breakpoint.name]\n    },\n    handleNextClick () {\n      setSelectedReturnMethod({ type: 'customer_keep' })\n      this.$router.push('/orders/' + this.order.order_hash + '/review')\n    }\n\n  },\n  mounted () {\n    document.title = this.CUSTOMER_KEEPS_TITLE\n    const refundTimingEstimate = this.policies.refund_timing_estimate ? this.policies.refund_timing_estimate : '5-7'\n    this.customerKeepSubtitle = this.CUSTOMER_KEEPS_SUB_TITLE.replace(\n      '$$refundTimingEstimate',\n      refundTimingEstimate\n    )\n  }\n}\n</script>\n\n<style scoped>\n.keeps-card {\n  pointer-events: none;\n  flex: 0;\n  border: 0px !important;\n}\n.keeps-wrapper{\n  max-width: 64vw !important;\n}\n</style>\n"],"sourceRoot":"src/components/pages"}]}