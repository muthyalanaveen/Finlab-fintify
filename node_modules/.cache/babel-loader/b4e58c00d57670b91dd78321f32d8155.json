step === RETURN_REASON_STEP.PRIMARY\"\n            @click.stop=\"backButtonClicked\"\n            :item=\"selectedItems[currentItemIndex]\"\n            class=\"mt-n4\"\n            @feedbackClicked=\"showReturnFeedback = true\"\n            :show-thumbnail=\"this.step === RETURN_REASON_STEP.PRIMARY\"\n            :order-brand=\"order.order.concept\"\n          />\n        </div>\n        <v-list\n          class=\"mb-2\"\n          :class=\"{\n            'mt-5': $vuetify.breakpoint.smAndDown,\n            'mt-1': $vuetify.breakpoint.mdAndUp,\n          }\"\n          md=\"3\"\n          v-if=\"this.step === RETURN_REASON_STEP.PRIMARY\"\n        >\n          <div v-if=\"exchangesLoading\">\n            <list-item-skeleton :list=\"this.reasons\" />\n          </div>\n          <template v-for=\"(reasonToDisplay, index) in this.reasons\" v-else>\n            <list-item\n              :key=\"index\"\n              :index=\"index\"\n              :item=\"reasonToDisplay\"\n              :aria-label=\"reasonToDisplay.name\"\n              @onClick.stop=\"returnReasonClicked(index)\"\n              class=\"ma-0 pa-0 regular\"\n              :show-loading=\"isLoading && reason.id === reasonToDisplay.id\"\n            />\n          </template>\n        </v-list>\n        <div class=\"d-flex justify-center mb-8\">\n          <action-button\n            v-if=\"this.showFeedbackButton\"\n            text\n            color=\"primary\"\n            @click.stop=\"showReturnFeedback = true\"\n          >\n            {{ LEAVE_FEEDBACK }}\n          </action-button>\n        </div>\n        <refund-or-exchange\n          v-if=\"this.step === RETURN_REASON_STEP.ACTION\"\n          :return-reason-clicked=\"returnReasonClicked\"\n          :is-map-loading=\"isMapLoading\"\n        />\n        <return-reason-feedback\n          :visible=\"showReturnFeedback\"\n          @close=\"showReturnFeedback = false\"\n          @click.stop=\"feedbackSubmitted\"\n        />\n        <v-overlay :value=\"loading\">\n          <v-progress-circular\n            v-if=\"loading\"\n            :width=\"3\"\n            :active=\"loading\"\n            color=\"gray\"\n            :indeterminate=\"loading\"\n            class=\"justify-center align-center\"\n          />\n        </v-overlay>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport ListItem from '@/components/molecules/ListItem.vue'\nimport ListItemSkeleton from '@/components/molecules/ListItemSkeleton.vue'\nimport ReturnReasonHeader from '@/components/organism/ReturnReasonHeader.vue'\nimport ReturnReasonFeedback from './ReturnReasonFeedback.vue'\nimport { EXCHANGE_REFUND, RETURN_REASON } from 'branding/strings.json'\nimport OToast from '@/components/atoms/OToast.vue'\nimport { mapGetters } from 'vuex'\nimport RefundOrExchange from '@/components/organism/RefundOrExchange.vue'\nimport { checkForExpressDropoffOptions } from '@/controllers/MapController.js'\nimport { CONSTANTS } from '@/constants'\nimport { IS_TRUST_WORTHY_USER, UPDATE_SELECTED_ITEM_EXCHANGE_ELIGIBILITY, DESTINATION } from '@/store/constants.js'\nimport { canExchange } from '@/controllers/ExchangesController'\nimport { getIsGift, getGiftEmail } from '@/store/sessionStorage.js'\n\nexport default {\n  components: {\n    ListItem,\n    ListItemSkeleton,\n    ReturnReasonHeader,\n    ReturnReasonFeedback,\n    ActionButton,\n    OToast,\n    RefundOrExchange\n  },\n  data: () => ({\n    LETS_MAKE_IT_RIGHT_TITLE: EXCHANGE_REFUND.LETS_MAKE_IT_RIGHT_TITLE,\n    IT_DID_NOT_WORK_OUT_TITLE: EXCHANGE_REFUND.IT_DID_NOT_WORK_OUT_TITLE,\n    WHY_ARE_YOU_RETURNING: RETURN_REASON.WHY_ARE_YOU_RETURNING,\n    TELL_US_MORE: RETURN_REASON.TELL_US_MORE,\n    PLEASE_EXPLAIN_FURTHER: RETURN_REASON.PLEASE_EXPLAIN_FURTHER,\n    LEAVE_FEEDBACK: RETURN_REASON.LEAVE_FEEDBACK,\n    TOAST_MESSAGE: RETURN_REASON.TOAST_MESSAGE,\n    loading: false,\n    showReturnFeedback: false,\n    title: RETURN_REASON.WHY_ARE_YOU_RETURNING,\n    show: false,\n    timeout: 5000,\n    isMapLoading: false,\n    RETURN_REASON_STEP: CONSTANTS.RETURN_REASON_STEP,\n    isLoading: false,\n    state_Email: getGiftEmail()\n  }),\n  watch: {\n    loading (val) {\n      if (!val) return\n      setTimeout(() => this.refresh(), 1000)\n    },\n    $route (val) {\n      if (val.name === 'reasons') {\n        this.$store.commit(\n          'returns/updateStep',\n          this.RETURN_REASON_STEP.PRIMARY\n        )\n      } else {\n        this.$store.commit(\n          'returns/updateStep',\n          this.RETURN_REASON_STEP.ACTION\n        )\n      }\n    }\n  },\n  beforeUnmount () {},\n  computed: {\n    ...mapGetters({\n      order: 'order/order',\n      exchangeEligibility: 'exchange/getExchangeEligibility',\n      reasons: 'returns/reasons',\n      step: 'returns/step',\n      currentItemIndex: 'returns/currentItemIndex',\n      selectedItems: 'order/selectedItems',\n      reason: 'returns/reason',\n      policies: 'returns/policies',\n      exchangesLoading: 'exchange/isLoading',\n      policyReturnMethods: 'returns/policyReturnMethods',\n      returnMethods: 'returns/returnMethods',\n      refundList: 'order/refundList',\n      exchangeList: 'order/exchangeList'\n    }),\n    item () {\n      return this.selectedItems[this.currentItemIndex]\n    },\n    showFeedbackButton () {\n      return (\n        this.step === this.RETURN_REASON_STEP.PRIMARY &&\n        this.policies?.return_feedback_allowed\n      )\n    },\n    isGiftReturn () {\n      return getIsGift()\n    }\n\n  },\n  mounted () {\n    if (this.order.order_hash === '') {\n      this.$router.push('/orders/' + this.$route.params.id)\n    }\n    if (this.$route.name === 'reasons') {\n      this.$store.commit('returns/updateStep', this.RETURN_REASON_STEP.PRIMARY)\n    } else {\n      this.$store.commit('returns/updateStep', this.RETURN_REASON_STEP.ACTION)\n    }\n  },\n  methods: {\n    refresh () {\n      this.loading = false\n    },\n    async checkForMap () {\n      this.isMapLoading = true\n      const willHaveMap = await checkForExpressDropoffOptions(\n        this.returnMethods,\n        this.policyReturnMethods\n      )\n      this.isMapLoading = false\n      return willHaveMap\n    },\n    async checkForCustomerKeeps () {\n      if (!this.policies.destination_api_allowed) {\n        return false\n      }\n      const items = []\n      let isCustomerKeeps\n      const isGift = getIsGift()\n      this.refundList.forEach(item => {\n        const formattedItem = {\n          return_reason: item.reason,\n          sku: item.sku,\n          upc: item.upc,\n          concept: item.concept,\n          tags: item.tags,\n          calculated_unit_price_amount_cents: item.product_amount_cents / (item?.quantity || 1)\n        }\n        if (item?.quantity > 1) {\n          items.push(...Array(item.quantity).fill(formattedItem))\n        } else {\n          items.push(formattedItem)\n        }\n      }\n      )\n      const params = {\n        items,\n        email: isGift ? getGiftEmail() : this.order.shopper_email,\n        is_gift: isGift\n      }\n\n      this.isLoading = true\n      await this.$store\n        .dispatch('order/' + DESTINATION, params)\n        .then((response) => {\n          this.$store.commit('ui/updateDestinationResponse', response)\n\n          isCustomerKeeps = response.customer_keep\n          this.isLoading = false\n        })\n      return isCustomerKeeps\n    },\n    async returnReasonClicked (index = null) {\n      switch (this.step) {\n        case this.RETURN_REASON_STEP.PRIMARY:\n          this.returnReasonStepPrimary(\n            this.reasons[index],\n            this.selectedItems[this.currentItemIndex]\n          )\n          break\n        case this.RETURN_REASON_STEP.ACTION:\n          this.returnReasonStepAction()\n          break\n        default:\n          break\n      }\n    },\n    async returnReasonStepAction () {\n      if (this.currentItemIndex === this.selectedItems.length - 1) {\n        const hasMap = await this.checkForMap()\n        const isCustomerKeeps = await this.checkForCustomerKeeps()\n        if (isCustomerKeeps) {\n          this.$router.push('/orders/' + this.order.order_hash + '/customer-keeps')\n        } else if (hasMap) {\n          this.$router.push(\n            '/orders/' + this.order.order_hash + '/mapshipping'\n          )\n        } else {\n          this.$router.push('/orders/' + this.order.order_hash + '/shipping')\n        }\n      } else {\n        this.title = this.WHY_ARE_YOU_RETURNING\n        this.$store.commit(\n          'returns/updateStep',\n          this.RETURN_REASON_STEP.PRIMARY\n        )\n        this.$store.commit('returns/incrementIndex', { root: true })\n        this.show = true\n      }\n    },\n    returnReasonStepPrimary (reason, selectedItem) {\n      this.$store.commit('returns/updateReason', {\n        reason: reason,\n        sku: selectedItem.sku\n      })\n\n      if (canExchange(reason.recommendation_type)) {\n        const momentaryRma = this.returnTrustWorthyPayload(reason, selectedItem)\n        let params = {\n          order_id: this.order.order_id,\n          momentary_rma: momentaryRma,\n          current_item: {\n            sku: selectedItem.sku,\n            total_refund_amount_cents: selectedItem.calculated_product_amount_cents,\n            return_cause: reason.name,\n            units: selectedItem.quantity,\n            event_occurred_at: new Date()\n          }\n        }\n\n        if (this.isGiftReturn) {\n          params = { ...params, giftee_email: this.state_Email }\n        }\n\n        this.isLoading = true\n        this.$store\n          .dispatch('order/' + IS_TRUST_WORTHY_USER, params)\n          .then((response) => {\n            if (response) {\n              this.$store.commit('exchange/setExchangeEligibility', response)\n              this.$store.commit(\n                'order/' + UPDATE_SELECTED_ITEM_EXCHANGE_ELIGIBILITY, {\n                  identifier: selectedItem.identifier,\n                  exchangeMetadata: response\n                })\n            }\n          })\n          .catch((err) => {\n            console.error({ err })\n          })\n          .finally(() => {\n            this.isTrustWorthyUserResponseCallback()\n          })\n      } else {\n        this.isTrustWorthyUserResponseCallback()\n      }\n    },\n    isTrustWorthyUserResponseCallback () {\n      this.$router.push('/orders/' + this.order.order_hash + '/refund-amount')\n      this.$store.commit('returns/updateStep', this.RETURN_REASON_STEP.ACTION)\n      this.isLoading = false\n    },\n    returnTrustWorthyPayload (reason, selectedItem) {\n      return this.selectedItems.map(selectedLocalItem => {\n        let returnCause = null\n        let refundedItem = null\n        const currentItem = selectedItem.variant_identifier === selectedLocalItem.variant_identifier\n\n        if (currentItem && reason) {\n          returnCause = reason.name\n        } else if (this.refundList.length) {\n          refundedItem = this.refundList.find(item => {\n            return item.variant_identifier === selectedLocalItem.variant_identifier\n          })\n        } else if (!refundedItem && this.exchangeList.length) {\n          refundedItem = this.exchangeList.find(item => {\n            return item.original_item.variant_identifier === selectedLocalItem.variant_identifier\n          })\n        } if (refundedItem?.reason) {\n          returnCause = refundedItem.reason.name\n        }\n\n        return {\n          sku: selectedLocalItem.sku,\n          total_refund_amount_cents: selectedLocalItem.calculated_product_amount_cents,\n          return_cause: returnCause,\n          units: selectedLocalItem.quantity,\n          event_occurred_at: new Date()\n        }\n      })\n    },\n    async feedbackSubmitted (event) {\n      this.showReturnFeedback = false\n      this.$store.commit('returns/updateStep', this.RETURN_REASON_STEP.ACTION)\n      this.title = ''\n    },\n    async backButtonClicked () {\n      switch (this.step) {\n        case this.RETURN_REASON_STEP.PRIMARY:\n          if (this.currentItemIndex === 0) {\n            this.$router.push('/orders/' + this.order.order_hash)\n          } else {\n            this.$store.commit('returns/decrementIndex')\n            this.title = ''\n            this.$store.commit(\n              'returns/updateStep',\n              this.RETURN_REASON_STEP.ACTION\n            )\n          }\n          break\n        case this.RETURN_REASON_STEP.ACTION:\n          this.title = this.WHY_ARE_YOU_RETURNING\n          this.$store.commit(\n            'returns/updateStep',\n            this.RETURN_REASON_STEP.PRIMARY\n          )\n          this.$router.push('/orders/' + this.order.order_hash + '/reasons')\n          break\n        default:\n          break\n      }\n    }\n  }\n}\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components/pages"}]}