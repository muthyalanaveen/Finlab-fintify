oC;AAClC,SAAO,CAACA,SAAS,IAAIA,SAAS,CAACO,2BAAvB,GACJP,SAAS,CAACO,2BAAV,GAAwC,GADpC,GAEJ,CAFG,EAGLC,OAHK,CAGG,CAHH,CAAP;AAID;;AAED,SAASjE,uBAAT,CAAkCqB,IAAlC,EAAwC;AACtC,MAAMxB,KAAK,GAAGC,cAAc,CAACuB,IAAD,CAA5B;AACA,oBAAW6C,MAAM,CAACrE,KAAD,CAAN,CAAcoE,OAAd,CAAsB,CAAtB,CAAX;AACD;;AAED,SAAS9B,gBAAT,CAA2Bd,IAA3B,EAAiC;AAC/B,SAAO,CAACA,IAAI,IAAIA,IAAI,CAAC8C,uBAAb,GACJ9C,IAAI,CAAC8C,uBAAL,GAA+B,GAD3B,GAEJ,CAFG,EAGLF,OAHK,CAGG,CAHH,CAAP;AAID;;AAED,SAAS5B,yBAAT,CAAoChB,IAApC,EAA0C;AACxC,MAAMxB,KAAK,GAAGsC,gBAAgB,CAACd,IAAD,CAA9B;AACA,oBAAW6C,MAAM,CAACrE,KAAD,CAAN,CAAcoE,OAAd,CAAsB,CAAtB,CAAX;AACD;;AAED,SAAS/D,SAAT,CAAoBuD,SAApB,EAA+B3E,UAA/B,EAA2C;AACzC,SAAO2E,SAAS,CAACW,YAAV,GACH,IAAIrF,IAAJ,CAAS0E,SAAS,CAACW,YAAnB,IAAmCtF,UADhC,GAEH,KAFJ;AAGD","sourcesContent":["import {\n  FETCH_START,\n  FETCH_ORDER_END,\n  SET_ERROR,\n  UPDATE_SELECTED_ITEM,\n  UPDATE_SELECTED_ITEM_QUANTITY,\n  SELECT_ALL_ITEMS,\n  UPDATE_REFUND_LIST,\n  UPDATE_REFUND_LIST_FOR_RETURN_USER,\n  UPDATE_EXCHANGE_LIST_FOR_RETURN_USER,\n  EMPTY_REFUND_LIST,\n  EMPTY_EXCHANGE_LIST,\n  UPDATE_SELECTED_ITEM_WITH_EXCHANGE_VARIANTS,\n  UPDATE_EXCHANGE_LIST,\n  REMOVE_ITEM_FROM_EXCHANGE_LIST\n} from '@/store/constants'\nimport { CONSTANTS } from '@/constants/index.js'\n\nexport default {\n  [FETCH_START] (state) {\n    state.isLoading = true\n  },\n  [FETCH_ORDER_END] (state, data) {\n    var expiryDate = new Date()\n    expiryDate.setDate(expiryDate.getDate() - data.policies.return_window_days)\n\n    state.order = {\n      ...data,\n      order: {\n        ...data.order,\n        items: data.order.items\n          ? data.order.items.map((o) => {\n            return {\n              ...o,\n              selected: false,\n              isSingleQuantity: o.quantity === 1,\n              subtitle: getSubtitle(o),\n              price: getRefundPrice(o),\n              formattedRefundPrice: getFormattedRefundPrice(o),\n              expired: isExpired(o, expiryDate),\n              original_quantity: o.quantity,\n              quantity: o.quantity > 1 ? 1 : o.quantity,\n              exchangeVariants: [],\n              color: getValueFromAttribute(o, 'color'),\n              size: getValueFromAttribute(o, 'size'),\n              quantity_in_stock: o?.variants?.length > 0 ? o.variants[0]?.quantity_in_stock : 0\n            }\n          })\n          : []\n      }\n    }\n\n    state.rmaList = {\n      rmaListItems: data.rma_list\n        ? data.rma_list.map((returnItems) => {\n          return {\n            ...returnItems,\n            return_status: getRMAStatus(returnItems),\n            rma_items: returnItems.rma_items.map((rmaItem) => {\n              return {\n                ...rmaItem,\n                order_item: {\n                  ...rmaItem.order_item,\n                  selected: false,\n                  isSingleQuantity: rmaItem.order_item.quantity === 1,\n                  subtitle: getSubtitle(rmaItem.order_item),\n                  price: getRefundPrice(rmaItem.order_item),\n                  formattedRefundPrice: getFormattedRefundPrice(\n                    rmaItem.order_item\n                  ),\n                  expired: isExpired(rmaItem.order_item, expiryDate),\n                  original_quantity: rmaItem.order_item.quantity,\n                  quantity:\n                      rmaItem.order_item.quantity > 1\n                        ? 1\n                        : rmaItem.order_item.quantity,\n                  exchangeVariants: [],\n                  color: getValueFromAttribute(rmaItem.order_item, 'color'),\n                  size: getValueFromAttribute(rmaItem.order_item, 'size'),\n                  quantity_in_stock: rmaItem?.variants?.length > 0 ? rmaItem.variants[0]?.quantity_in_stock : 0\n                }\n              }\n            })\n          }\n        })\n        : []\n    }\n\n    state.isLoading = false\n  },\n  [SET_ERROR] (state) {\n    state.isLoading = false\n  },\n  [UPDATE_SELECTED_ITEM] (state, identifier) {\n    const item = state.order.order.items.find(\n      (x) => x.identifier === identifier\n    )\n    item.selected = !item.selected\n  },\n  [UPDATE_SELECTED_ITEM_QUANTITY] (state, { identifier, quantity }) {\n    const item = state.order.order.items.find(\n      (x) => x.identifier === identifier\n    )\n    item.quantity = quantity\n  },\n  [SELECT_ALL_ITEMS] (state) {\n    state.order.order.items = state.order.order.items.map((l) => {\n      return { ...l, selected: true }\n    })\n  },\n  [UPDATE_REFUND_LIST_FOR_RETURN_USER] (state, list) {\n    state.refundList = list\n  },\n  [UPDATE_EXCHANGE_LIST_FOR_RETURN_USER] (state, list) {\n    state.exchangeList = list\n  },\n  [UPDATE_REFUND_LIST] (state, refundItem) {\n    const existingIds = state.refundList.map((x) => x.identifier)\n    if (\n      state.refundList.indexOf(refundItem) === -1 &&\n      !existingIds.includes(refundItem.identifier)\n    ) {\n      state.refundList.push(refundItem)\n\n      // remove item from exchange list\n      const filteredExchangeList = state.exchangeList.filter(\n        (item) => item.identifier !== refundItem.identifier\n      )\n      state.exchangeList = filteredExchangeList\n    } else {\n      state.refundList = state.refundList.map(item => {\n        if (item.identifier !== refundItem.identifier) {\n          return item\n        } else {\n          return refundItem\n        }\n      })\n    }\n  },\n\n  [EMPTY_REFUND_LIST] (state) {\n    state.refundList = []\n  },\n\n  [UPDATE_SELECTED_ITEM_WITH_EXCHANGE_VARIANTS] (state, { data, identifier }) {\n    const item = state.order.order.items.find((x) => x.identifier === identifier)\n    item.exchangeVariants = data.map((item) => {\n      return {\n        ...item,\n        price: getExchangePrice(item),\n        formattedExchangePrice: getFormattedExchangePrice(item),\n        sku: item.SKU,\n        identifier: item.SKU\n      }\n    })\n  },\n\n  [REMOVE_ITEM_FROM_EXCHANGE_LIST] (state, identifier) {\n    const filteredReturnList = state.exchangeList.filter(\n      (item) => item.identifier !== identifier\n    )\n    state.exchangeList = filteredReturnList\n  },\n\n  [UPDATE_EXCHANGE_LIST] (state, exchangeItem) {\n    // if exchangeItem doesn't exist in the list, add it and check if it's already in the refund list, if so remove it from the refund list\n    // else update the item in the exhcange list\n\n    const existingIds = state.exchangeList.map((x) => x.identifier)\n    if (\n      state.exchangeList.indexOf(exchangeItem) === -1 &&\n      !existingIds.includes(exchangeItem.identifier)\n    ) {\n      state.exchangeList.push(exchangeItem)\n\n      // remove from refund list if exists\n      const filteredRefundList = state.refundList.filter(\n        (item) => item.identifier !== exchangeItem.identifier\n      )\n      state.refundList = filteredRefundList\n    } else {\n      state.exchangeList = state.exchangeList.map(item => {\n        if (item.identifier !== exchangeItem.identifier) {\n          return item\n        } else {\n          return exchangeItem\n        }\n      })\n    }\n  },\n  [EMPTY_EXCHANGE_LIST] (state) {\n    state.exchangeList = []\n  }\n}\n\n// TODO: move statuses to constants\nfunction getRMAStatus (returnItem) {\n  let rmaStatusRes = 'STARTED'\n  const rmaStatus = returnItem.status.toUpperCase()\n  const goodsStatus = returnItem.goods_status.toUpperCase()\n  var returnMethodType = ''\n  if (returnItem && returnItem.return_method) {\n    returnMethodType = returnItem.return_method.type\n  }\n\n  if (returnMethodType && returnMethodType === CONSTANTS.RETURN_METHODS.MAIL_BACK) {\n    if (rmaStatus === 'CREATED' && goodsStatus === 'UNKNOWN') {\n      rmaStatusRes = 'STARTED'\n    } else if (\n      rmaStatus === 'CREATED' &&\n      (goodsStatus === 'PRE_TRANSIT' || goodsStatus === 'TRANSIT')\n    ) {\n      rmaStatusRes = 'IN_TRANSIT'\n    } else if (\n      (rmaStatus === 'REFUNDED' || rmaStatus === 'REFUND_ERROR') &&\n      goodsStatus === 'TRANSIT'\n    ) {\n      rmaStatusRes = 'RECEIVED' // Transit status will be skipped and goes directly to RECEIVED status\n    } else if (\n      rmaStatus === 'READY_TO_REFUND' &&\n      (goodsStatus === 'PRE_TRANSIT' || goodsStatus === 'DELIVERED')\n    ) {\n      rmaStatusRes = 'RECEIVED'\n    } else if (\n      (rmaStatus === 'REFUNDED' ||\n        rmaStatus === 'REFUND_ERROR' ||\n        rmaStatus === 'PARTIAL_REFUNDED') &&\n      goodsStatus === 'DELIVERED'\n    ) {\n      rmaStatusRes = 'COMPLETED'\n    }\n  } else if (returnMethodType && returnMethodType === CONSTANTS.RETURN_METHODS.EXPRESS_RETURNS) {\n    if (rmaStatus === 'CREATED' && goodsStatus === 'CREATED') {\n      rmaStatusRes = 'STARTED'\n    } else if (rmaStatus === 'CREATED' && goodsStatus === 'ACCEPTED') {\n      rmaStatusRes = 'IN_TRANSIT'\n    } else if (rmaStatus === 'READY_TO_REFUND' && goodsStatus === 'ACCEPTED') {\n      rmaStatusRes = 'RECEIVED'\n    } else if (rmaStatus === 'REFUNDED' && goodsStatus === 'ACCEPTED') {\n      rmaStatusRes = 'COMPLETED'\n    }\n  } else if (returnMethodType && returnMethodType === CONSTANTS.RETURN_METHODS.IN_STORE) {\n    if (rmaStatus === 'CREATED' && goodsStatus === 'CREATED') {\n      rmaStatusRes = 'STARTED'\n    } else if (rmaStatus === 'READY_TO_REFUND' && goodsStatus === 'ACCEPTED') {\n      rmaStatusRes = 'RECEIVED'\n    } else if (\n      (rmaStatus === 'REFUNDED' ||\n        rmaStatus === 'REFUND_ERROR' ||\n        rmaStatus === 'PARTIAL_REFUNDED') &&\n      goodsStatus === 'RECEIVED'\n    ) {\n      rmaStatusRes = 'COMPLETED'\n    }\n  }\n\n  if (rmaStatus === 'CANCELED') {\n    rmaStatusRes = 'CANCELED'\n  }\n\n  return rmaStatusRes\n}\n\nfunction getSubtitle (orderItem) {\n  return orderItem?.variants?.length > 0\n    ? orderItem.variants[0].attributes\n      .filter((x) => x.name !== 'Disposition')\n      .map((x) => x.value)\n      .join(' - ')\n    : ''\n}\n\nfunction getValueFromAttribute (orderItem, attr) {\n  return orderItem.variants?.length > 0\n    ? orderItem.variants[0].attributes.find(\n      (x) => x.name.toLowerCase() === attr.toLowerCase()\n    ).value\n    : ''\n}\n\nfunction getRefundPrice (orderItem) {\n  return (orderItem && orderItem.calculated_unit_price_cents\n    ? orderItem.calculated_unit_price_cents / 100\n    : 0\n  ).toFixed(2)\n}\n\nfunction getFormattedRefundPrice (item) {\n  const price = getRefundPrice(item)\n  return `$${Number(price).toFixed(2)}`\n}\n\nfunction getExchangePrice (item) {\n  return (item && item.unit_price_amount_cents\n    ? item.unit_price_amount_cents / 100\n    : 0\n  ).toFixed(2)\n}\n\nfunction getFormattedExchangePrice (item) {\n  const price = getExchangePrice(item)\n  return `$${Number(price).toFixed(2)}`\n}\n\nfunction isExpired (orderItem, expiryDate) {\n  return orderItem.shipped_date\n    ? new Date(orderItem.shipped_date) < expiryDate\n    : false\n}\n"]}]}