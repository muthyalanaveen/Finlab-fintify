,KAAA,aAFA,CAAA;AAIA,YAAA,KAAA,GAAA,gCAAA,CAAA,KAAA,aAAA,CAAA;AACA,aAAA,0BAAA,GAAA,KAAA;;AACA,YAAA,CAAA,KAAA,YAAA,IAAA,KAAA,CAAA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA,CAAA,UAAA,EAAA;AACA,eAAA,YAAA,GAAA,KAAA,CAAA,CAAA,CAAA,CAAA,IAAA;AACA,eAAA,gBAAA,GAAA,KAAA,CAAA,CAAA,CAAA;AACA,SAHA,MAGA;AACA,eAAA,YAAA,GAAA,YAAA,CAAA,IAAA;AACA,eAAA,gBAAA,GAAA,YAAA;AACA;AACA;;AAEA,WAAA,gBAAA;AACA;AA3EA,GAjDA;AA8HA,EAAA,OA9HA,qBA8HA;AAAA;;AACA,SAAA,YAAA,GAAA,KAAA,IAAA;AACA,QAAA,YAAA,GAAA,qBAAA,EAAA;AACA,SAAA,2BAAA,GAAA,YAAA;AAEA,SAAA,0BAAA,GAAA,gCAAA,gBACA,KAAA,IADA,gDACA,YAAA,KADA,CAAA;AAIA,QAAA,YAAA,GAAA,eAAA,CACA,KAAA,WAAA,CAAA,IADA,EAEA,KAAA,2BAAA,CAAA,CAAA,EAAA,KAFA,CAAA;AAIA,SAAA,gBAAA,GAAA,YAAA;AACA,SAAA,iBAAA,GAAA,YAAA,CAAA,IAAA,CACA,UAAA,OAAA;AAAA,aAAA,OAAA,CAAA,KAAA,MAAA,YAAA,aAAA,YAAA,uBAAA,YAAA,CAAA,KAAA,CAAA;AAAA,KADA,CAAA;AAGA,SAAA,YAAA,4BAAA,KAAA,gBAAA,0DAAA,sBAAA,IAAA;AACA,SAAA,aAAA,4BAAA,KAAA,iBAAA,0DAAA,sBAAA,KAAA;AACA,SAAA,oBAAA,CAAA,KAAA,YAAA;AACA,SAAA,gBAAA;AACA;AAnJA,CAAA","sourcesContent":["<template>\n  <v-sheet tile>\n    <v-sheet\n      class=\"mt-6\"\n      tile\n      v-if=\"!this.isLoading\"\n    >\n      <header-title class=\"mt-7 mb-2\">\n        {{ title1_text }}\n      </header-title>\n      <header-small-title class=\"my-4\">\n        {{ title2_text }}\n      </header-small-title>\n\n      <product-slide-view\n        class=\"mt-2 p-1\"\n        :class=\"{\n          'ml-0': $vuetify.breakpoint.smAndDown,\n          'ml-n4': $vuetify.breakpoint.mdAndUp,\n        }\"\n        :cards=\"exchangeVariantColorOptions\"\n        :on-card-selected=\"onColorSelected\"\n        :default-value=\"this.defaultColorValue\"\n      />\n      <header-small-title class=\"my-6\">\n        {{ title3_text }}\n      </header-small-title>\n      <size-dropdown\n        :default-value=\"this.defaultSizeValue\"\n        :items=\"exchangeVariantSizeOptions\"\n        :on-change=\"onSizeSelected\"\n      />\n      <v-row\n        class=\"mt-4\"\n      >\n        <v-col>\n          <action-button\n            @click.stop=\"onClickMethod\"\n            primary-color\n            :loading=\"isLoading || isMapLoading\"\n            :block=\"$vuetify.breakpoint.smAndDown\"\n            large\n            :disabled=\"isDisabled\"\n          >\n            {{ isDisabled ? action1_Text : action1_Selected_Text }}\n          </action-button>\n        </v-col>\n      </v-row>\n    </v-sheet>\n    <div v-else>\n      <v-skeleton-loader type=\"card\" />\n    </div>\n  </v-sheet>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\nimport {\n  getColorOptionsForPDP,\n  saveSelectedItemToStore,\n  getSizeOptionsForSelectedVariant,\n  handleColorOrSizeOptionSelected,\n  getDefaultValue,\n  isSameVariant\n} from '@/controllers/ExchangesController'\nimport { CONSTANTS } from '@/constants'\n\nexport default {\n  name: 'ExchangeOtherView',\n  components: {\n    ProductSlideView: () =>\n      import('@/components/organism/ProductSlideView.vue'),\n    ActionButton: () => import('@/components/atoms/ActionButton.vue'),\n    SizeDropdown: () => import('@/components/molecules/SizeDropdown.vue'),\n    HeaderTitle: () => import('@/components/molecules/HeaderTitle.vue'),\n    HeaderSmallTitle: () =>\n      import('@/components/molecules/HeaderSmallTitle.vue')\n  },\n  data: () => ({\n    title1_text: \"Let's try another size or color\",\n    title2_text: 'Select a color',\n    title3_text: 'Select a size',\n    action1_Text: 'Select an item to exchange',\n    action1_Selected_Text: 'Exchange for this item',\n    action2_Text: 'See all sizes and colors...',\n    selectedItem: {},\n    exchangeVariantColorOptions: [],\n    exchangeVariantSizeOptions: [],\n    RETURN_REASON_ACTION: CONSTANTS.RETURN_REASON_ACTION,\n    ACTION_TYPE: CONSTANTS.ACTION_TYPE,\n    isDisabled: true,\n    defaultSizeValue: null,\n    defaultColorValue: null\n  }),\n  props: {\n    returnReasonClicked: {\n      type: Function,\n      required: true\n    },\n    isMapLoading: {\n      type: Boolean,\n      required: true\n    }\n  },\n  computed: {\n    ...mapGetters({\n      isLoading: 'exchange/isLoading',\n      currentItemIndex: 'returns/currentItemIndex',\n      selectedItems: 'order/selectedItems',\n      exchangeList: 'order/exchangeList',\n      reason: 'returns/reason'\n    }),\n    item () {\n      return this.selectedItems[this.currentItemIndex]\n    }\n  },\n  methods: {\n    onColorSelected (selectedItem) {\n      if (!selectedItem.disabled) {\n        this.selectedColor = selectedItem.color\n        this.handleOptionSelected(selectedItem)\n      }\n    },\n    onSizeSelected (selectedItem) {\n      if (!selectedItem.disabled) {\n        this.selectedSize = selectedItem.size\n        this.handleOptionSelected(selectedItem, this.ACTION_TYPE.SIZE)\n      }\n    },\n    onClickMethod (event) {\n      let optionForSelectedSizeAndColor = this.item?.exchangeVariants.find(\n        (variant) =>\n          variant.size === this.selectedSize &&\n          variant.color === this.selectedColor\n      )\n\n      if (optionForSelectedSizeAndColor) {\n        optionForSelectedSizeAndColor = {\n          ...optionForSelectedSizeAndColor,\n          disabled: false,\n          subtitle: ''\n        }\n        saveSelectedItemToStore(optionForSelectedSizeAndColor)\n        this.returnReasonClicked()\n        this.$emit('click', event)\n      }\n    },\n    updateIsDisabled () {\n      this.isDisabled = !(\n        this.selectedSize &&\n        this.selectedColor &&\n        this.selectedItem &&\n        !isSameVariant(this.selectedItem) &&\n        !this.selectedItem.disabled &&\n        !this.isLoading\n      )\n    },\n    handleOptionSelected (selectedItem, action = this.ACTION_TYPE.COLOR) {\n      const {\n        selectedSize,\n        selectedColor,\n        selectedItem: updatedSelectedItem\n      } = handleColorOrSizeOptionSelected(\n        selectedItem,\n        action,\n        this.item,\n        this.selectedSize,\n        this.selectedColor\n      )\n\n      this.selectedSize = selectedSize\n      this.selectedColor = selectedColor\n      this.selectedItem = updatedSelectedItem\n\n      if (action === this.ACTION_TYPE.COLOR) {\n        const defaultValue = getDefaultValue(\n          this.ACTION_TYPE.SIZE,\n          this.selectedColor\n        )\n        const sizes = getSizeOptionsForSelectedVariant(this.selectedColor)\n        this.exchangeVariantSizeOptions = sizes\n        if (!this.selectedSize && sizes.length === 1 && !sizes[0].isDisabled) {\n          this.selectedSize = sizes[0].size\n          this.defaultSizeValue = sizes[0]\n        } else {\n          this.selectedSize = defaultValue.size\n          this.defaultSizeValue = defaultValue\n        }\n      }\n\n      this.updateIsDisabled()\n    }\n  },\n  mounted () {\n    this.selectedItem = this.item\n    const colorOptions = getColorOptionsForPDP()\n    this.exchangeVariantColorOptions = colorOptions\n\n    this.exchangeVariantSizeOptions = getSizeOptionsForSelectedVariant(\n      this.item?.color\n    )\n\n    const defaultValue = getDefaultValue(\n      this.ACTION_TYPE.SIZE,\n      this.exchangeVariantColorOptions[0].color\n    )\n    this.defaultSizeValue = defaultValue\n    this.defaultColorValue = colorOptions.find(\n      (variant) => variant.color === defaultValue?.color\n    )\n    this.selectedSize = this.defaultSizeValue?.size\n    this.selectedColor = this.defaultColorValue?.color\n    this.handleOptionSelected(this.selectedItem)\n    this.updateIsDisabled()\n  }\n}\n</script>\n"],"sourceRoot":"src/components/pages"}]}