nimport ReturnMethods from '@/components/pages/ReturnMethods.vue'\nimport OrdersView from '@/components/pages/orders.vue'\nimport Typography from '@/components/pages/Typography.vue'\nimport PDPPage from '@/components/pages/PDPPage.vue'\nimport ReviewView from '@/components/pages/ReviewView.vue'\nimport ConfirmationView from '@/components/pages/ConfirmationView.vue'\nimport PageNotFound from '@/components/pages/PageNotFound.vue'\nimport TrackerView from '@/components/pages/TrackerView.vue'\nimport Login from '@/components/pages/Login.vue'\nimport PrivacyPolicy from '@/components/pages/PrivacyPolicy.vue'\nimport TermsOfService from '@/components/pages/TermsOfService.vue'\nimport ReturnMethodsMap from '@/components/pages/ReturnMethodsMap.vue'\nimport { SEARCH } from 'branding/strings.json'\n\nimport { appLoginRequired } from '@/common/config'\n\nVue.use(VueRouter)\n\nconst routes = [\n  {\n    path: '/',\n    name: 'Home',\n    component: Home\n  },\n  {\n    path: '/zipcode',\n    name: 'zipcode',\n    component: ZipCodeView\n  },\n  {\n    path: '/orderandzipcode',\n    name: 'orderandzipcode',\n    component: OrderandZipCodeView\n  },\n  {\n    path: '/orders/:id',\n    component: OrdersView,\n    children: [\n      { path: '', component: ReturnListView },\n      {\n        path: 'reasons',\n        name: 'reasons',\n        component: ReturnReasonWrapper\n      },\n      {\n        path: 'refund-amount',\n        name: 'refund-amount',\n        component: ReturnReasonWrapper\n      },\n      {\n        path: 'shipping',\n        component: ReturnMethods\n      },\n      {\n        path: 'mapshipping',\n        name: 'mapshipping',\n        component: ReturnMethodsMap\n      },\n      {\n        path: 'review',\n        component: ReviewView,\n        props: true\n      },\n      {\n        path: 'confirm/:identifier',\n        component: ConfirmationView,\n        props: {\n          header: true,\n          content: true\n        }\n      },\n      {\n        path: 'tracker',\n        component: TrackerView\n      },\n      {\n        path: 'tracker/:identifier',\n        component: TrackerView\n      }\n    ]\n  },\n  {\n    path: '/howitworks',\n    name: 'howitworks',\n    component: HowItWorksView\n  },\n  {\n    path: '/returngift',\n    name: 'returngift',\n    component: ReturnGiftView\n  },\n  {\n    path: '/pdppage',\n    name: 'pdppage',\n    component: PDPPage\n  },\n  {\n    path: '/Typography',\n    name: 'Typography',\n    component: Typography\n  },\n  {\n    path: '/login',\n    name: 'login',\n    component: Login\n  },\n  {\n    path: '/privacy-policy',\n    name: 'privacy-policy',\n    component: PrivacyPolicy\n  },\n  {\n    path: '/terms-of-service',\n    name: 'terms-of-service',\n    component: TermsOfService\n  },\n  {\n    path: '*',\n    component: PageNotFound\n  }\n]\n\nconst router = new VueRouter({\n  mode: 'history',\n  routes,\n  scrollBehavior: (to, from, savedPosition) => {\n    if (to.hash) return { selector: to.hash }\n    if (savedPosition) return savedPosition\n    return { x: 0, y: 0 }\n  }\n})\n\nrouter.beforeEach((to, from, next) => {\n  const isAuthenticated = localStorage.getItem('isAuthenticated')\n  const meta = document.createElement('meta')\n  meta.name = 'description'\n  meta.content = SEARCH.DESCRIPTION\n  document.getElementsByTagName('head')[0].appendChild(meta)\n  if (\n    appLoginRequired === 'true' &&\n    isAuthenticated !== 'true' &&\n    to.name !== 'login'\n  ) {\n    next({ name: 'login' })\n  } else next()\n})\n\nexport default router\n"]}]}