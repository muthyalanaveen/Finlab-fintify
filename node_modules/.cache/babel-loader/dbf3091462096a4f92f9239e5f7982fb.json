AA;AAAA;AAAA;AAAA;AAAA;AAyBA,KAzCA;AA0CA,IAAA,UA1CA,sBA0CA,IA1CA,EA0CA;AACA,aAAA,IAAA,CAAA,WAAA,IAAA,IAAA,CAAA,aAAA;AACA,KA5CA;AA6CA,IAAA,eA7CA,2BA6CA,YA7CA,EA6CA;AACA,UAAA,KAAA,GAAA,EAAA;AACA,UAAA,IAAA,GAAA,EAAA;;AACA,UACA,YAAA,KAAA,KAAA,aAAA,CAAA,OAAA,IACA,YAAA,KAAA,KAAA,aAAA,CAAA,UADA,IAEA,YAAA,KAAA,KAAA,aAAA,CAAA,QAHA,EAIA;AACA,QAAA,KAAA,GAAA,KAAA,MAAA;AACA,QAAA,IAAA,GAAA,eAAA;AACA,OAPA,MAOA,IAAA,YAAA,KAAA,KAAA,aAAA,CAAA,SAAA,EAAA;AACA,QAAA,KAAA,GAAA,KAAA,SAAA;AACA,QAAA,IAAA,GAAA,MAAA;AACA,OAHA,MAGA,IAAA,YAAA,KAAA,KAAA,aAAA,CAAA,QAAA,EAAA;AACA,QAAA,KAAA,GAAA,KAAA,QAAA;AACA,QAAA,IAAA,GAAA,OAAA;AACA;;AACA,aAAA;AAAA,QAAA,KAAA,EAAA,KAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA;AA/DA,IA/BA;AAgGA,EAAA,OAhGA,qBAgGA;AACA,SAAA,eAAA;AACA;AAlGA,CAAA","sourcesContent":["<template>\n  <div>\n    <template v-for=\"(returnItem, returnItemIndex) in this.rmaList.rmaListItems\">\n      <v-row :key=\"returnItemIndex\">\n        <div class=\"bg-well my-2\">\n          <div class=\"mx-5\">\n            <div class=\"d-flex justify-space-between align-center my-5\">\n              <div>\n                <o-text sm bold>\n                  {{ getReturnDate(returnItem.created_at) }}\n                </o-text>\n              </div>\n              <div class=\"d-flex align-center\">\n                <v-icon small bold>\n                  {{ getStepperlabel(returnItem.return_status).icon }}\n                </v-icon>\n                <o-text\n                  sm\n                  bold\n                  class=\"ml-2\"\n                >\n                  {{ getStepperlabel(returnItem.return_status).title }}\n                </o-text>\n              </div>\n            </div>\n\n            <v-divider />\n\n            <div v-if=\"isExchangeOrder(returnItem.rma_items)\">\n              <o-text sm bold>\n                {{ EXCHANGE }}\n              </o-text>\n\n              <template v-for=\"(item, index) in returnItem.rma_items\">\n                <v-row :key=\"index\">\n                  <div v-if=\"isExchange(item)\" class=\"d-flex mb-4\">\n                    <v-row>\n                      <v-row>\n                        <v-col cols=\"10\">\n                          <product-thumbnail\n                            :key=\"index\"\n                            :item=\"item.exchange_item\"\n                            :index=\"index\"\n                            :title=\"ORIGINAL_PURCHASE\"\n                            :sub-title1=\"item.exchange_item.originalItem.title\"\n                            :sub-title2=\"\n                              item.exchange_item.originalItem.color +\n                                ' - ' +\n                                item.exchange_item.originalItem.size\n                            \"\n                            :image-url=\"\n                              item.exchange_item.originalItem.image_url\n                            \"\n                            is-small-thumbnail\n                            from-return-item\n                          />\n                          <div class=\"ml-6\">\n                            <v-img\n                              :src=\"require('@/assets/images/down_arrow.png')\"\n                              max-width=\"22\"\n                              max-height=\"22\"\n                              style=\"z-index: 1\"\n                              contain\n                            />\n                          </div>\n                        </v-col>\n                        <!-- <v-col cols=\"2\" align-self=\"center\">\n                        <div class=\"d-flex justify-center align-center\">\n                          <o-text sm>\n                            {{ item.exchange_item.originalItem.formattedRefundPrice }}\n                          </o-text>\n                        </div>\n                      </v-col> -->\n                      </v-row>\n                      <v-row class=\"mb-1 mt-n3\">\n                        <v-col cols=\"10\">\n                          <product-thumbnail\n                            :key=\"index\"\n                            :item=\"item.exchange_item\"\n                            :index=\"index\"\n                            :title=\"INCOMING_ITEM\"\n                            :sub-title1=\"item.exchange_item.title\"\n                            :sub-title2=\"\n                              item.exchange_item.color +\n                                ' - ' +\n                                item.exchange_item.size\n                            \"\n                            :image-url=\"item.exchange_item.image_url\"\n                            is-small-thumbnail\n                            from-return-item\n                          />\n                        </v-col>\n                        <!-- <v-col cols=\"2\" align-self=\"center\">\n                        <div class=\"d-flex justify-center align-center\">\n                          <o-text sm>\n                            {{ item.exchange_item.formattedExchangePrice }}\n                          </o-text>\n                        </div>\n                      </v-col> -->\n                      </v-row>\n                    </v-row>\n                  </div>\n                </v-row>\n              </template>\n            </div>\n\n            <div v-if=\"!isExchangeOrder(returnItem.rma_items)\">\n              <o-text sm bold>\n                {{ REFUND }}\n              </o-text>\n\n              <template v-for=\"(item, index) in returnItem.rma_items\">\n                <v-row :key=\"index\">\n                  <div v-if=\"!isExchange(item)\" class=\"d-flex mb-4\">\n                    <v-row>\n                      <v-col cols=\"8\">\n                        <product-thumbnail\n                          :key=\"index\"\n                          :item=\"item\"\n                          :index=\"index\"\n                          :title=\"item.order_item.title\"\n                          :sub-title1=\"item.order_item.subtitle\"\n                          :image-url=\"item.order_item.image_url\"\n                          is-small-thumbnail\n                          from-return-item\n                        />\n                      </v-col>\n                      <v-col\n                        cols=\"2\"\n                        class=\"ml-auto\"\n                      >\n                        <div class=\"d-flex align-center justify-end\" v-if=\"item.quantity && item.quantity > 0\">\n                          <v-icon style=\"color: #000000;\" x-small>\n                            close\n                          </v-icon>\n                          <o-text\n                            v-html=\"item.quantity\"\n                            class=\"ml-1 mr-2 mb-0 \"\n                            sm\n                            semi-bold\n                          />\n                        </div>\n                      </v-col>\n                      <v-col style=\"max-width: 50px\">\n                        <o-text\n                          sm\n                          :key=\"index\"\n                          class=\"d-flex justify-end\"\n                        >\n                          {{ getRefundPrice(returnItem) }}\n                        </o-text>\n                      </v-col>\n                    </v-row>\n                  </div>\n                </v-row>\n              </template>\n            </div>\n\n            <v-divider />\n\n            <div\n              class=\"d-flex justify-content-between align-center justify-center my-5\"\n            >\n              <action-button\n                @click.stop=\"trackMyReturn(returnItem)\"\n                primary-color\n                :block=\"$vuetify.breakpoint.smAndDown\"\n              >\n                {{\n                  isExchangeOrder(returnItem.rma_items)\n                    ? TRACK_MY_EXCHANGE\n                    : TRACK_MY_RETURN\n                }}\n              </action-button>\n            </div>\n          </div>\n        </div>\n      </v-row>\n    </template>\n  </div>\n</template>\n\n<script>\nimport ProductThumbnail from '@/components/organism/ProductThumbnail.vue'\nimport { mapGetters, mapMutations } from 'vuex'\nimport OText from '@/components/atoms/OText.vue'\nimport { getFormattedDate } from '@/utils'\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport {\n  UPDATE_REFUND_LIST_FOR_RETURN_USER,\n  UPDATE_EXCHANGE_LIST_FOR_RETURN_USER\n} from '@/store/constants.js'\nimport { TRACKING_VIEW, RETURN_REVIEW } from 'branding/strings.json'\nimport { CONSTANTS } from '@/constants'\nexport default {\n  name: 'ReturnUserView',\n\n  components: {\n    ProductThumbnail,\n    OText,\n    ActionButton\n  },\n\n  data () {\n    return {\n      COMPLETED: TRACKING_VIEW.COMPLETED,\n      ACTIVE: TRACKING_VIEW.ACTIVE,\n      CANCELED: TRACKING_VIEW.CANCELED,\n      REFUND: TRACKING_VIEW.REFUND,\n      EXCHANGE: TRACKING_VIEW.EXCHANGE,\n      TRACK_MY_RETURN: TRACKING_VIEW.TRACK_MY_RETURN,\n      TRACK_MY_EXCHANGE: TRACKING_VIEW.TRACK_MY_EXCHANGE,\n      ORIGINAL_PURCHASE: RETURN_REVIEW.ORIGINAL_PURCHASE,\n      INCOMING_ITEM: RETURN_REVIEW.INCOMING_ITEM,\n      RETURN_STATUS: CONSTANTS.RETURN_STATUS,\n      rmaStatus: ''\n    }\n  },\n\n  computed: {\n    ...mapGetters({ rmaList: 'order/rmaList', order: 'order/order' })\n  },\n\n  props: {},\n\n  methods: {\n    ...mapMutations({\n      setSelectedReturnMethod: 'returns/setSelectedReturnMethod'\n    }),\n    getRefundPrice (items) {\n      const price = items?.rma_items_original?.rma_items\n        .map(item => item?.order_item?.calculated_unit_price_cents / 100)\n        .reduce((prev, curr) => prev + curr, 0)\n      return `$${Number(price).toFixed(2)}`\n    },\n    getReturnDate (createdAt) {\n      return 'Requested on ' + getFormattedDate(createdAt)\n    },\n    isExchangeOrder (rmaItems) {\n      return rmaItems?.filter((rmaItem) => rmaItem?.is_exchange)?.length > 0\n    },\n    async trackMyReturn (returnItem) {\n      this.$store.commit('returns/updateSelectedReturnItem', returnItem)\n      const refundList = []\n      const exchangeList = []\n\n      returnItem.rma_items.map((item) => {\n        if (!item?.is_exchange) {\n          refundList.push(item.order_item)\n        } else {\n          exchangeList.push(item.exchange_item)\n        }\n      })\n      this.$store.commit(\n        'order/' + UPDATE_REFUND_LIST_FOR_RETURN_USER,\n        refundList\n      )\n      this.$store.commit(\n        'order/' + UPDATE_EXCHANGE_LIST_FOR_RETURN_USER,\n        exchangeList\n      )\n\n      this.setSelectedReturnMethod(returnItem.return_method)\n      this.$router.push(\n        '/orders/' + this.order.order_hash + '/tracker/' + returnItem.identifier\n      )\n    },\n    isExchange (item) {\n      return item.is_exchange && item.exchange_item\n    },\n    getStepperlabel (returnStatus) {\n      var title = ''\n      var icon = ''\n      if (\n        returnStatus === this.RETURN_STATUS.STARTED ||\n        returnStatus === this.RETURN_STATUS.IN_TRANSIT ||\n        returnStatus === this.RETURN_STATUS.RECEIVED\n      ) {\n        title = this.ACTIVE\n        icon = 'hourglass_top'\n      } else if (returnStatus === this.RETURN_STATUS.COMPLETED) {\n        title = this.COMPLETED\n        icon = 'done'\n      } else if (returnStatus === this.RETURN_STATUS.CANCELED) {\n        title = this.CANCELED\n        icon = 'block'\n      }\n      return { title, icon }\n    }\n  },\n  mounted () {\n    this.getStepperlabel()\n  }\n}\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components/pages"}]}