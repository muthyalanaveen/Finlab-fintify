terEvent (event) {\n      this.$emit('onKeyEnter', event)\n    },\n    onPasteMethod (event) {\n      this.$emit('onPaste', event)\n    }\n  },\n  computed: {\n    defaultValue () {\n      const item = this.items.find((e) => e.subtitle === 'Original Purchase')\n      // return the previously selected item by default unless the previously selected item was not of the same color as the default item\n      let defaultItem = this.previouslySelectedOption || item || this.items[0]\n      if (item && this.previouslySelectedOption && item.color !== this.previouslySelectedOption?.color) {\n        defaultItem = item\n      }\n      if (defaultItem && defaultItem.disabled === false) {\n        this.$store.commit('returns/updateReturnReasonSelected', true)\n      }\n      return defaultItem\n    }\n  }\n}\n</script>\n\n<style></style>\n"],"sourceRoot":"src/components/molecules"}]}