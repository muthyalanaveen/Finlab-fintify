Ab,GACJzE,IAAI,CAACyE,uBAAL,GAA+B,GAD3B,GAEJ,CAFG,EAGLF,OAHK,CAGG,CAHH,CAAP;AAID;;AAED,SAASrD,yBAAT,CAAoClB,IAApC,EAA0C;AACxC,MAAMrB,KAAK,GAAGqC,gBAAgB,CAAChB,IAAD,CAA9B;AACA,oBAAWwE,MAAM,CAAC7F,KAAD,CAAN,CAAc4F,OAAd,CAAsB,CAAtB,CAAX;AACD;;AAED,SAASlF,cAAT,CAAyB+D,SAAzB,EAAoC;AAAA;;AAClC,MAAI,EAACA,SAAD,aAACA,SAAD,uCAACA,SAAS,CAAEC,QAAZ,iDAAC,qBAAqBC,MAAtB,CAAJ,EAAkC,OAAO,IAAP;AAElC,SAAO,CAAAF,SAAS,SAAT,IAAAA,SAAS,WAAT,YAAAA,SAAS,CAAEC,QAAX,CAAoBpD,IAApB,CAAyB,UAAAyE,OAAO;AAAA,WAAIA,OAAO,CAACC,kBAAR,KAA+BvB,SAAS,CAACuB,kBAA7C;AAAA,GAAhC,OACLvB,SADK,aACLA,SADK,uBACLA,SAAS,CAAEC,QAAX,CAAoBpD,IAApB,CAAyB,UAAAyE,OAAO;AAAA,WAAIA,OAAO,CAACvD,GAAR,KAAgBiC,SAAS,CAACjC,GAA9B;AAAA,GAAhC,CADK,CAAP;AAED","sourcesContent":["import {\n  FETCH_START,\n  FETCH_ORDER_END,\n  SET_ERROR,\n  UPDATE_SELECTED_ITEM,\n  UPDATE_SELECTED_ITEM_QUANTITY,\n  SELECT_ALL_ITEMS,\n  UPDATE_REFUND_LIST,\n  UPDATE_REFUND_LIST_FOR_RETURN_USER,\n  UPDATE_EXCHANGE_LIST_FOR_RETURN_USER,\n  UPDATE_SELECTED_ITEM_EXCHANGE_ELIGIBILITY,\n  EMPTY_REFUND_LIST,\n  EMPTY_EXCHANGE_LIST,\n  UPDATE_SELECTED_ITEM_WITH_EXCHANGE_VARIANTS,\n  UPDATE_EXCHANGE_LIST,\n  REMOVE_ITEM_FROM_EXCHANGE_LIST\n} from '@/store/constants'\nimport { CONSTANTS } from '@/constants/index.js'\n\nconst { RMA_STATUS, RETURN_METHODS, GOODS_STATUS, RETURN_STATUS } = CONSTANTS\n\nexport default {\n  [FETCH_START] (state) {\n    state.isLoading = true\n  },\n  [FETCH_ORDER_END] (state, data) {\n    state.order = {\n      ...data,\n      order: {\n        ...data.order,\n        items: data.order.items\n          ? data.order.items.map((o) => {\n            return {\n              ...o,\n              selected: false,\n              isSingleQuantity: o.quantity === 1,\n              subtitle: getSubtitle(o),\n              price: getRefundPrice(o),\n              formattedRefundPrice: getFormattedRefundPrice(o),\n              original_quantity: o.quantity,\n              quantity: o.quantity > 1 ? 1 : o.quantity,\n              exchangeVariants: [],\n              color: getValueFromAttribute(o, 'color'),\n              size: getValueFromAttribute(o, 'size'),\n              quantity_in_stock: getVariantData(o)?.quantity_in_stock || 0\n            }\n          })\n          : []\n      }\n    }\n\n    state.rmaList = {\n      rmaListItems: data.rma_list\n        ? data.rma_list.map((returnItems) => {\n          return {\n            ...returnItems,\n            return_status: getRMAStatus(returnItems),\n            rma_items: returnItems.rma_items.map((rmaItem) => {\n              return {\n                ...rmaItem,\n                order_item: {\n                  ...rmaItem.order_item,\n                  selected: false,\n                  isSingleQuantity: rmaItem.order_item.quantity === 1,\n                  subtitle: getSubtitle(rmaItem.order_item),\n                  price: getRefundPrice(rmaItem.order_item),\n                  formattedRefundPrice: getFormattedRefundPrice(\n                    rmaItem.order_item\n                  ),\n                  original_quantity: rmaItem.order_item.quantity,\n                  quantity:\n                  rmaItem.order_item.quantity > 1\n                    ? 1\n                    : rmaItem.order_item.quantity,\n                  exchangeVariants: [],\n                  color: getValueFromAttribute(rmaItem.order_item, 'color'),\n                  size: getValueFromAttribute(rmaItem.order_item, 'size'),\n                  quantity_in_stock: getVariantData(rmaItem.order_item)?.quantity_in_stock || 0\n                }\n              }\n            })\n          }\n        })\n        : []\n    }\n\n    state.isLoading = false\n  },\n  [SET_ERROR] (state) {\n    state.isLoading = false\n  },\n  [UPDATE_SELECTED_ITEM] (state, identifier) {\n    const item = state.order.order.items.find(\n      (x) => x.identifier === identifier\n    )\n    item.selected = !item.selected\n  },\n  [UPDATE_SELECTED_ITEM_QUANTITY] (state, { identifier, quantity }) {\n    const item = state.order.order.items.find(\n      (x) => x.identifier === identifier\n    )\n    item.quantity = quantity\n  },\n  [UPDATE_SELECTED_ITEM_EXCHANGE_ELIGIBILITY] (state, { identifier, exchangeMetadata }) {\n    const item = state.order.order.items.find(\n      (x) => x.identifier === identifier\n    )\n    item.exchange_metadata = exchangeMetadata\n  },\n  [SELECT_ALL_ITEMS] (state) {\n    state.order.order.items = state.order.order.items.map((l) => {\n      return { ...l, selected: true }\n    })\n  },\n  [UPDATE_REFUND_LIST_FOR_RETURN_USER] (state, list) {\n    state.refundList = list\n  },\n  [UPDATE_EXCHANGE_LIST_FOR_RETURN_USER] (state, list) {\n    state.exchangeList = list\n  },\n  [UPDATE_REFUND_LIST] (state, refundItem) {\n    const existingIds = state.refundList.map((x) => x.identifier)\n    if (\n      state.refundList.indexOf(refundItem) === -1 &&\n      !existingIds.includes(refundItem.identifier)\n    ) {\n      state.refundList.push(refundItem)\n\n      // remove item from exchange list\n      const filteredExchangeList = state.exchangeList.filter(\n        (item) => item.identifier !== refundItem.identifier\n      )\n      state.exchangeList = filteredExchangeList\n    } else {\n      state.refundList = state.refundList.map((item) => {\n        if (item.identifier !== refundItem.identifier) {\n          return item\n        } else {\n          return refundItem\n        }\n      })\n    }\n  },\n\n  [EMPTY_REFUND_LIST] (state) {\n    state.refundList = []\n  },\n\n  [UPDATE_SELECTED_ITEM_WITH_EXCHANGE_VARIANTS] (state, { data, identifier }) {\n    const item = state.order.order.items.find(\n      (x) => x.identifier === identifier\n    )\n    item.exchangeVariants = data.map((item) => {\n      return {\n        ...item,\n        price: getExchangePrice(item),\n        formattedExchangePrice: getFormattedExchangePrice(item),\n        sku: item.SKU,\n        identifier: item.SKU\n      }\n    })\n  },\n\n  [REMOVE_ITEM_FROM_EXCHANGE_LIST] (state, identifier) {\n    const filteredReturnList = state.exchangeList.filter(\n      (item) => item.identifier !== identifier\n    )\n    state.exchangeList = filteredReturnList\n  },\n\n  [UPDATE_EXCHANGE_LIST] (state, exchangeItem) {\n    // if exchangeItem doesn't exist in the list, add it and check if it's already in the refund list, if so remove it from the refund list\n    // else update the item in the exhcange list\n\n    const existingIds = state.exchangeList.map((x) => x.identifier)\n    if (\n      state.exchangeList.indexOf(exchangeItem) === -1 &&\n      !existingIds.includes(exchangeItem.identifier)\n    ) {\n      state.exchangeList.push(exchangeItem)\n\n      // remove from refund list if exists\n      const filteredRefundList = state.refundList.filter(\n        (item) => item.identifier !== exchangeItem.identifier\n      )\n      state.refundList = filteredRefundList\n    } else {\n      state.exchangeList = state.exchangeList.map((item) => {\n        if (item.identifier !== exchangeItem.identifier) {\n          return item\n        } else {\n          return exchangeItem\n        }\n      })\n    }\n  },\n  [EMPTY_EXCHANGE_LIST] (state) {\n    state.exchangeList = []\n  }\n}\n\nfunction getRMAStatus (returnItem) {\n  let rmaStatusRes = RETURN_STATUS.STARTED\n  const rmaStatus = returnItem.status.toUpperCase()\n  const goodsStatus = returnItem.goods_status.toUpperCase()\n  var returnMethodType = ''\n  if (returnItem && returnItem.return_method) {\n    returnMethodType = returnItem.return_method.type\n  }\n\n  if (returnMethodType && returnMethodType === RETURN_METHODS.MAIL_BACK) {\n    if (\n      rmaStatus === RMA_STATUS.CREATED &&\n      goodsStatus === GOODS_STATUS.UNKNOWN\n    ) {\n      rmaStatusRes = RETURN_STATUS.STARTED\n    } else if (\n      rmaStatus === RMA_STATUS.CREATED &&\n      (goodsStatus === GOODS_STATUS.PRE_TRANSIT ||\n        goodsStatus === GOODS_STATUS.TRANSIT ||\n        goodsStatus === GOODS_STATUS.DELIVERED)\n    ) {\n      rmaStatusRes = RETURN_STATUS.IN_TRANSIT\n    } else if (\n      (rmaStatus === RMA_STATUS.REFUNDED ||\n        rmaStatus === RMA_STATUS.REFUND_ERROR) &&\n      goodsStatus === GOODS_STATUS.TRANSIT\n    ) {\n      rmaStatusRes = RETURN_STATUS.RECEIVED // Transit status will be skipped and goes directly to RECEIVED status\n    } else if (\n      rmaStatus === RMA_STATUS.READY_TO_REFUND &&\n      (goodsStatus === GOODS_STATUS.PRE_TRANSIT ||\n        goodsStatus === GOODS_STATUS.DELIVERED ||\n        goodsStatus === GOODS_STATUS.RECEIVED)\n    ) {\n      rmaStatusRes = RETURN_STATUS.RECEIVED\n    } else if (\n      (rmaStatus === RMA_STATUS.REFUNDED ||\n        rmaStatus === RMA_STATUS.REFUND_ERROR ||\n        rmaStatus === RMA_STATUS.PARTIAL_REFUNDED) &&\n      (goodsStatus === GOODS_STATUS.DELIVERED ||\n        goodsStatus === GOODS_STATUS.RECEIVED)\n    ) {\n      rmaStatusRes = RETURN_STATUS.COMPLETED\n    }\n  } else if (\n    returnMethodType &&\n    returnMethodType === RETURN_METHODS.EXPRESS_RETURNS\n  ) {\n    if (\n      rmaStatus === RMA_STATUS.CREATED &&\n      goodsStatus === GOODS_STATUS.CREATED\n    ) {\n      rmaStatusRes = RETURN_STATUS.STARTED\n    } else if (\n      rmaStatus === RMA_STATUS.CREATED &&\n      goodsStatus === GOODS_STATUS.ACCEPTED\n    ) {\n      rmaStatusRes = RETURN_STATUS.IN_TRANSIT\n    } else if (\n      rmaStatus === RMA_STATUS.READY_TO_REFUND &&\n      goodsStatus === GOODS_STATUS.ACCEPTED\n    ) {\n      rmaStatusRes = RETURN_STATUS.RECEIVED\n    } else if (\n      rmaStatus === RMA_STATUS.REFUNDED &&\n      goodsStatus === GOODS_STATUS.ACCEPTED\n    ) {\n      rmaStatusRes = RETURN_STATUS.COMPLETED\n    }\n  } else if (returnMethodType && returnMethodType === RETURN_METHODS.IN_STORE) {\n    if (\n      rmaStatus === RMA_STATUS.CREATED &&\n      goodsStatus === GOODS_STATUS.CREATED\n    ) {\n      rmaStatusRes = RETURN_STATUS.STARTED\n    } else if (\n      rmaStatus === RMA_STATUS.READY_TO_REFUND &&\n      goodsStatus === GOODS_STAT