ss=\"mr-2\">\n            schedule\n          </v-icon>\n          <o-text>\n            {{ carrier.openingHours.isOpen ? 'Open' : 'Closed' }}\n            <span v-if=\"carrier.openingHours.todayClosingTime !== ''\">\n              <span class=\"mr-.5 ml-.5\">&middot;</span>\n              {{ carrier.openingHours.isOpen ? `Closes at ${carrier.openingHours.todayClosingTime}`:`Opens at ${carrier.openingHours.todayOpeningTime}` }}\n            </span>\n          </o-text>\n        </div>\n        <div class=\"d-flex align-center\">\n          <v-icon\n            v-if=\"carrier.requirePackageAndPrint\"\n            small\n            class=\"mr-2\"\n          >\n            print\n          </v-icon>\n          <v-icon\n            v-else\n            small\n            class=\"mr-2\"\n          >\n            print_disabled\n          </v-icon>\n          <o-text>\n            {{ carrier.requirePackageAndPrint ? PACKAGE_AND_PRINT : NO_PACKAGE_AND_PRINT }}\n          </o-text>\n        </div>\n        <div v-if=\"carrier.isSustainable\" class=\"d-flex align-center\">\n          <v-icon small class=\"mr-2\">\n            spa\n          </v-icon>\n          <o-text>\n            {{ SUSTAINABLE }}\n          </o-text>\n        </div>\n      </section>\n      <div class=\"d-flex mt-4 pb-6 align-center justify-content-between\">\n        <action-button\n          @click.stop=\"openModal({\n            type: DIALOG_CARRIER_INFORMATION,\n            selectedCarrier: carrier,\n            disableModalGoBack: true\n          })\"\n          tertiary-color\n          text\n        >\n          Learn more\n        </action-button>\n        <action-button\n          @click=\"handleCarrierSelection(carrier)\"\n          primary-color\n          large\n        >\n          Choose\n        </action-button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport OText from '@/components/atoms/OText.vue'\nimport { RETURN_METHOD_SCREENS } from 'branding/strings.json'\nimport { mapMutations } from 'vuex'\nimport { DIALOG_CARRIER_INFORMATION, DIALOG_CARRIER_LOCATIONS } from '@/store/constants'\nimport { getReturnsHelperText } from '@/utils'\n\nexport default {\n  name: 'ShippingCarrierCard',\n  components: {\n    ActionButton,\n    OText\n  },\n  data () {\n    return ({\n      PACKAGE_AND_PRINT: RETURN_METHOD_SCREENS.PACKAGE_AND_PRINT,\n      NO_PACKAGE_AND_PRINT: RETURN_METHOD_SCREENS.NO_PACKAGE_AND_PRINT,\n      SUSTAINABLE: RETURN_METHOD_SCREENS.SUSTAINABLE,\n      DIALOG_CARRIER_INFORMATION,\n      DIALOG_CARRIER_LOCATIONS\n    })\n  },\n  methods: {\n    ...mapMutations({\n      openModal: 'returns/openModal'\n    }),\n    getReturnsHelperText (returnType) {\n      return getReturnsHelperText(returnType)\n    }\n  },\n  mounted () {},\n  props: {\n    isShowCase: {\n      type: Boolean,\n      default: false\n    },\n    carrier: {\n      type: Object,\n      required: true\n    },\n    handleCarrierSelection: {\n      type: Function,\n      required: true\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components/organism"}]}