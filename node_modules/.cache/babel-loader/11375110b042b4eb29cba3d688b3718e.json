d\n              class=\"mt-2\"\n              heading\n              :level=\"Number(1)\"\n            >\n              {{ RETURN_CONFIRMED }}\n            </o-text>\n            <o-text sm class=\"mt-1\">\n              {{ getConfirmationEmail() }}\n            </o-text>\n            <div class=\"d-flex mt-4 align-center justify-center\">\n              <action-button\n                @click.stop=\"openReturnLabelLink\"\n                secondary-color\n                outlined\n                :loading=\"isLoading\"\n                :disabled=\"isLoading\"\n                v-if=\"this.selectedReturnMethod.type === MAIL_BACK || this.selectedReturnMethod.type === EXPRESS_RETURNS\"\n              >\n                {{\n                  this.selectedReturnMethod.type === MAIL_BACK\n                    ? PRINT_RETURN_LABEL\n                    : VIEW_RETURN_CODES_BUTTON\n                }}\n              </action-button>\n              <o-dialog\n                :visible=\"showReturnCodes\"\n                is-full-screen\n                @close=\"toggleShowReturnCodes\"\n              >\n                <QRCodeView\n                  v-if=\"this.selectedReturnMethod.type === EXPRESS_RETURNS\"\n                  :qr-data=\"this.qrData\"\n                  from-return-methods\n                  :is-exchange-or-refund=\"this.isExchangeOrRefund\"\n                />\n              </o-dialog>\n              <action-button\n                tertiary-color\n                text\n                class=\"ml-4\"\n                @click.stop=\"toggleReturnDetails(true)\"\n              >\n                {{ VIEW_RETURN_DETAILS_BUTTON }}\n              </action-button>\n              <o-dialog\n                :visible=\"showReturnDetails\"\n                is-full-screen\n                @close=\"toggleReturnDetails(false)\"\n              >\n                <ReviewView\n                  is-modal\n                  :show-confirm-action-view=\"false\"\n                  :title=\"this.RETURN_DETAILS_TITLE\"\n                  :sub-title=\"getReturnOrderNumber()\"\n                  show-order-details\n                  :return-date=\"getReturnDate()\"\n                />\n              </o-dialog>\n            </div>\n          </div>\n        </v-row>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport OText from '@/components/atoms/OText.vue'\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport { CONFIRMATION_VIEW, RETURN_REVIEW, PAGE_TITLES } from 'branding/strings.json'\nimport ODialog from '@/components/atoms/ODialog.vue'\nimport ReviewView from '@/components/pages/ReviewView.vue'\nimport QRCodeView from '@/components/pages/QRCodeView.vue'\nimport { mapGetters } from 'vuex'\nimport { getFormattedDate, returnFormattedOrderNumber } from '@/utils'\nimport { GET_SHIPPING_LABEL, GET_QR_CODE_LABEL } from '@/store/constants.js'\nimport { CONSTANTS } from '@/constants/index.js'\nimport {\n  isExchangeOrRefundOrderItems\n} from '@/controllers/OrdersController.js'\nimport { getIsGift, getGiftEmail } from '@/store/sessionStorage.js'\n\nexport default {\n  name: 'ConfirmationView',\n\n  components: {\n    OText,\n    ActionButton,\n    ODialog,\n    ReviewView,\n    QRCodeView\n  },\n\n  computed: {\n    ...mapGetters({\n      order: 'order/order',\n      selectedReturnMethod: 'returns/getSelectedReturnMethod',\n      rmaCreatedAt: 'ui/rmaCreatedAt',\n      refundList: 'order/refundList',\n      exchangeList: 'order/exchangeList'\n    }),\n    isExchangeOrRefund () {\n      return isExchangeOrRefundOrderItems()\n    }\n  },\n  data () {\n    return {\n      RETURN_CONFIRMED: CONFIRMATION_VIEW.RETURN_CONFIRMED,\n      CONFIRMATION_EMAIL_MESSAGE: CONFIRMATION_VIEW.CONFIRMATION_EMAIL_MESSAGE,\n      VIEW_RETURN_CODES_BUTTON: CONFIRMATION_VIEW.VIEW_RETURN_CODES_BUTTON,\n      PRINT_RETURN_LABEL: CONFIRMATION_VIEW.PRINT_RETURN_LABEL,\n      VIEW_RETURN_DETAILS_BUTTON: CONFIRMATION_VIEW.VIEW_RETURN_DETAILS_BUTTON,\n      MAIL_BACK: CONSTANTS.RETURN_METHODS.MAIL_BACK,\n      EXPRESS_RETURNS: CONSTANTS.RETURN_METHODS.EXPRESS_RETURNS,\n      IN_STORE: CONSTANTS.RETURN_METHODS.IN_STORE,\n      RETURN_DETAILS_TITLE: RETURN_REVIEW.RETURN_DETAILS_TITLE,\n      RETURN_DETAILS_DATE_PREFIX: RETURN_REVIEW.RETURN_DETAILS_DATE_PREFIX,\n      showReturnDetails: false,\n      showReturnCodes: false,\n      returnLabelLink: '',\n      isGiftReturn: getIsGift(),\n      isLoading: false,\n      qrData: null,\n      state_Email: getGiftEmail()\n    }\n  },\n\n  props: {\n  },\n\n  methods: {\n    getReturnDate () {\n      return 'Requested on ' + getFormattedDate(this.rmaCreatedAt)\n    },\n    getReturnOrderNumber () {\n      return this.order ? returnFormattedOrderNumber(this.order.order_id) : ''\n    },\n    getConfirmationEmail () {\n      return this.CONFIRMATION_EMAIL_MESSAGE + (this.isGiftReturn && this.state_Email ? this.state_Email : this.order.shopper_email)\n    },\n    openReturnLabelLink () {\n      if (this.selectedReturnMethod.type !== this.MAIL_BACK) {\n        document.title = PAGE_TITLES.NEXT_STEPS\n      }\n      this.selectedReturnMethod.type === this.MAIL_BACK ? window.open(this.returnLabelLink) : this.showReturnCodes = true\n    },\n    toggleReturnDetails (opening) {\n      if (opening) {\n        this.showReturnDetails = true\n        document.title = PAGE_TITLES.REVIEW\n      } else {\n        this.showReturnDetails = false\n        document.title = PAGE_TITLES.CONFIRMATION\n      }\n    },\n    toggleShowReturnCodes () {\n      this.showReturnCodes = false\n      document.title = PAGE_TITLES.CONFIRMATION\n    }\n  },\n\n  mounted () {\n    const identifier = this.$route.params.identifier\n    const params = {\n      identifier\n    }\n    this.isLoading = true\n    if (this.selectedReturnMethod.type === this.MAIL_BACK) {\n      this.$store\n        .dispatch('order/' + GET_SHIPPING_LABEL, params)\n        .then((response) => {\n          if (response) {\n            this.returnLabelLink = response.label_url\n          }\n        })\n        .catch((err) => {\n          console.error({ err })\n        })\n        .finally(() => {\n          this.isLoading = false\n        })\n    } else if (this.selectedReturnMethod.type === this.EXPRESS_RETURNS || this.selectedReturnMethod.type === this.IN_STORE) {\n      this.$store\n        .dispatch('order/' + GET_QR_CODE_LABEL, params)\n        .then((response) => {\n          if (response) {\n            this.qrData = response\n          }\n        })\n        .catch((err) => {\n          console.error({ err })\n        })\n        .finally(() => {\n          this.isLoading = false\n        })\n    }\n  },\n  beforeRouteLeave (to, from, next) {\n    if (!to.hash) {\n      window.location.replace('/')\n      return\n    }\n    next()\n  },\n  created () {}\n}\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/components/pages"}]}