1\">\n          <o-text\n            sm\n            class=\"mr-1\"\n            v-if=\"this.order.order.created_at\"\n          >\n            {{ this.order.order.items.length > 0 && this.order.rma_list.length > 0 ? 'Placed on ' : 'Purchased on ' }}{{ getReturnDate(this.order.order.created_at) }}\n          </o-text>\n          <o-text sm>\n            {{ this.totalItemsCountTitle }}\n          </o-text>\n          <o-text\n            sm\n            class=\"ml-1\"\n            v-if=\"this.rmaItemsCountTitle\"\n          >\n            {{ this.rmaItemsCountTitle }}\n          </o-text>\n        </div>\n      </div>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport { ORDER_DISPLAY } from 'branding/strings.json'\nimport { mapGetters } from 'vuex'\nimport OText from '@/components/atoms/OText.vue'\nimport HeaderMediumTitle from '@/components/molecules/HeaderMediumTitle.vue'\nimport { getFormattedDate } from '@/utils'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\n\nexport default {\n  name: 'ProductListHeaderView',\n  data () {\n    return {\n      orderId: '',\n      zipCode: '',\n      isDisabled: true,\n      SELECT_ITEMS_TO_RETURN: ORDER_DISPLAY.SELECT_ITEMS_TO_RETURN\n    }\n  },\n  components: {\n    OText,\n    HeaderMediumTitle,\n    HeaderTitle\n  },\n  props: {\n    selectedCount: {\n      type: Number,\n      required: true\n    },\n    buttonTitle: {\n      type: String,\n      required: false,\n      default: ''\n    },\n    orderNumber: {\n      type: String,\n      required: true\n    },\n    totalItemsCountTitle: {\n      type: String,\n      required: true\n    },\n    rmaItemsCountTitle: {\n      type: String,\n      required: true\n    },\n    isReturnUser: {\n      type: Boolean,\n      required: false\n    }\n  },\n  methods: {\n    getReturnDate (createdAt) {\n      return getFormattedDate(createdAt)\n    }\n  },\n  computed: { ...mapGetters({ order: 'order/order' }) }\n}\n</script>\n\n<style scoped>\n</style>\n"]}]}