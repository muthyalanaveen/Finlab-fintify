ify-center align-center\">\n          <p\n            class=\"ma-0 mt-0 pb-3 headingSubTitleBoldPrimary\"\n            v-html=\"tab.name\"\n          />\n          <div\n            v-if=\"tab.index !== active_tab\"\n            class=\"v-tabs-slider-wrapper\"\n            style=\"height: 1px; width: 100%; color: #D9D9D9\"\n          >\n            <div class=\"v-tabs-slider\" />\n          </div>\n        </v-col>\n      </v-tab>\n    </v-tabs>\n    <v-tabs-items v-model=\"active_tab\">\n      <v-tab-item\n        v-for=\"tab of tabs\"\n        :key=\"tab.index\"\n        role=\"tabpanel\"\n      >\n        <slot v-if=\"tab.index === 0\" />\n        <refund-card\n          v-if=\"tab.index === 1\"\n          @click.once=\"returnReasonClicked\"\n          :is-map-loading=\"isMapLoading\"\n        />\n      </v-tab-item>\n    </v-tabs-items>\n  </div>\n</template>\n\n<script>\nimport { PAGE_TITLES } from 'branding/strings.json'\n\nexport default {\n  components: {\n    RefundCard: () => import('@/components/organism/RefundView.vue')\n  },\n  name: 'ExchangeTabWrapper',\n  data: () => ({\n    active_tab: 0,\n    tabs: [\n      {\n        index: 0,\n        name:\n          \"<div class='headingSubTitleBoldPrimary mt-2'>Exchange</div><div class='headingSubTitleSmallBold mt-1'>Ships immediately</div>\"\n      },\n      {\n        index: 1,\n        name:\n          \"<div class='headingSubTitleBoldPrimary mt-2'>Refund</div><div class='headingSubTitleSmallBold mt-1'>After item received</div>\"\n      }\n    ]\n  }),\n  watch: {\n    active_tab: {\n      handler () {\n        this.active_tab === 0 ? document.title = PAGE_TITLES.EXCHANGE : document.title = PAGE_TITLES.REFUND\n      }\n    }\n  },\n  props: {\n    returnReasonClicked: {\n      type: Function,\n      required: true\n    },\n    isMapLoading: {\n      required: true,\n      type: Boolean\n    }\n  },\n  methods: {},\n  computed: {\n  },\n  created () {\n  },\n  mounted () { // lands on exchanges tab\n    document.title = PAGE_TITLES.EXCHANGE\n  },\n  beforeUnmount () {}\n}\n</script>\n\n<style>\n#exchange-tab .v-slide-group__prev {\n  display: none !important;\n}\n</style>\n"]}]}