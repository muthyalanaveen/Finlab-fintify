{"remainingRequest":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/pages/OrderandZipCodeView.vue?vue&type=style&index=0&id=f1ed2ac2&scoped=true&lang=css&","dependencies":[{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/pages/OrderandZipCodeView.vue","mtime":1665066062322},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/css-loader/index.js","mtime":1649977086861},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":1649977090577},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/cache-loader/dist/cjs.js","mtime":1649977086874},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js","mtime":1649977089802}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["OrderandZipCodeView.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkPA;AACA;AACA","file":"OrderandZipCodeView.vue","sourceRoot":"src/components/pages","sourcesContent":["<template>\n  <v-container>\n    <v-row class=\"d-flex justify-center\">\n      <v-col\n        md=\"4\"\n        sm=\"8\"\n        xs=\"12\"\n        :class=\"{\n          'px-7': $vuetify.breakpoint.smAndDown,\n          'py-5': $vuetify.breakpoint.smAndDown,\n        }\"\n      >\n        <v-form>\n          <div class=\"d-flex align-center\" role=\"alert\">\n            <v-icon\n              style=\"color: #d83933;\"\n              small\n              aria-label=\"error\"\n              role=\"img\"\n            >\n              mdi-alert-circle\n            </v-icon>\n            <o-text\n              sm\n              semi-bold\n              text-red\n              class=\"pl-2\"\n            >\n              {{ NO_ORDER_FOUND }}\n            </o-text>\n          </div>\n\n          <header-title\n            :class=\"{\n              'pb-3': $vuetify.breakpoint.smAndDown,\n              'pb-5': $vuetify.breakpoint.mdAndUp,\n            }\"\n          >\n            {{ PLEASE_TRY_AGAIN }}\n          </header-title>\n          <o-text-field\n            :title=\"ENTER_ORDER_ID\"\n            id=\"orderId\"\n            v-model=\"orderId\"\n            @onChange=\"onOrderChange($event)\"\n            @onKeyEnter=\"lookupOrder\"\n            @onPaste=\"validate()\"\n            :class=\"{ 'pb-3': $vuetify.breakpoint.mdAndUp }\"\n            :placeholder=\"ENTER_ORDER_ID_PLACEHOLDER\"\n          />\n          <o-text-field\n            :title=\"EMAIL_TEXT_FIELD_TITLE\"\n            id=\"email\"\n            v-if=\"isGiftReturn\"\n            v-model=\"email\"\n            @onChange=\"onEmailChange($event)\"\n            @onKeyEnter=\"lookupOrder\"\n            @onPaste=\"validate()\"\n            :class=\"{ 'pb-3': $vuetify.breakpoint.mdAndUp }\"\n            :placeholder=\"EMAIL\"\n          />\n          <o-text-field\n            :title=\"ZIP_CODE_TEXT_FIELD_TITLE\"\n            id=\"zipCode\"\n            v-model=\"zipCode\"\n            @onChange=\"onZipChange($event)\"\n            @onKeyEnter=\"lookupOrder\"\n            :placeholder=\"ENTER_ZIP_CODE_PLACEHOLDER\"\n            @onPaste=\"validate()\"\n          />\n          <action-button\n            @click=\"lookupOrder\"\n            :class=\"{\n              'mt-2': $vuetify.breakpoint.smAndDown,\n              'mt-0': $vuetify.breakpoint.mdAndUp,\n            }\"\n            :loading=\"isLoading\"\n            :disabled=\"isDisabled\"\n            primary-color\n            block\n            x-large\n          >\n            {{ LOOKUP_ORDER }}\n          </action-button>\n        </v-form>\n        <v-col class=\"mt-8 mb-2\">\n          <a :href=\"'tel:'+this.customerSupportContact\" class=\"teleLink\">\n            <o-text\n              sm\n              text-blue\n              class=\"d-flex justify-center mb-1\"\n            >\n              Have a question? Give us a call.\n            </o-text>\n            <o-text\n              sm\n              text-blue\n              class=\"d-flex justify-center\"\n            >\n              {{ this.customerSupportContact }}\n            </o-text>\n          </a>\n        </v-col>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\nimport { FETCH_ORDER, GET_CONFIG } from '@/store/constants.js'\nimport OTextField from '@/components/molecules/OTextField.vue'\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport { validateOrderId, validateZipcode, validateEmail } from '@/utils'\nimport {\n  ORDER_LOOKUP,\n  ORDER_LOOKUP_TRY_AGAIN,\n  ZIP_CODE_LOOKUP,\n  EMAIL_LOOKUP\n} from 'branding/strings.json'\nimport OText from '@/components/atoms/OText.vue'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\nimport { setGiftEmail, getIsGift, getGiftEmail } from '@/store/sessionStorage.js'\n\nexport default {\n  components: {\n    OTextField,\n    ActionButton,\n    OText,\n    HeaderTitle\n  },\n  data () {\n    return {\n      orderId: '',\n      zipCode: '',\n      email: '',\n      isDisabled: true,\n      customerSupportContact: '',\n      NO_ORDER_FOUND: ORDER_LOOKUP_TRY_AGAIN.NO_ORDER_FOUND,\n      PLEASE_TRY_AGAIN: ORDER_LOOKUP_TRY_AGAIN.PLEASE_TRY_AGAIN,\n      ENTER_ORDER_ID: ORDER_LOOKUP.ENTER_ORDER_ID,\n      ENTER_ORDER_ID_PLACEHOLDER: ORDER_LOOKUP.ENTER_ORDER_ID_PLACEHOLDER,\n      ZIP_CODE_TEXT_FIELD_TITLE: ZIP_CODE_LOOKUP.ZIP_CODE_TEXT_FIELD_TITLE,\n      ENTER_ZIP_CODE_PLACEHOLDER: ZIP_CODE_LOOKUP.ENTER_ZIP_CODE_PLACEHOLDER,\n      LOOKUP_ORDER: ZIP_CODE_LOOKUP.LOOKUP_ORDER,\n      EMAIL_TEXT_FIELD_TITLE: EMAIL_LOOKUP.EMAIL_TEXT_FIELD_TITLE,\n      EMAIL: EMAIL_LOOKUP.EMAIL,\n      isGiftReturn: getIsGift()\n    }\n  },\n  computed: {\n    ...mapGetters({\n      order: 'order/order',\n      isLoading: 'order/isLoading',\n      state_orderId: 'ui/orderId',\n      state_zipCode: 'ui/zipCode',\n      config: 'returns/config'\n    })\n  },\n  mounted () {\n    // TODO: depending on when you refresh sometimes it will only save one of these values\n    this.orderId = this.state_orderId\n    this.zipCode = this.state_zipCode\n    this.email = getGiftEmail()\n    this.validate()\n\n    if (!this.config) {\n      this.$store\n        .dispatch('order/' + GET_CONFIG, null)\n        .then((response) => {\n          if (response) {\n            this.customerSupportContact =\n              response.retailer_info?.customer_support_contact\n          }\n        })\n        .catch((err) => {\n          console.error({ err })\n        })\n    } else {\n      this.customerSupportContact = this.config?.retailer_info?.customer_support_contact\n    }\n  },\n  methods: {\n    validate () {\n      this.isDisabled = !(\n        this.isGiftReturn\n          ? validateOrderId(this.orderId) &&\n        validateZipcode(this.zipCode) &&\n        validateEmail(this.email) : validateOrderId(this.orderId) &&\n        validateZipcode(this.zipCode)\n      )\n    },\n    onZipChange (event) {\n      const value = event.target.value\n      this.zipCode = value\n      this.validate()\n    },\n    onEmailChange (event) {\n      const value = event.target.value\n      this.email = value\n      this.validate()\n    },\n    onOrderChange (event) {\n      const value = event.target.value\n      this.orderId = value\n      this.validate()\n    },\n    async lookupOrder () {\n      this.$store.commit('ui/updateOrderId', this.orderId)\n      this.$store.commit('ui/updateZipCode', this.zipCode)\n      setGiftEmail(this.email)\n\n      let params = {\n        order_id: this.orderId,\n        zip_code: this.zipCode\n      }\n\n      if (this.isGiftReturn) {\n        params = { ...params, email: this.email }\n      }\n      this.$store\n        .dispatch('order/' + FETCH_ORDER, params)\n        .then((response) => {\n          if (response) {\n            const orderHash =\n              response && response != null\n                ? response.order_hash\n                : this.order.orderId\n            this.$router.push('/orders/' + orderHash)\n          } else {\n            this.$router.push('/orderandzipcode')\n          }\n        })\n        .catch((err) => {\n          console.error({ err })\n        })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.container {\n  padding-top: 60px;\n}\n</style>\n"]}]}