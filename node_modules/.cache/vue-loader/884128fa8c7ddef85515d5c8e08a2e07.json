{"remainingRequest":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/organism/ShippingCarrierCard.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/organism/ShippingCarrierCard.vue","mtime":1653330817041},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/cache-loader/dist/cjs.js","mtime":1649977086874},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/babel-loader/lib/index.js","mtime":1649977089120},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/cache-loader/dist/cjs.js","mtime":1649977086874},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js","mtime":1649977089802}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["ShippingCarrierCard.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6IA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"ShippingCarrierCard.vue","sourceRoot":"src/components/organism","sourcesContent":["<template>\n  <div class=\"bg-well px-4 mb-4\">\n    <div v-if=\"this.isShowCase\" class=\"d-flex align-center justify-content-between pt-2\">\n      <o-text\n        text-primary\n        bold\n        :md=\"$vuetify.breakpoint.smAndDown\"\n        :lg=\"$vuetify.breakpoint.mdAndUp\"\n        class=\"pt-2 pb-2\"\n      >\n        {{ this.getSelectedReturnMethodDropOffDescription(carrier) }}\n      </o-text>\n      <o-text\n        text-secondary\n        base\n        bold\n      >\n        {{ this.getShippingAmtText() }}\n      </o-text>\n    </div>\n    <div class=\"mt-2\">\n      <section class=\"d-flex\" v-if=\"!isMailBack\">\n        <o-text\n          text-secondary\n          class=\"mb-0\"\n        >\n          Nearest {{ carrier.distance || 'N/A' }}\n        </o-text>\n        <span class=\"mr-1 ml-1\">&middot;</span>\n        <o-text\n          text-secondary\n          class=\"mb-0\"\n        >\n          {{ carrier.formattedAddress || 'N/A' }}\n        </o-text>\n      </section>\n      <section v-if=\"!isUSPS\">\n        <o-text\n          style=\"cursor:pointer;\"\n          text-blue\n          :class=\"!isUSPS ? 'mb-2' : 'mb-4'\"\n          @click=\"openModal({type: DIALOG_CARRIER_LOCATIONS, selectedCarrier: carrier})\"\n        >\n          More locations\n        </o-text>\n      </section>\n      <section>\n        <div v-if=\"carrier.openingHours.isOpen !== undefined && !isMailBack\" class=\"d-flex align-center\">\n          <v-icon small class=\"mr-2\">\n            schedule\n          </v-icon>\n          <o-text>\n            {{ carrier.openingHours.isOpen ? 'Open' : 'Closed' }}\n            <span v-if=\"carrier.openingHours.todayClosingTime !== ''\">\n              <span class=\"mr-.5 ml-.5\">&middot;</span>\n              {{ carrier.openingHours.isOpen ? `Closes at ${carrier.openingHours.todayClosingTime}`:`Opens at ${carrier.openingHours.todayOpeningTime}` }}\n            </span>\n          </o-text>\n        </div>\n        <div class=\"d-flex align-center mb-1\" v-if=\"isUSPS\">\n          <v-icon\n            small\n            class=\"mr-2 mb-5\"\n          >\n            markunread_mailbox\n          </v-icon>\n          <o-text>\n            {{ USPS_PACKAGE }}\n          </o-text>\n        </div>\n        <div class=\"d-flex align-center\">\n          <v-icon\n            v-if=\"carrier.requirePackageAndPrint\"\n            small\n            class=\"mr-2\"\n          >\n            print\n          </v-icon>\n          <v-icon\n            v-else\n            small\n            class=\"mr-2\"\n          >\n            print_disabled\n          </v-icon>\n          <o-text v-if=\"!carrier.requirePackageAndPrint\">\n            {{ carrier.requirePackageAndPrint ? PACKAGE_AND_PRINT : NO_PACKAGE_AND_PRINT }}\n          </o-text>\n          <o-text v-else-if=\"carrier.requirePackageAndPrint && isMailBack\">\n            {{ PRINT }}\n          </o-text>\n          <o-text v-else>\n            {{ PACKAGE_AND_PRINT }}\n          </o-text>\n        </div>\n        <div class=\"d-flex align-center\" v-if=\"isMailBack && carrier.requirePackageAndPrint\">\n          <v-icon\n            small\n            class=\"mr-2\"\n          >\n            inventory\n          </v-icon>\n          <o-text>\n            {{ PACKAGE }}\n          </o-text>\n        </div>\n        <div v-if=\"carrier.isSustainable\" class=\"d-flex align-center\">\n          <v-icon small class=\"mr-2\">\n            spa\n          </v-icon>\n          <o-text>\n            {{ SUSTAINABLE }}\n          </o-text>\n        </div>\n      </section>\n      <div :class=\"isMailBack && !isShowCase ? 'd-flex mt-4 pb-3 align-center justify-content-between' : 'd-flex mt-4 pb-6 align-center justify-content-between'\">\n        <action-button\n          @click.stop=\"openModal({\n            type: DIALOG_CARRIER_INFORMATION,\n            selectedCarrier: carrier,\n            disableModalGoBack: true\n          })\"\n          tertiary-color\n          text\n          class=\"pl-0\"\n        >\n          Learn more\n        </action-button>\n        <action-button\n          @click=\"handleCarrierSelection(carrier)\"\n          primary-color\n          large\n        >\n          Choose\n        </action-button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport OText from '@/components/atoms/OText.vue'\nimport { RETURN_METHOD_SCREENS } from 'branding/strings.json'\nimport { mapMutations } from 'vuex'\nimport { DIALOG_CARRIER_INFORMATION, DIALOG_CARRIER_LOCATIONS } from '@/store/constants'\nimport { isUSPSCarrier, isMailBackReturn, getSelectedReturnMethodDropOffDescription, getShippingAmt } from '@/controllers/ReturnMethodsController.js'\n\nexport default {\n  name: 'ShippingCarrierCard',\n  components: {\n    ActionButton,\n    OText\n  },\n  data () {\n    return ({\n      PACKAGE_AND_PRINT: RETURN_METHOD_SCREENS.PACKAGE_AND_PRINT,\n      NO_PACKAGE_AND_PRINT: RETURN_METHOD_SCREENS.NO_PACKAGE_AND_PRINT,\n      SUSTAINABLE: RETURN_METHOD_SCREENS.SUSTAINABLE,\n      PACKAGE: RETURN_METHOD_SCREENS.PACKAGE,\n      USPS_PACKAGE: RETURN_METHOD_SCREENS.USPS_PACKAGE,\n      PRINT: RETURN_METHOD_SCREENS.PRINT,\n      DIALOG_CARRIER_INFORMATION,\n      DIALOG_CARRIER_LOCATIONS\n    })\n  },\n  computed: {\n    isMailBack () {\n      return isMailBackReturn(this.carrier)\n    },\n    isUSPS () {\n      return isUSPSCarrier(this.carrier)\n    }\n  },\n  methods: {\n    ...mapMutations({\n      openModal: 'returns/openModal'\n    }),\n    getSelectedReturnMethodDropOffDescription (returnType) {\n      return getSelectedReturnMethodDropOffDescription(returnType)\n    },\n    getShippingAmtText () {\n      return getShippingAmt(this.carrier)\n    }\n  },\n  mounted () {},\n  props: {\n    isShowCase: {\n      type: Boolean,\n      default: false\n    },\n    carrier: {\n      type: Object,\n      required: true\n    },\n    handleCarrierSelection: {\n      type: Function,\n      required: true\n    }\n  }\n}\n</script>\n"]}]}