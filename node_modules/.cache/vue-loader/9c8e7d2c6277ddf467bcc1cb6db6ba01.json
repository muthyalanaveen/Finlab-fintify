{"remainingRequest":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/organism/SearchReturnLocation.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/organism/SearchReturnLocation.vue","mtime":1651264216778},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/cache-loader/dist/cjs.js","mtime":1649977086874},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/babel-loader/lib/index.js","mtime":1649977089120},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/cache-loader/dist/cjs.js","mtime":1649977086874},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js","mtime":1649977089802}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["SearchReturnLocation.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuCA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"SearchReturnLocation.vue","sourceRoot":"src/components/organism","sourcesContent":["<template>\n  <div>\n    <back-button\n      override-go-back\n      v-show=\"!disableModalGoBack && !(source === 'mapView')\"\n      :override-on-click=\"() => openModal({ type: DIALOG_CARRIER_LOCATIONS })\"\n    />\n    <div style=\"max-width: 664px\" class=\"mx-auto\">\n      <div>\n        <header-title>{{ SEARCH_ELSEWHERE }}</header-title>\n        <o-text text-secondary bold>\n          Enter address\n        </o-text>\n        <div class=\"mt-4\">\n          <o-places-auto-complete\n            :id=\"id\"\n            placeholder=\"Start typing\"\n            @placechanged=\"getAddressData\"\n            enable-geolocation\n            @change=\"change\"\n          />\n        </div>\n        <action-button\n          block\n          x-large\n          primary-color\n          :disabled=\"disabled\"\n          :loading=\"isLoading\"\n\n          @click=\"onClick\"\n        >\n          Select\n        </action-button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport OText from '@/components/atoms/OText.vue'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\nimport { RETURN_METHOD_DIALOGS } from 'branding/strings.json'\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport BackButton from '@/components/atoms/BackButton.vue'\nimport { mapMutations, mapGetters } from 'vuex'\nimport { DIALOG_CARRIER_LOCATIONS } from '@/store/constants'\nimport OPlacesAutoComplete from '@/components/atoms/OPlacesAutoComplete.vue'\nimport { formatReturnMethods } from '@/utils'\nexport default {\n  components: {\n    OText,\n    HeaderTitle,\n    ActionButton,\n    BackButton,\n    OPlacesAutoComplete\n  },\n  computed: {\n    ...mapGetters({\n      policyReturnMethods: 'returns/policyReturnMethods',\n      policies: 'returns/policies',\n      disableModalGoBack: 'returns/getDisableModalGoBack'\n    })\n  },\n  watch: {\n    policies: function () {\n      this.openReturnLocations()\n    }\n  },\n  methods: {\n    ...mapMutations({\n      openModal: 'returns/openModal',\n      setAddress: 'returns/setAddress'\n    }),\n    getAddressData (data) {\n      this.address = data\n    },\n    change () {\n      this.disabled = !document.getElementById(this.id).value\n    },\n    onClick () {\n      const newAddress = {\n        city: this.address.locality,\n        state: this.address.administrative_area_level_1,\n        zip_code: this.address.postal_code,\n        country_code: this.address.country,\n        longitude: this.address.longitude,\n        latitude: this.address.latitude,\n        street1: this.address.route\n      }\n      this.setAddress(newAddress)\n      this.isLoading = true\n      this.$store\n        .dispatch('returns/fetchAddressInfo', {\n          shippingAddress: newAddress,\n          returnMethods: formatReturnMethods(this.policyReturnMethods)\n        })\n      if (this.selectClicked !== null) {\n        setTimeout(() => { this.isLoading = false; this.selectClicked() }, 3000)\n      } else {\n        setTimeout(() => { if (!this.isReturnLocationsLoaded) { this.openReturnLocations() } }, 5000)\n      }\n    },\n    openReturnLocations () {\n      this.isReturnLocationsLoaded = true\n      this.isLoading = false\n      this.openModal({ type: DIALOG_CARRIER_LOCATIONS })\n    }\n  },\n  props: {\n    source: {\n      type: String,\n      required: true\n    },\n    selectClicked: { // what function to run after changing the address\n      required: false,\n      type: Function,\n      default: null\n    }\n  },\n  data () {\n    return {\n      SEARCH_ELSEWHERE: RETURN_METHOD_DIALOGS.SEARCH_ELSEWHERE,\n      DIALOG_CARRIER_LOCATIONS,\n      address: null,\n      id: `search-return-location_${this.source}`,\n      disabled: true,\n      isReturnLocationsLoaded: false,\n      isLoading: false\n    }\n  }\n}\n</script>\n\n<style></style>\n"]}]}