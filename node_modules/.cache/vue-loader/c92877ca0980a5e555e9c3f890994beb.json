{"remainingRequest":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/pages/TrackerView.vue?vue&type=style&index=0&lang=scss&","dependencies":[{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/pages/TrackerView.vue","mtime":1652147736905},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/css-loader/index.js","mtime":1649977086861},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":1649977090577},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/sass-loader/dist/cjs.js","mtime":1649977086757},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/cache-loader/dist/cjs.js","mtime":1649977086874},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js","mtime":1649977089802}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["TrackerView.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqkBA;AACA;AACA;AACA;AACA","file":"TrackerView.vue","sourceRoot":"src/components/pages","sourcesContent":["<template>\n  <div>\n    <div>\n      <div\n        :class=\"{'ml-6 mt-2': $vuetify.breakpoint.smAndDown, 'ml-8 mt-8': $vuetify.breakpoint.mdAndUp}\"\n        v-if=\"!this.isModal\"\n      >\n        <back-button />\n      </div>\n      <div\n        :class=\"{'mt-n12': $vuetify.breakpoint.mdAndUp}\"\n      >\n        <div\n          style=\"max-width: 664px\"\n          :class=\"{'mx-6': $vuetify.breakpoint.smAndDown && !this.isModal, 'mx-auto': $vuetify.breakpoint.mdAndUp}\"\n        >\n          <v-row\n            class=\"mx-auto mb-4\"\n            :class=\"{'mx-3': this.isModal,'justify-content-between': $vuetify.breakpoint.smAndDown}\"\n            align=\"center\"\n          >\n            <div\n              class=\"ml-n3\"\n            >\n              <header-title>\n                {{ getPageTitle() }}\n              </header-title>\n              <action-button\n                tertiary-color\n                text\n                @click.stop=\"showReturnViewPage\"\n                class=\"ml-n3\"\n                v-if=\"showTracker\"\n              >\n                {{ VIEW_RETURN_DETAILS_BUTTON }}\n              </action-button>\n              <o-dialog\n                :visible=\"showReturnDetails\"\n                is-full-screen\n                @close=\"showReturnDetails = false\"\n              >\n                <ReviewView\n                  is-modal\n                  :show-confirm-action-view=\"false\"\n                  :title=\"getReturnOrderNumber()\"\n                  show-order-details\n                  :return-date=\"getReturnDate()\"\n                  :is-gift=\"this.getSelectedReturnItem.is_gift\"\n                />\n              </o-dialog>\n            </div>\n          </v-row>\n          <div v-if=\"showTracker\">\n            <div\n              :class=\"{'mx-n12': $vuetify.breakpoint.smAndDown}\"\n            >\n              <v-stepper alt-labels elevation=\"0\">\n                <v-stepper-header>\n                  <v-stepper-step\n                    step=\"\"\n                    :complete=\"stepComplete('STARTED')\"\n                    :color=\"stepStatus('STARTED')\"\n                  >\n                    {{ getStepperlabel('STARTED') }}\n                  </v-stepper-step>\n\n                  <v-divider v-if=\"this.showInTransit()\" />\n                  <v-stepper-step\n                    step=\"\"\n                    :complete=\"stepComplete('IN_TRANSIT')\"\n                    :color=\"stepStatus('IN_TRANSIT')\"\n                    v-if=\"this.showInTransit()\"\n                  >\n                    {{ getStepperlabel('IN_TRANSIT') }}\n                  </v-stepper-step>\n\n                  <v-divider v-if=\"this.getSelectedReturnItem.return_method && this.getSelectedReturnItem.return_method.type !== IN_STORE\" />\n                  <v-stepper-step\n                    step=\"\"\n                    :complete=\"stepComplete('RECEIVED')\"\n                    :color=\"stepStatus('RECEIVED')\"\n                    v-if=\"this.getSelectedReturnItem.return_method && this.getSelectedReturnItem.return_method.type !== IN_STORE\"\n                  >\n                    {{ getStepperlabel('RECEIVED') }}\n                  </v-stepper-step>\n\n                  <v-divider />\n                  <v-stepper-step\n                    :complete=\"stepComplete('COMPLETED')\"\n                    :color=\"stepStatus('COMPLETED')\"\n                    step=\"\"\n                  >\n                    {{ getStepperlabel('COMPLETED') }}\n                  </v-stepper-step>\n                </v-stepper-header>\n              </v-stepper>\n            </div>\n            <o-text\n              lg\n              bold\n              class=\"mt-4\"\n            >\n              {{ getReturnMethodTitle() }}\n            </o-text>\n            <o-text\n              m\n              class=\"mt-3\"\n            >\n              {{ getReturnMethodSubTitle() }}\n            </o-text>\n            <div class=\"d-flex mt-6 justify-start justify-space-between\">\n              <action-button\n                primary-color\n                v-if=\"shouldShowViewQRCodeButton()\"\n                @click.stop=\"showReturnCodes=true\"\n              >\n                {{ VIEW_QR_CODE }}\n              </action-button>\n              <o-dialog\n                :visible=\"showReturnCodes\"\n                is-full-screen\n                @close=\"showReturnCodes = false\"\n              >\n                <QRCodeView\n                  v-if=\"this.selectedReturnMethod && this.selectedReturnMethod.type !== MAIL_BACK\"\n                  :qr-data=\"this.getSelectedReturnItem.express_details\"\n                  :from-return-methods=\"false\"\n                />\n              </o-dialog>\n              <action-button\n                @click.stop=\"openReturnLabelLink\"\n                primary-color\n                v-if=\"shouldShowDownloadShippingLabelButton()\"\n              >\n                {{ DOWNLOAD_SHIPPING_LABEL }}\n              </action-button>\n              <action-button\n                tertiary-color\n                text\n                :block=\"$vuetify.breakpoint.smAndDown && this.selectedReturnMethod.type === IN_STORE\"\n                @click=\"openModal({type: DIALOG_CARRIER_LOCATIONS, selectedCarrier: carrier})\"\n                v-if=\"shouldShowLocationsButton()\"\n              >\n                {{ SEE_LOCATIONS }}\n              </action-button>\n              <o-dialog\n                :visible=\"isModalOpen\"\n                is-full-screen\n                @close=\"closeModal\"\n              >\n                <return-locations\n                  v-if=\"activeModal === DIALOG_CARRIER_LOCATIONS\"\n                  :show-select-locations=\"false\"\n                  :title=\"EXPLORE_LOCATIONS\"\n                  :return-type=\"getReturnMethodType()\"\n                  :from-return-methods=\"false\"\n                />\n                <return-store-info v-if=\"activeModal === DIALOG_CARRIER_INFORMATION\" />\n                <search-return-location v-if=\"activeModal === DIALOG_SEARCH_LOCATION\" source=\"trackerView\" />\n              </o-dialog>\n              <action-button\n                tertiary-color\n                text\n                :block=\"$vuetify.breakpoint.smAndDown\"\n                v-if=\"shouldShowTrackShippmentButton()\"\n                @click.stop=\"openTrackShipmentLabelLink\"\n              >\n                Track Shipment\n              </action-button>\n            </div>\n          </div>\n          <div\n            v-else\n          >\n            <div v-if=\"this.currentStep === 'CANCELED'\">\n              <o-text\n                lg\n                bold\n                class=\"mt-4\"\n              >\n                {{ this.CANCELED_INSTRUCTIONS_TITLE }}\n              </o-text>\n              <o-text\n                m\n                class=\"mt-3\"\n              >\n                {{ this.canceledInstructions }}\n              </o-text>\n            </div>\n            <div v-else>\n              <o-text\n                lg\n                bold\n                class=\"mt-4\"\n              >\n                {{ this.NOT_FOUND_INSTRUCTIONS_TITLE }}\n              </o-text>\n              <o-text\n                m\n                class=\"mt-3\"\n              >\n                {{ this.notFoundInstructions }}\n              </o-text>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport dayjs from 'dayjs'\nimport OText from '@/components/atoms/OText.vue'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\nimport { TRACKING_VIEW, CONFIRMATION_VIEW, RETURN_METHOD_DIALOGS } from 'branding/strings.json'\nimport BackButton from '@/components/atoms/BackButton.vue'\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport { mapGetters, mapMutations } from 'vuex'\nimport ReturnLocations from '@/components/organism/ReturnLocations.vue'\nimport { getFormattedDate } from '@/utils'\nimport ODialog from '@/components/atoms/ODialog.vue'\nimport ReviewView from '@/components/pages/ReviewView.vue'\nimport QRCodeView from '@/components/pages/QRCodeView.vue'\nimport { CONSTANTS } from '@/constants/index.js'\nimport { DIALOG_CARRIER_INFORMATION, DIALOG_CARRIER_LOCATIONS, DIALOG_SEARCH_LOCATION } from '@/store/constants'\nimport ReturnStoreInfo from '@/components/organism/ReturnStoreInfo.vue'\nimport SearchReturnLocation from '@/components/organism/SearchReturnLocation.vue'\nimport { FETCH_ORDER_BY_HASH, UPDATE_REFUND_LIST_FOR_RETURN_USER, UPDATE_EXCHANGE_LIST_FOR_RETURN_USER } from '@/store/constants.js'\n\nexport default {\n  name: 'TrackerView',\n\n  components: {\n    OText,\n    HeaderTitle,\n    BackButton,\n    ActionButton,\n    ReturnLocations,\n    ODialog,\n    ReviewView,\n    QRCodeView,\n    ReturnStoreInfo,\n    SearchReturnLocation\n  },\n\n  props: {\n    isModal: {\n      type: Boolean,\n      required: false\n    },\n    showConfirmActionView: {\n      type: Boolean,\n      required: false,\n      default: true\n    },\n    title: {\n      type: String,\n      required: false,\n      default: 'Review your return'\n    },\n    showOrderDetails: {\n      type: Boolean,\n      required: false,\n      default: false\n    },\n    returnDate: {\n      type: String,\n      required: false,\n      default: ''\n    }\n  },\n  computed: {\n    ...mapGetters(\n      {\n        order: 'order/order',\n        refundList: 'order/refundList',\n        policies: 'returns/policies',\n        rmaList: 'order/rmaList',\n        selectedReturnMethod: 'returns/getSelectedReturnMethod',\n        getSelectedReturnItem: 'returns/getSelectedReturnItem',\n        isModalOpen: 'returns/getIsModalOpen',\n        activeModal: 'returns/getActiveModal',\n        carrier: 'returns/getSelectedCarrier'\n      }),\n    isExchangeOrRefund () {\n      const isExchange = this.getSelectedReturnItem.rma_items.findIndex(item => item.is_exchange === true) !== -1\n      const isRefund = this.getSelectedReturnItem.rma_items.findIndex(item => item.is_exchange === false) !== -1\n      if (isExchange && isRefund) {\n        return CONSTANTS.EXCHANGE_AND_REFUND\n      } else if (isExchange && !isRefund) {\n        return CONSTANTS.EXCHANGE\n      }\n      return CONSTANTS.REFUND\n    }\n  },\n  data () {\n    return {\n      DIALOG_CARRIER_INFORMATION,\n      DIALOG_CARRIER_LOCATIONS,\n      DIALOG_SEARCH_LOCATION,\n      MAIL_BACK: CONSTANTS.RETURN_METHODS.MAIL_BACK,\n      IN_STORE: CONSTANTS.RETURN_METHODS.IN_STORE,\n      EXPRESS_RETURNS: CONSTANTS.RETURN_METHODS.EXPRESS_RETURNS,\n      TRACKING_VIEW_TITLE: TRACKING_VIEW.TRACKING_VIEW_TITLE,\n      STARTED: TRACKING_VIEW.STARTED,\n      IN_TRANSIT: TRACKING_VIEW.IN_TRANSIT,\n      RECEIVED: TRACKING_VIEW.RECEIVED,\n      COMPLETED: TRACKING_VIEW.COMPLETED,\n      SEE_LOCATIONS: CONFIRMATION_VIEW.SEE_LOCATIONS,\n      VIEW_RETURN_DETAILS_BUTTON: CONFIRMATION_VIEW.VIEW_RETURN_DETAILS_BUTTON,\n      VIEW_RETURN_CODES_BUTTON: CONFIRMATION_VIEW.VIEW_RETURN_CODES_BUTTON,\n      DOWNLOAD_SHIPPING_LABEL: CONFIRMATION_VIEW.DOWNLOAD_SHIPPING_LABEL,\n      VIEW_QR_CODE: CONFIRMATION_VIEW.VIEW_QR_CODE,\n      PAGE_TITLE_STEP1: TRACKING_VIEW.PAGE_TITLE_STEP1,\n      PAGE_TITLE_STEP2: TRACKING_VIEW.PAGE_TITLE_STEP2,\n      PAGE_TITLE_STEP3: TRACKING_VIEW.PAGE_TITLE_STEP3,\n      PAGE_TITLE_STEP4: TRACKING_VIEW.PAGE_TITLE_STEP4,\n      MAIL_BACK_TITLE_STEP1: TRACKING_VIEW.MAIL_BACK_TITLE_STEP1,\n      MAIL_BACK_TITLE_STEP2: TRACKING_VIEW.MAIL_BACK_TITLE_STEP2,\n      MAIL_BACK_TITLE_STEP3: TRACKING_VIEW.MAIL_BACK_TITLE_STEP3,\n      MAIL_BACK_SUB_TITLE_STEP1: TRACKING_VIEW.MAIL_BACK_SUB_TITLE_STEP1,\n      MAIL_BACK_SUB_TITLE_STEP2: TRACKING_VIEW.MAIL_BACK_SUB_TITLE_STEP2,\n      MAIL_BACK_SUB_TITLE_STEP3: TRACKING_VIEW.MAIL_BACK_SUB_TITLE_STEP3,\n      DROP_OFF_SUB_TITLE_STEP1: TRACKING_VIEW.DROP_OFF_SUB_TITLE_STEP1,\n      TRACKING_COMPLETE_STEP_TITLE: TRACKING_VIEW.TRACKING_COMPLETE_STEP_TITLE,\n      TRACKING_COMPLETE_STEP_SUB_TITLE: TRACKING_VIEW.TRACKING_COMPLETE_STEP_SUB_TITLE,\n      EXPRESS_RETURN_SUB_TITLE_STEP1: TRACKING_VIEW.EXPRESS_RETURN_SUB_TITLE_STEP1,\n      CANCELED_INSTRUCTIONS_TITLE: TRACKING_VIEW.CANCELED_INSTRUCTIONS_TITLE,\n      CANCELED_INSTRUCTIONS_DESCRIPTION: TRACKING_VIEW.CANCELED_INSTRUCTIONS_DESCRIPTION,\n      NOT_FOUND_INSTRUCTIONS_TITLE: TRACKING_VIEW.NOT_FOUND_INSTRUCTIONS_TITLE,\n      NOT_FOUND_INSTRUCTIONS_DESCRIPTION: TRACKING_VIEW.NOT_FOUND_INSTRUCTIONS_DESCRIPTION,\n      SEE_RETURN_LOCATIONS: RETURN_METHOD_DIALOGS.SEE_RETURN_LOCATIONS,\n      el: 1,\n      showReturnDetails: false,\n      currentStep: 'Started',\n      showReturnCodes: false,\n      EXPLORE_LOCATIONS: RETURN_METHOD_DIALOGS.EXPLORE_LOCATIONS,\n      showTracker: false,\n      canceledInstructions: '',\n      notFoundInstructions: ''\n    }\n  },\n\n  mounted () {\n    if (this.order.order_hash === '') {\n      this.getData()\n    } else {\n      this.configureTracker(this.getSelectedReturnItem.return_status)\n    }\n  },\n\n  methods: {\n    ...mapMutations({\n      openModal: 'returns/openModal',\n      closeModal: 'returns/closeModal',\n      setSelectedReturnMethod: 'returns/setSelectedReturnMethod'\n    }),\n    async getData () {\n      const hash = this.$route.params.id\n      const identifierFromUrl = this.$route.params.identifier\n      await this.$store\n        .dispatch('order/' + FETCH_ORDER_BY_HASH, hash)\n        .then((response) => {\n          if (response) {\n            const selectedRMA = this.rmaList?.rmaListItems?.find(rma => rma.identifier === identifierFromUrl)\n            if (selectedRMA) {\n              this.$store.commit('returns/updateSelectedReturnItem', selectedRMA)\n              const refundList = []\n              const exchangeList = []\n\n              selectedRMA.rma_items.map(item => {\n                if (!item?.is_exchange) {\n                  refundList.push(item.order_item)\n                } else {\n                  exchangeList.push(item.exchange_item)\n                }\n              })\n              this.$store.commit('order/' + UPDATE_REFUND_LIST_FOR_RETURN_USER, refundList)\n              this.$store.commit('order/' + UPDATE_EXCHANGE_LIST_FOR_RETURN_USER, exchangeList)\n\n              this.setSelectedReturnMethod(selectedRMA.return_method)\n              this.configureTracker(this.getSelectedReturnItem.return_status)\n            } else {\n              this.notFoundInstructions = this.NOT_FOUND_INSTRUCTIONS_DESCRIPTION.replace('$$email', this.policies.retailer_info.customer_support_email).replace('$$phone', this.policies.retailer_info.customer_support_contact)\n            }\n          }\n        })\n        .catch((err) => {\n          this.$router.push('/orders/' + hash)\n          console.error({ err })\n        })\n    },\n    configureTracker (currentStatus) {\n      this.updateDescription()\n      this.currentStep = currentStatus\n      this.showTracker = this.currentStep !== 'CANCELED'\n    },\n    getReturnOrderNumber () {\n      return 'Order #' + (this.order ? this.order.order_id : '') // + '|'\n    },\n\n    stepComplete (step) {\n      var isComplete = false\n\n      if (this.currentStep === 'STARTED') {\n        if (step === 'STARTED') {\n          isComplete = true\n        }\n      } else if (this.currentStep === 'IN_TRANSIT') {\n        if (step === 'STARTED' || step === 'IN_TRANSIT') {\n          isComplete = true\n        }\n      } else if (this.currentStep === 'RECEIVED') {\n        if (step === 'STARTED' || step === 'IN_TRANSIT' || step === 'RECEIVED') {\n          isComplete = true\n        }\n      } else if (this.currentStep === 'COMPLETED') {\n        if (step === 'STARTED' || step === 'IN_TRANSIT' || step === 'RECEIVED' || step === 'COMPLETED') {\n          isComplete = true\n        }\n      }\n      return isComplete\n    },\n\n    stepStatus (step) {\n      return this.stepComplete(step) ? '#333333' : '#dddddd'\n    },\n\n    getStepperlabel (step) {\n      var title = ''\n      if (step === 'STARTED') {\n        title = this.STARTED\n      } else if (step === 'IN_TRANSIT') {\n        title = this.IN_TRANSIT\n      } else if (step === 'RECEIVED') {\n        title = this.RECEIVED\n      } else if (step === 'COMPLETED') {\n        title = this.COMPLETED\n      }\n      return title\n    },\n\n    getPageTitle: function () {\n      var title = this.PAGE_TITLE_STEP1\n      if (this.currentStep === 'STARTED') {\n        title = (this.isExchangeOrRefund !== CONSTANTS.EXCHANGE) ? this.PAGE_TITLE_STEP1 : TRACKING_VIEW.PAGE_TITLE_STEP1_EXCHANGE\n      } else if (this.currentStep === 'IN_TRANSIT') {\n        title = this.PAGE_TITLE_STEP2\n      } else if (this.currentStep === 'RECEIVED') {\n        title = this.PAGE_TITLE_STEP3\n      } else if (this.currentStep === 'COMPLETED') {\n        title = this.PAGE_TITLE_STEP4\n      }\n      return title\n    },\n\n    getReturnMethodTitle: function () {\n      var title = ''\n      const returnMethod = this.getSelectedReturnItem.return_method\n      if (this.currentStep === 'STARTED') {\n        if (returnMethod && (returnMethod.type === this.IN_STORE || returnMethod.type === this.EXPRESS_RETURNS)) {\n          title = TRACKING_VIEW.DROP_OFF_RETURN_METHOD_TITLE\n        } else if (returnMethod && returnMethod.type === this.MAIL_BACK) {\n          title = TRACKING_VIEW.MAIL_BACK_RETURN_METHOD_TITLE\n        }\n        if (returnMethod) {\n          title = title.replace('$$returnMethodName', returnMethod.customer_facing_carrier).replace('$$returnByDate', dayjs(this.getSelectedReturnItem.created_at).add('1', 'month').format('MMM DD'))\n        }\n      } else if (this.currentStep === 'IN_TRANSIT') {\n        title = this.MAIL_BACK_TITLE_STEP2\n      } else if (this.currentStep === 'RECEIVED') {\n        title = this.MAIL_BACK_TITLE_STEP3\n      } else if (this.currentStep === 'COMPLETED') {\n        title = this.TRACKING_COMPLETE_STEP_TITLE\n      }\n      return title\n    },\n\n    showInTransit () {\n      const isMailBack = this.getSelectedReturnItem?.return_method?.type === this.MAIL_BACK\n      const isExpressReturns = this.getSelectedReturnItem?.return_method?.type === this.EXPRESS_RETURNS\n      var showTransit = isMailBack || isExpressReturns\n      if (this.policies?.refund_trigger_status.length > 0 && ((isMailBack && this.policies?.refund_trigger_status?.includes('PRE_TRANSIT')) || (isExpressReturns && this.policies?.refund_trigger_status?.includes('ACCEPTED')))) {\n        showTransit = false\n      }\n      return showTransit\n    },\n\n    getReturnMethodSubTitle: function () {\n      var title = ''\n      const returnMethod = this.getSelectedReturnItem.return_method\n      const refundTimingEstimate = this.policies.refund_timing_estimate ? this.policies.refund_timing_estimate : '5-7'\n\n      if (returnMethod && returnMethod.type === this.IN_STORE) {\n        if (this.currentStep === 'STARTED') {\n          title = this.DROP_OFF_SUB_TITLE_STEP1\n        }\n      } else if (returnMethod && returnMethod.type === this.EXPRESS_RETURNS) {\n        if (this.currentStep === 'STARTED') {\n          if (this.isExchangeOrRefund === CONSTANTS.EXCHANGE) {\n            title = TRACKING_VIEW.EXPRESS_RETURN_SUB_TITLE_STEP1_EXCHANGE.replace('$$CARRIER', returnMethod.customer_facing_carrier)\n          } else if (this.isExchangeOrRefund === CONSTANTS.REFUND) {\n            title = TRACKING_VIEW.EXPRESS_RETURN_SUB_TITLE_STEP1.replace('$$refundTimingEstimate', refundTimingEstimate).replace('$$CARRIER', returnMethod?.customer_facing_carrier)\n          } else {\n            title = TRACKING_VIEW.EXPRESS_RETURN_SUB_TITLE_STEP1_EXCHANGE_REFUND.replace('$$CARRIER', returnMethod.customer_facing_carrier)\n          }\n        }\n      } else if (returnMethod && returnMethod.type === this.MAIL_BACK) {\n        if (this.currentStep === 'STARTED') {\n          if (this.isExchangeOrRefund === CONSTANTS.EXCHANGE) {\n            title = TRACKING_VIEW.MAIL_BACK_SUB_TITLE_STEP1_EXCHANGE.replace('$$mailBack', returnMethod.customer_facing_carrier)\n          } else if (this.isExchangeOrRefund === CONSTANTS.REFUND) {\n            title = TRACKING_VIEW.MAIL_BACK_SUB_TITLE_STEP1.replace('$$refundTimingEstimate', refundTimingEstimate).replace('$$mailBack', returnMethod?.customer_facing_carrier)\n          } else {\n            title = TRACKING_VIEW.MAIL_BACK_SUB_TITLE_STEP1_EXCHANGE_REFUND.replace('$$refundTimingEstimate', refundTimingEstimate).replace('$$mailBack', returnMethod?.customer_facing_carrier)\n          }\n        }\n      }\n\n      if (this.currentStep === 'IN_TRANSIT') {\n        title = this.MAIL_BACK_SUB_TITLE_STEP2\n      } else if (this.currentStep === 'RECEIVED') {\n        title = this.MAIL_BACK_SUB_TITLE_STEP3\n      } else if (this.currentStep === 'COMPLETED') {\n        title = this.TRACKING_COMPLETE_STEP_SUB_TITLE\n      }\n      return title\n    },\n    getReturnDate () {\n      return 'Date ' + getFormattedDate(this.getSelectedReturnItem.created_at)\n    },\n    shouldShowLocationsButton () {\n      return this.currentStep === 'STARTED'\n    },\n    shouldShowViewQRCodeButton () {\n      const returnMethod = this.getSelectedReturnItem.return_method\n      return returnMethod && returnMethod.type === this.EXPRESS_RETURNS && this.currentStep === 'STARTED'\n    },\n    shouldShowDownloadShippingLabelButton () {\n      const returnMethod = this.getSelectedReturnItem.return_method\n      return returnMethod && returnMethod.type === this.MAIL_BACK && this.currentStep === 'STARTED'\n    },\n    shouldShowTrackShippmentButton () {\n      const returnMethod = this.getSelectedReturnItem.return_method\n      return returnMethod && returnMethod.type === this.MAIL_BACK && this.currentStep === 'IN_TRANSIT'\n    },\n    openReturnLabelLink () {\n      const labelUrl = this.getSelectedReturnItem?.shipping_label_details?.label_url\n      if (labelUrl) {\n        window.open(labelUrl)\n      }\n    },\n    openTrackShipmentLabelLink () {\n      const labelUrl = this.getSelectedReturnItem?.shipping_label_details?.tracking_url\n      if (labelUrl) {\n        window.open(labelUrl)\n      }\n    },\n    showReturnViewPage () {\n      this.showReturnDetails = true\n    },\n    getReturnMethodType: function () {\n      return this.getSelectedReturnItem?.return_method?.type\n    },\n    updateDescription () {\n      const refundTimingEstimate = this.policies.refund_timing_estimate ? this.policies.refund_timing_estimate : '5-7'\n      this.MAIL_BACK_SUB_TITLE_STEP1 = TRACKING_VIEW.MAIL_BACK_SUB_TITLE_STEP1.replace('$$refundTimingEstimate', refundTimingEstimate).replace('$$mailBack', this.selectedReturnMethod?.customer_facing_carrier)\n      this.MAIL_BACK_SUB_TITLE_STEP2 = TRACKING_VIEW.MAIL_BACK_SUB_TITLE_STEP2.replace('$$refundTimingEstimate', refundTimingEstimate)\n      this.TRACKING_COMPLETE_STEP_SUB_TITLE = TRACKING_VIEW.TRACKING_COMPLETE_STEP_SUB_TITLE.replace('$$refundTimingEstimate', refundTimingEstimate)\n      this.DROP_OFF_SUB_TITLE_STEP1 = TRACKING_VIEW.DROP_OFF_SUB_TITLE_STEP1.replace('$$refundTimingEstimate', refundTimingEstimate)\n      this.EXPRESS_RETURN_SUB_TITLE_STEP1 = TRACKING_VIEW.EXPRESS_RETURN_SUB_TITLE_STEP1.replace('$$refundTimingEstimate', refundTimingEstimate)\n      this.canceledInstructions = this.CANCELED_INSTRUCTIONS_DESCRIPTION.replace('$$email', this.policies.retailer_info.customer_support_email).replace('$$phone', this.policies.retailer_info.customer_support_contact)\n    }\n  }\n\n}\n</script>\n\n<style lang=\"scss\">\n\n  @media only screen and (max-width: 959px) {\n    .v-stepper:not(.v-stepper--vertical) .v-stepper__label {\n      display: flex !important;\n    }\n  }\n\n</style>\n"]}]}