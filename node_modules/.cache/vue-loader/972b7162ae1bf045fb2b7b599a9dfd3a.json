=\"pr-2 pl-2\"\n        @click=\"tabClicked\"\n      >\n        <v-col class=\"d-flex justify-center align-center\">\n          <o-text\n            class=\"pb-3\"\n            v-html=\"tab.name\"\n          />\n          <div\n            v-if=\"tab.index !== active_tab\"\n            class=\"v-tabs-slider-wrapper\"\n            style=\"height: 1px; width: 100%; color: #D9D9D9\"\n          >\n            <div class=\"v-tabs-slider\" />\n          </div>\n        </v-col>\n      </v-tab>\n    </v-tabs>\n    <v-tabs-items v-model=\"active_tab\" touchless>\n      <v-tab-item v-for=\"tab of tabs\" :key=\"tab.index\">\n        <component :is=\"tab.component\" :class=\"tab.class\" />\n      </v-tab-item>\n    </v-tabs-items>\n  </div>\n</template>\n\n<script>\nimport OText from '@/components/atoms/OText.vue'\n\nexport default {\n  components: {\n    OText\n  },\n  props: {\n    tabs: {\n      required: true,\n      type: Array\n    },\n    onClickMethod: {\n      type: Function,\n      required: false,\n      default: function () {}\n    }\n  },\n  data () {\n    return {\n      active_tab: 0\n    }\n  },\n  methods: {\n    tabClicked () {\n      this.onClickMethod(this.active_tab)\n    }\n  }\n}\n</script>\n\n<style>\n    #o-tab .v-slide-group__prev {\n        display: none !important;\n    }\n    #o-tab .v-tabs-slider-wrapper, #o-tab .v-tabs-slider {\n      overflow: hidden;\n    }\n</style>\n"]}]}