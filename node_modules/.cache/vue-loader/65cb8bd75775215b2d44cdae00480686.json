{"remainingRequest":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/pages/ReturnMethods.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/pages/ReturnMethods.vue","mtime":1663867175559},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/cache-loader/dist/cjs.js","mtime":1649977086874},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/babel-loader/lib/index.js","mtime":1649977089120},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/cache-loader/dist/cjs.js","mtime":1649977086874},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js","mtime":1649977089802}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["ReturnMethods.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"ReturnMethods.vue","sourceRoot":"src/components/pages","sourcesContent":["<template>\n  <div>\n    <div\n      :class=\"{\n        'ml-3 mt-2': $vuetify.breakpoint.smAndDown,\n        'ml-8 mt-8': $vuetify.breakpoint.mdAndUp,\n      }\"\n      v-if=\"!hasMap\"\n    >\n      <back-button />\n    </div>\n    <not-found-return-method\n      v-if=\"!this.returnMethods || this.returnMethods === 0\"\n      class=\"text-center mt-16\"\n      :customer-support-contact=\"customerSupportContact\"\n      :title=\"noReturnMethods\"\n    />\n    <div\n      v-else\n      :class=\"{\n        'mt-2': $vuetify.breakpoint.smAndDown,\n        'mt-n12': $vuetify.breakpoint.mdAndUp && !hasMap,\n      }\"\n    >\n      <div\n        :style=\"!hasMap ? 'max-width: 810px; margin: auto;' : 'margin: auto;'\"\n        :class=\"{\n          'mx-auto': $vuetify.breakpoint.mdAndUp,\n          'm-2': $vuetify.breakpoint.xs && hasMap\n        }\"\n      >\n        <v-row\n          v-if=\"!hasMap\"\n          :class=\"{\n            'ml-4': $vuetify.breakpoint.smAndDown,\n            'mx-auto': $vuetify.breakpoint.mdAndUp,\n          }\"\n          align=\"center\"\n        >\n          <header-title>\n            {{ CHOOSE_METHOD }}\n          </header-title>\n        </v-row>\n        <v-row\n          justify=\"center\"\n          class=\"mb-4 mt-4 mx-auto\"\n        >\n          <shipping-carrier-card\n            :class=\"hasMap ? 'mt-3' : ''\"\n            :is-show-case=\"true\"\n            :carrier=\"showCaseCarrier\"\n            :handle-carrier-selection=\"handleCarrierSelection\"\n            class=\"normalWt regularButtonWt\"\n          />\n        </v-row>\n        <o-text\n          bold\n          heading\n          :class=\"{\n            'ml-4': $vuetify.breakpoint.smAndDown,\n            'mx-auto': $vuetify.breakpoint.mdAndUp,\n          }\"\n          class=\"normalWt\"\n          v-if=\"accordionOptions.length > 0\"\n        >\n          Other options\n        </o-text>\n        <v-row justify=\"center\" class=\"mb-4 mt-4\">\n          <accordion\n            :options=\"accordionOptions\"\n            :handle-selection=\"handleAccordionSelection\"\n            :opened-panel=\"computeOpenedPanel\"\n          >\n            <shipping-carrier-card\n              :carrier=\"openAccordionCarrier\"\n              :handle-carrier-selection=\"handleCarrierSelection\"\n            />\n          </accordion>\n        </v-row>\n      </div>\n      <o-dialog\n        :visible=\"isModalOpen\"\n        is-full-screen\n        @close=\"closeModal\"\n      >\n        <return-locations\n          v-if=\"activeModal === DIALOG_CARRIER_LOCATIONS\"\n          show-search-area\n          :title=\"EXPLORE_LOCATIONS\"\n          :return-type=\"getReturnMethodType()\"\n          :show-select-locations=\"false\"\n          from-return-methods\n        />\n        <return-store-info\n          v-if=\"activeModal === DIALOG_CARRIER_INFORMATION\"\n          :is-rma-submitted=\"false\"\n        />\n        <search-return-location v-if=\"activeModal === DIALOG_SEARCH_LOCATION\" source=\"returnMethods\" />\n      </o-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport OText from '@/components/atoms/OText.vue'\nimport Accordion from '@/components/molecules/Accordion.vue'\nimport ShippingCarrierCard from '@/components/organism/ShippingCarrierCard.vue'\nimport ReturnLocations from '@/components/organism/ReturnLocations.vue'\nimport ODialog from '@/components/atoms/ODialog.vue'\nimport ReturnStoreInfo from '@/components/organism/ReturnStoreInfo.vue'\nimport BackButton from '@/components/atoms/BackButton.vue'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\nimport {\n  RETURN_METHOD_SCREENS,\n  RETURN_METHOD_DIALOGS,\n  PAGE_TITLES\n} from 'branding/strings.json'\nimport { mapGetters, mapMutations } from 'vuex'\nimport SearchReturnLocation from '@/components/organism/SearchReturnLocation.vue'\nimport NotFoundReturnMethod from '@/components/molecules/NotFoundReturnMethod.vue'\nimport {\n  DIALOG_SEARCH_LOCATION,\n  DIALOG_CARRIER_INFORMATION,\n  DIALOG_CARRIER_LOCATIONS\n} from '@/store/constants'\nimport { returnMethodsWithinDistance, setSelectedReturnMethod } from '@/controllers/MapController.js'\nimport { getReturnsHelperText, getShippingAmt } from '@/controllers/ReturnMethodsController.js'\n\nexport default {\n  components: {\n    ShippingCarrierCard,\n    Accordion,\n    OText,\n    ReturnLocations,\n    ODialog,\n    ReturnStoreInfo,\n    BackButton,\n    HeaderTitle,\n    SearchReturnLocation,\n    NotFoundReturnMethod\n  },\n  computed: {\n    ...mapGetters({\n      order: 'order/order',\n      isModalOpen: 'returns/getIsModalOpen',\n      activeModal: 'returns/getActiveModal',\n      returnMethods: 'returns/returnMethods',\n      selectedCarrier: 'returns/getSelectedCarrier',\n      policies: 'returns/policies',\n      policyReturnMethods: 'returns/policyReturnMethods'\n    }),\n    noReturnMethods () {\n      const distance = this.showCaseCarrier?.distance_threshold\n      return RETURN_METHOD_SCREENS.NO_RETURN_METHODS.replace('$$distanceRadius', distance)\n    },\n    showCaseCarrier () {\n      let returnMethod = null\n      const returnMethods = returnMethodsWithinDistance(this.hasMap)\n      if (returnMethods && returnMethods.length) {\n        returnMethod = returnMethods[0]\n      }\n      return returnMethod\n    },\n    accordionOptions () {\n      var accordionReturnMethods = []\n      const returnMethods = returnMethodsWithinDistance(this.hasMap)\n      if (returnMethods && returnMethods.length) {\n        accordionReturnMethods = returnMethods.slice(1)\n      }\n      this.closeAllPanels()\n      return accordionReturnMethods.map((opt) => ({\n        primaryText: `${getReturnsHelperText(opt.type)} ${opt.customer_facing_carrier}`,\n        secondaryText: `${getShippingAmt(opt)}`,\n        id: opt.id\n      }))\n    },\n    computeOpenedPanel () {\n      return this.openedPanel\n    }\n  },\n  methods: {\n    ...mapMutations({\n      openModal: 'returns/openModal',\n      closeModal: 'returns/closeModal',\n      setSelectedCarrier: 'returns/setSelectedCarrier',\n      setSelectedReturnMethod: 'returns/setSelectedReturnMethod'\n    }),\n    openPanel (index) {\n      this.openedPanel = index\n    },\n    closeAllPanels () {\n      this.openedPanel = null\n    },\n    handleCarrierSelection (carrier) {\n      setSelectedReturnMethod(carrier)\n      this.$router.push('/orders/' + this.order.order_hash + '/review')\n    },\n    handleAccordionSelection ({ id }) {\n      const option = this.returnMethods.find((m) => m.id === id)\n      this.openAccordionCarrier = option\n      this.setSelectedCarrier(option)\n      this.openPanel(option)\n    },\n    getReturnMethodType: function () {\n      return this.selectedCarrier ? this.selectedCarrier.type : ''\n    }\n  },\n  data () {\n    return {\n      openAccordionCarrier: null,\n      openedPanel: null,\n      customerSupportContact: '',\n      CHOOSE_METHOD: RETURN_METHOD_SCREENS.CHOOSE_METHOD,\n      DIALOG_SEARCH_LOCATION,\n      DIALOG_CARRIER_INFORMATION,\n      DIALOG_CARRIER_LOCATIONS,\n      EXPLORE_LOCATIONS: RETURN_METHOD_DIALOGS.EXPLORE_LOCATIONS,\n      hasMap: false\n    }\n  },\n  created () {\n    this.hasMap = this.$route.name === 'mapshipping'\n  },\n  mounted () {\n    document.title = PAGE_TITLES.RETURN_METHOD\n    this.customerSupportContact = this.policies?.retailer_info?.customer_support_contact\n  }\n}\n</script>\n\n<style scoped>\n</style>\n"]}]}