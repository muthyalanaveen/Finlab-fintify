olor: var(--button-primary);\"\n            small\n            aria-hidden=\"true\"\n          >\n            arrow_back\n          </v-icon>\n        </v-btn>\n        <v-spacer v-if=\"$vuetify.breakpoint.smAndDown\" />\n        <div\n          class=\"d-flex align-center ml-8 text-body-2\"\n          :class=\"{'mt-3 mr-4': $vuetify.breakpoint.smAndDown}\"\n          v-if=\"count>1\"\n        >\n          <o-text\n            base-bold\n            class=\"mr-1\"\n          >\n            Item {{ formattedIndex }}\n          </o-text>\n          <o-text>\n            /\n          </o-text>\n          <o-text class=\"ml-1\">\n            {{ formattedCount }}\n          </o-text>\n        </div>\n      </span>\n      <header-title\n        v-if=\"this.showThumbnail\"\n        class=\"d-flex justify-start px-3\"\n        :class=\"{'mt-5 mb-0': $vuetify.breakpoint.smAndDown, 'mt-9 mb-0': $vuetify.breakpoint.mdAndUp}\"\n      >\n        {{ title }}\n      </header-title>\n      <o-text\n        base\n        class=\"d-flex justify-start mt-1 px-3\"\n        v-if=\"subTitle != ''\"\n      >\n        {{ subTitle }}\n      </o-text>\n      <div\n        v-if=\"this.showThumbnail\"\n        class=\"d-flex justify-start mt-0\"\n        :class=\"{'mt-6 mb-3': $vuetify.breakpoint.smAndDown, 'mt-6 mb-8': $vuetify.breakpoint.mdAndUp}\"\n      >\n        <image-thumbnail\n          class=\"normalWt\"\n          :item=\"item\"\n          :title=\"item.title\"\n          :sub-title1=\"item.subtitle\"\n          :sub-title2=\"formattedPrice\"\n          :image-url=\"item.image_url\"\n          :order-brand=\"orderBrand\"\n          product-thumbnail\n          is-small-thumbnail\n        />\n      </div>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport ImageThumbnail from './ImageThumbnail.vue'\nimport { pad } from '@/utils'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\nimport OText from '@/components/atoms/OText.vue'\n\nexport default {\n  name: 'ReturnReasonHeaderView',\n  components: {\n    ImageThumbnail,\n    HeaderTitle,\n    OText\n  },\n  props: {\n    title: {\n      type: String,\n      default: ''\n    },\n    subTitle: {\n      type: String,\n      default: ''\n    },\n    index: {\n      type: Number,\n      default: 1\n    },\n    count: {\n      type: Number,\n      default: 1\n    },\n    showFeedback: {\n      type: Boolean,\n      default: false\n    },\n    showThumbnail: {\n      type: Boolean,\n      default: true\n    },\n    item: { type: Object, default: null },\n    orderBrand: { type: String, default: '' }\n  },\n  methods: {\n    onClickMethod (event) {\n      this.$emit('click', event)\n    },\n    onFeedbackClickedMethod (event) {\n      this.$emit('feedbackClicked', event)\n    }\n  },\n  computed: {\n    formattedIndex: function () {\n      return `${pad(this.index + 1, 2)}`\n    },\n    formattedCount: function () {\n      return `${pad(this.count, 2)}`\n    },\n    formattedPrice: function () {\n      return `$${this.item.price}`\n    }\n  }\n}\n</script>\n\n<style scoped>\n .circle {\n  display: inline-block;\n  border-radius: 60px;\n  border: 0.2px solid #c8c8c8;\n  padding: 0.4em 0.6em;\n\n}\n</style>\n"]}]}