d-flex align-start mt-4\">\n          <v-icon md>\n            mdi-swap-horizontal\n          </v-icon>\n          <o-text class=\"ml-8\">\n            {{ STEP_1 }}\n          </o-text>\n        </div>\n\n        <!-- <div class=\"d-flex align-start mt-4\">\n          <v-icon md>\n            flash_on\n          </v-icon>\n          <o-text class=\"ml-8\">\n            {{ STEP_3 }}\n          </o-text>\n        </div> -->\n\n        <div class=\"d-flex align-start mt-4\">\n          <v-icon md>\n            storefront\n          </v-icon>\n          <o-text class=\"ml-8\">\n            {{ STEP_4 }}\n          </o-text>\n        </div>\n\n        <div class=\"d-flex align-start mt-4\">\n          <v-icon md>\n            paid\n          </v-icon>\n          <o-text class=\"ml-8\">\n            {{ STEP_2 }}\n          </o-text>\n        </div>\n      </v-form>\n    </div>\n  </o-dialog>\n</template>\n\n<script>\nimport { HOW_IT_WORKS } from 'branding/strings.json'\nimport ODialog from '@/components/atoms/ODialog.vue'\nimport OText from '@/components/atoms/OText.vue'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\nimport { GET_POLICY } from '@/store/constants.js'\nimport { mapGetters } from 'vuex'\n\nexport default {\n  props: {\n    visible: Boolean\n  },\n  components: {\n    ODialog,\n    OText,\n    HeaderTitle\n  },\n  data () {\n    return {\n      HOW_IT_WORKS: HOW_IT_WORKS.HOW_IT_WORKS,\n      EASY: HOW_IT_WORKS.EASY,\n      FAST_AND_FREE: HOW_IT_WORKS.FAST_AND_FREE,\n      CONVENIENT: HOW_IT_WORKS.CONVENIENT,\n      STEP_1: HOW_IT_WORKS.HOW_IT_WORKS_STEP_1,\n      STEP_2: HOW_IT_WORKS.HOW_IT_WORKS_STEP_2,\n      STEP_3: HOW_IT_WORKS.HOW_IT_WORKS_STEP_3,\n      STEP_4: HOW_IT_WORKS.HOW_IT_WORKS_STEP_4\n    }\n  },\n  mounted () {\n    this.orderId = ''\n    this.$store\n      .dispatch('order/' + GET_POLICY, null)\n      .then((response) => {\n        if (response) {\n          var refundTimingEstimate = response.policy?.refund_timing_estimate ? response.policy?.refund_timing_estimate : '5-7'\n          this.STEP_2 = HOW_IT_WORKS.HOW_IT_WORKS_STEP_2.replace('$$refundTimingEstimate', refundTimingEstimate)\n        }\n      })\n      .catch((err) => {\n        console.error({ err })\n      })\n      .finally(() => {\n      })\n  },\n  computed: {\n    computed: {\n      ...mapGetters({\n        policies: 'returns/policies'\n      })\n    },\n    show: {\n      get () {\n        return this.visible\n      },\n      set (value) {\n        if (!value) {\n          this.$emit('close')\n        }\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.container {\n  padding-top: 60px;\n}\n</style>\n"]}]}