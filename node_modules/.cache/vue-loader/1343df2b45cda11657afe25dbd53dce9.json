iv>\n    </template>\n    <template slot=\"item\" slot-scope=\"data\">\n      <div>\n        <span class=\"boldText\">{{ data.item.title }}</span>\n        {{ data.item.subtitle }}\n      </div>\n    </template>\n  </o-select>\n</template>\n\n<script>\nimport OSelect from '@/components/atoms/OSelect.vue'\nexport default {\n  name: 'SizeDropdown',\n  components: {\n    OSelect\n  },\n  props: {\n    items: {\n      type: Array,\n      required: true\n    },\n    previouslySelectedOption: {\n      type: Object,\n      required: false,\n      default: () => {}\n    },\n    onChange: {\n      type: Function,\n      required: false,\n      default: () => {}\n    }\n  },\n  data: () => ({\n    reset: false\n  }),\n  methods: {\n    onChangeMethod (event) {\n      this.$emit('onChange', event)\n    },\n    onKeyEnterEvent (event) {\n      this.$emit('onKeyEnter', event)\n    },\n    onPasteMethod (event) {\n      this.$emit('onPaste', event)\n    }\n  },\n  computed: {\n    defaultValue () {\n      const item = this.items.find((e) => e.subtitle === 'Original Purchase')\n      // return the previously selected item by default unless the previously selected item was not of the same color as the default item\n      let defaultItem = this.previouslySelectedOption || item || this.items[0]\n      if (item && this.previouslySelectedOption && item.color !== this.previouslySelectedOption?.color) {\n        defaultItem = item\n      }\n      if (defaultItem && defaultItem.disabled === false) {\n        this.$store.commit('returns/updateReturnReasonSelected', true)\n      }\n      return defaultItem\n    }\n  }\n}\n</script>\n\n<style></style>\n"]}]}