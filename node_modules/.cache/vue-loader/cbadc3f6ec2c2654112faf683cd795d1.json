    />\n      <div\n        class=\"d-flex justify-start mt-4\"\n        v-if=\"$vuetify.breakpoint.mdAndUp\"\n      >\n        <action-button\n          @click.stop=\"onClickMethod\"\n          primary-color\n          x-large\n          :disabled=\"isDisabled\"\n        >\n          {{ SUBMIT_FEEDBACK }}\n        </action-button>\n      </div>\n      <div\n        v-if=\"$vuetify.breakpoint.smAndDown\"\n        class=\"d-flex justify-center\"\n      >\n        <action-button\n          fab\n          fixed\n          bottom\n          block\n          class=\"mb-n4\"\n          primary-color\n          style=\"border-radius: 0em; height: 66px\"\n          :disabled=\"isDisabled\"\n          @click.stop=\"onClickMethod\"\n        >\n          Submit feedback\n        </action-button>\n      </div>\n    </v-form>\n  </o-dialog>\n</template>\n\n<script>\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport OTextArea from '@/components/atoms/OTextArea.vue'\nimport { LEAVE_FEEDBACK } from 'branding/strings.json'\nimport ODialog from '@/components/atoms/ODialog.vue'\nimport { mapGetters } from 'vuex'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\n\nexport default {\n  props: {\n    visible: Boolean\n  },\n  components: {\n    ODialog,\n    ActionButton,\n    OTextArea,\n    HeaderTitle\n  },\n  data () {\n    return {\n      isDisabled: true,\n      feedback: '',\n      TELL_US_MORE: LEAVE_FEEDBACK.TELL_US_MORE,\n      LEAVE_FEEDBACK_ON_YOUR_ORDER: LEAVE_FEEDBACK.LEAVE_FEEDBACK_ON_YOUR_ORDER,\n      SUBMIT_FEEDBACK: LEAVE_FEEDBACK.SUBMIT_FEEDBACK\n    }\n  },\n  computed: {\n    ...mapGetters({\n      currentItemIndex: 'returns/currentItemIndex',\n      selectedItems: 'order/selectedItems'\n    }),\n    item () {\n      return this.selectedItems[this.currentItemIndex]\n    },\n    show: {\n      get () {\n        return this.visible\n      },\n      set (value) {\n        if (!value) {\n          this.$emit('close')\n        }\n      }\n    }\n  },\n  methods: {\n    onChange (e) {\n      this.isDisabled = false\n    },\n    onClickMethod (event) {\n      const sku = this.item.sku\n      this.$store.commit('returns/updateReturnItemFeedback', {\n        feedback: this.feedback,\n        itemId: sku\n      })\n      this.feedback = ''\n\n      this.$store.commit(\n        'returns/updateReason', {\n          reason: '',\n          itemSku: sku\n        })\n      this.$emit('click', event)\n    }\n  }\n}\n</script>\n\n<style scoped>\n  .container {\n    padding-top: 60px;\n  }\n  .v-dialog {\n    border-radius: 0px !important;\n  }\n  .v-card {\n    border-radius: 0px !important;\n  }\n</style>\n"]}]}