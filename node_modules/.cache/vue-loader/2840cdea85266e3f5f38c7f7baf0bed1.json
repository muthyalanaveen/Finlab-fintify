o text-center hidden-md-and-up\"\n      style=\"background-color: #F6F6F6;\"\n    >\n      <v-row class=\"mt-4 mb-4\">\n        <v-col class=\"my-auto\">\n          <o-text\n            xl\n            bold\n          >\n            {{ this.getRefundPrice(item) }}\n          </o-text>\n          <o-text\n            semi-bold\n            xs\n            text-secondary\n            class=\"regularWt\"\n          >\n            {{ ESTIMATED_RETURN }}\n          </o-text>\n        </v-col>\n      </v-row>\n\n      <div style=\"height: 24px;\" class=\"d-flex align-center justify-center\">\n        <v-divider class=\"mx-4 dviderLine\" />\n        <v-img\n          :src=\"require('@/assets/images/downArrow.png')\"\n          max-width=\"16\"\n          max-height=\"16\"\n          alt=\"\"\n          aria-label=\"Refund will be returned to the original payment card\"\n        />\n        <v-divider class=\"mx-4 dviderLine\" />\n      </div>\n\n      <v-row class=\"mt-4 mb-4\">\n        <v-col class=\"my-auto\">\n          <v-img\n            v-if=\"!isGiftReturn\"\n            :src=\"require('@/assets/images/payment_card.png')\"\n            max-width=\"48\"\n            max-height=\"32\"\n            class=\"mx-auto mt-3\"\n            alt=\"\"\n          />\n          <v-img\n            v-else\n            :src=\"require('@/assets/images/gift-card-icon.png')\"\n            max-width=\"48\"\n            max-height=\"32\"\n            class=\"mx-auto mt-3\"\n            alt=\"\"\n          />\n          <o-text\n            v-if=\"isGiftReturn\"\n            class=\"mt-1\"\n            sm\n            bold\n          >\n            Gift card\n          </o-text>\n          <o-text\n            v-if=\"!isGiftReturn\"\n            class=\"mt-1\"\n            sm\n            bold\n          >\n            Original\n          </o-text>\n          <o-text\n            v-if=\"!isGiftReturn\"\n            class=\"mt-n1\"\n            sm\n            bold\n          >\n            Payment\n          </o-text>\n        </v-col>\n      </v-row>\n    </v-col>\n\n    <v-row\n      class=\"mx-auto align-center hidden-sm-and-down\"\n      style=\"background-color: #F6F6F6;\"\n    >\n      <v-col class=\"mt-8 mb-8 ml-4\">\n        <div>\n          <o-text\n            class=\"\"\n            xl\n            bold\n          >\n            {{ this.getRefundPrice(item) }}\n          </o-text>\n          <o-text\n            semi-bold\n            text-secondary\n            xs\n            class=\"regularWt\"\n          >\n            {{ ESTIMATED_RETURN }}\n          </o-text>\n        </div>\n      </v-col>\n\n      <v-col style=\"height: 24px;\" class=\"d-flex align-center justify-center\">\n        <v-img\n          :src=\"require('@/assets/images/rightArrow.png')\"\n          max-width=\"16\"\n          max-height=\"16\"\n          alt=\"\"\n          aria-label=\"Refund will be returned to the original payment card\"\n        />\n      </v-col>\n\n      <div class=\"mt-0 mb-4 mr-n4\" style=\"max-width: 165px\">\n        <v-row class=\"\">\n          <v-col :class=\"isGiftReturn ? 'mr-n8' : ''\">\n            <o-text\n              v-if=\"isGiftReturn\"\n              sm\n              bold\n            >\n              Gift\n            </o-text>\n            <o-text\n              v-if=\"isGiftReturn\"\n              class=\"mt-n1\"\n              sm\n              bold\n            >\n              Card\n            </o-text>\n            <o-text\n              v-if=\"!isGiftReturn\"\n              sm\n              bold\n            >\n              Original\n            </o-text>\n            <o-text\n              v-if=\"!isGiftReturn\"\n              class=\"mt-n1\"\n              sm\n              bold\n            >\n              Payment\n            </o-text>\n          </v-col>\n          <v-col class=\"\">\n            <v-img\n              v-if=\"!isGiftReturn\"\n              :src=\"require('@/assets/images/payment_card.png')\"\n              max-width=\"48\"\n              max-height=\"32\"\n              class=\"ml-n6\"\n              alt=\"\"\n            />\n            <v-img\n              v-else\n              :src=\"require('@/assets/images/gift-card-icon.png')\"\n              max-width=\"48\"\n              height=\"35\"\n              class=\"ml-n6\"\n              alt=\"\"\n            />\n          </v-col>\n        </v-row>\n      </div>\n    </v-row>\n\n    <o-text\n      :class=\"{\n        'mt-2': $vuetify.breakpoint.smAndDown,\n        'mt-6': $vuetify.breakpoint.mdAndUp,\n      }\"\n      semi-bold\n      xs\n      text-secondary\n      class=\"regularWt newLine\"\n    >\n      {{ !isGiftReturn ? REFUND_CLEAR_MESSAGE : GIFT_CARD_REFUND_MESSAGE }}\n    </o-text>\n    <action-button\n      @click.stop=\"updateRefundItem\"\n      class=\"mt-6\"\n      :block=\"$vuetify.breakpoint.smAndDown\"\n      primary-color\n      aria-label=\"Go to Return Methods Page\"\n      large\n      :loading=\"isMapLoading\"\n      x-large\n    >\n      {{ NEXT }}\n    </action-button>\n  </div>\n</template>\n\n<script>\nimport ActionButton from '@/components/atoms/ActionButton.vue'\nimport OText from '@/components/atoms/OText.vue'\nimport { EXCHANGE_REFUND, ORDER_LOOKUP, PAGE_TITLES } from 'branding/strings.json'\nimport { mapGetters } from 'vuex'\nimport { UPDATE_REFUND_LIST } from '@/store/constants.js'\nimport HeaderTitle from '../molecules/HeaderTitle.vue'\nimport { getIsGift } from '@/store/sessionStorage.js'\n\nexport default {\n  name: 'RefundView',\n  components: {\n    ActionButton,\n    OText,\n    HeaderTitle\n  },\n  computed: {\n    ...mapGetters({\n      reason: 'returns/reason',\n      currentItemIndex: 'returns/currentItemIndex',\n      policies: 'returns/policies',\n      selectedItems: 'order/selectedItems'\n    }),\n    item () {\n      return this.selectedItems[this.currentItemIndex]\n    },\n    isGiftReturn () {\n      return getIsGift()\n    }\n  },\n  data: () => ({\n    GET_A_REFUND: EXCHANGE_REFUND.GET_A_REFUND,\n    REFUND_CLEAR_MESSAGE: '',\n    ESTIMATED_RETURN: EXCHANGE_REFUND.ESTIMATED_RETURN,\n    NEXT: ORDER_LOOKUP.NEXT,\n    GIFT_CARD_REFUND_MESSAGE: EXCHANGE_REFUND.GIFT_CARD_REFUND_MESSAGE\n  }),\n  mounted () {\n    document.title = PAGE_TITLES.REFUND\n    var refundTimingEstimate = this.policies.refund_timing_estimate ? this.policies.refund_timing_estimate : '5-7'\n    this.REFUND_CLEAR_MESSAGE = EXCHANGE_REFUND.REFUND_CLEAR_MESSAGE.replace('$$refundTimingEstimate', refundTimingEstimate)\n    this.GIFT_CARD_REFUND_MESSAGE = EXCHANGE_REFUND.GIFT_CARD_REFUND_MESSAGE.replace('$$refundTimingEstimate', refundTimingEstimate)\n  },\n  methods: {\n    updateRefundItem: function (event) {\n      this.$store.commit('order/' + UPDATE_REFUND_LIST, { ...this.item, reason: this.reason })\n      this.$emit('click', event)\n    },\n    getRefundPrice (item) {\n      const price = item ? item.price * item.quantity : 0\n      return `$${Number(price).toFixed(2)}`\n    }\n  },\n  props: {\n    isMapLoading: {\n      type: Boolean,\n      required: true\n    },\n    isRefundOnly: {\n      type: Boolean,\n      required: false\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n.dviderLine{\n  height: 2px;\n  color: #545454;\n}\n\n.newLine ::v-deep {\n  white-space: pre-line;\n}\n\n</style>\n"]}]}