nimport { setSelectedReturnMethod } from '@/controllers/MapController.js'\nimport ShippingLocationCard from '@/components/organism/ShippingLocationCard.vue'\nimport { mapGetters } from 'vuex'\n\nexport default {\n  name: 'MapTiles',\n  components: { ShippingLocationCard },\n  data: () => ({\n  }),\n  mounted () {},\n  computed: {\n    ...mapGetters({\n      order: 'order/order'\n    })\n  },\n  props: {\n    carriers: {\n      type: Array,\n      required: true\n    },\n    scrollToView: {\n      type: Function,\n      required: true\n    }\n  },\n  methods: {\n    handleCarrierSelection (carrier) {\n      setSelectedReturnMethod(carrier)\n      this.$router.push('/orders/' + this.order.order_hash + '/review')\n    }\n  }\n}\n</script>\n\n<style scoped>\n  .virtual-scroll {\n    overflow-y: scroll;\n    height: 750px;\n    padding: 0;\n  }\n  .virtual-scroll::-webkit-scrollbar, .virtual-scroll-sm::-webkit-scrollbar {\n    display: none;  /* Safari and Chrome */\n  }\n  .virtual-scroll-sm {\n    display: flex;\n    overflow-x: scroll;\n    height: unset;\n    overflow-y: unset;\n    padding-bottom: 35px;\n  }\n  .list-item {\n    padding: 0;\n  }\n</style>\n"]}]}