{"remainingRequest":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/pages/ReturnMethods.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/src/components/pages/ReturnMethods.vue","mtime":1649977023158},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/cache-loader/dist/cjs.js","mtime":1649977086874},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/babel-loader/lib/index.js","mtime":1649977089120},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/cache-loader/dist/cjs.js","mtime":1649977086874},{"path":"/Users/naveenmuthyala/Documents/Projects/returns_portal_ui/returns_portal_ui/node_modules/vue-loader/lib/index.js","mtime":1649977089802}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["ReturnMethods.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkHA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"ReturnMethods.vue","sourceRoot":"src/components/pages","sourcesContent":["<template>\n  <div class=\"mb-16\">\n    <div\n      :class=\"{\n        'ml-3 mt-2': $vuetify.breakpoint.smAndDown,\n        'ml-8 mt-8': $vuetify.breakpoint.mdAndUp,\n      }\"\n    >\n      <back-button />\n    </div>\n    <div v-if=\"!this.returnMethods || this.returnMethods == 0\" class=\"text-center mt-16\">\n      <div class=\"mt-4\">\n        <o-text\n          lg\n          bold\n        >\n          No return methods found within 10 miles\n        </o-text>\n      </div>\n\n      <div class=\"mt-4\">\n        <o-text\n          md\n          text-blue\n        >\n          Have a question? Give us a call.\n        </o-text>\n      </div>\n      <div class=\"mt-2\">\n        <o-text\n          md\n          text-blue\n        >\n          {{ this.customerSupportContact }}\n        </o-text>\n      </div>\n    </div>\n    <div\n      v-else\n      :class=\"{\n        'mt-2': $vuetify.breakpoint.smAndDown,\n        'mt-n12': $vuetify.breakpoint.mdAndUp,\n      }\"\n    >\n      <div\n        style=\"max-width: 664px; margin: auto\"\n        :class=\"{ 'mx-auto': $vuetify.breakpoint.mdAndUp }\"\n      >\n        <v-row\n          :class=\"{\n            'ml-4': $vuetify.breakpoint.smAndDown,\n            'mx-auto': $vuetify.breakpoint.mdAndUp,\n          }\"\n          align=\"center\"\n        >\n          <header-title>\n            {{ CHOOSE_METHOD }}\n          </header-title>\n        </v-row>\n        <v-row\n          justify=\"center\"\n          class=\"mb-4 mt-4 mx-auto\"\n          :class=\"{}\"\n        >\n          <shipping-carrier-card\n            is-show-case\n            :carrier=\"showCaseCarrier\"\n            :handle-carrier-selection=\"handleCarrierSelection\"\n          />\n        </v-row>\n        <o-text\n          bold\n          :class=\"{\n            'ml-4': $vuetify.breakpoint.smAndDown,\n            'mx-auto': $vuetify.breakpoint.mdAndUp,\n          }\"\n          v-if=\"accordionOptions.length > 0\"\n        >\n          Other options\n        </o-text>\n        <v-row justify=\"center\" class=\"mb-4 mt-4\">\n          <accordion\n            :options=\"accordionOptions\"\n            :handle-selection=\"handleAccordionSelection\"\n            :opened-panel=\"computeOpenedPanel\"\n          >\n            <shipping-carrier-card\n              :carrier=\"openAccordionCarrier\"\n              :handle-carrier-selection=\"handleCarrierSelection\"\n            />\n          </accordion>\n        </v-row>\n      </div>\n      <o-dialog\n        :visible=\"isModalOpen\"\n        is-full-screen\n        @close=\"closeModal\"\n      >\n        <return-locations\n          v-if=\"activeModal === DIALOG_CARRIER_LOCATIONS\"\n          show-search-area\n          :title=\"EXPLORE_LOCATIONS\"\n          :return-type=\"getReturnMethodType()\"\n          :show-select-locations=\"false\"\n          from-return-methods\n        />\n        <return-store-info v-if=\"activeModal === DIALOG_CARRIER_INFORMATION\" />\n        <search-return-location v-if=\"activeModal === DIALOG_SEARCH_LOCATION\" source=\"returnMethods\" />\n      </o-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport OText from '@/components/atoms/OText.vue'\nimport Accordion from '@/components/molecules/Accordion.vue'\nimport ShippingCarrierCard from '@/components/organism/ShippingCarrierCard.vue'\nimport ReturnLocations from '@/components/organism/ReturnLocations.vue'\nimport ODialog from '@/components/atoms/ODialog.vue'\nimport ReturnStoreInfo from '@/components/organism/ReturnStoreInfo.vue'\nimport BackButton from '@/components/atoms/BackButton.vue'\nimport HeaderTitle from '@/components/molecules/HeaderTitle.vue'\nimport {\n  RETURN_METHOD_SCREENS,\n  RETURN_METHOD_DIALOGS\n} from 'branding/strings.json'\nimport { mapGetters, mapMutations } from 'vuex'\nimport SearchReturnLocation from '@/components/organism/SearchReturnLocation.vue'\nimport {\n  DIALOG_SEARCH_LOCATION,\n  DIALOG_CARRIER_INFORMATION,\n  DIALOG_CARRIER_LOCATIONS\n} from '@/store/constants'\nimport { getReturnsHelperText, getDistanceValue } from '@/utils'\n\nexport default {\n  components: {\n    ShippingCarrierCard,\n    Accordion,\n    OText,\n    ReturnLocations,\n    ODialog,\n    ReturnStoreInfo,\n    BackButton,\n    HeaderTitle,\n    SearchReturnLocation\n  },\n  computed: {\n    ...mapGetters({\n      order: 'order/order',\n      isModalOpen: 'returns/getIsModalOpen',\n      activeModal: 'returns/getActiveModal',\n      returnMethods: 'returns/returnMethods',\n      selectedCarrier: 'returns/getSelectedCarrier',\n      policies: 'returns/policies'\n    }),\n    returnMethodsWithinDistance () {\n      return this.returnMethods\n        .slice()\n        .sort(\n          (method1, method2) =>\n            method1.returnMethodRank - method2.returnMethodRank ||\n            getDistanceValue(method1?.distance) -\n              getDistanceValue(method2?.distance)\n        )\n    },\n    showCaseCarrier () {\n      return this.returnMethodsWithinDistance[0]\n    },\n    accordionOptions () {\n      const accordionReturnMethods = this.returnMethodsWithinDistance.slice(1)\n      this.closeAllPanels()\n      return accordionReturnMethods.map((opt) => ({\n        primaryText: `${getReturnsHelperText(opt.type)} ${opt.customer_facing_carrier}`,\n        secondaryText: `${\n          opt.free_shipping ? 'Free' : `$${opt.flat_fee.amount}`\n        }`,\n        id: opt.id\n      }))\n    },\n    computeOpenedPanel () {\n      return this.openedPanel\n    }\n  },\n  methods: {\n    ...mapMutations({\n      openModal: 'returns/openModal',\n      closeModal: 'returns/closeModal',\n      setSelectedCarrier: 'returns/setSelectedCarrier',\n      setSelectedReturnMethod: 'returns/setSelectedReturnMethod'\n    }),\n    openPanel (index) {\n      this.openedPanel = index\n    },\n    closeAllPanels () {\n      this.openedPanel = null\n    },\n    handleCarrierSelection (carrier) {\n      this.setSelectedReturnMethod(carrier)\n      this.$router.push('/orders/' + this.order.order_hash + '/review')\n      // this.$router.push({ path: '/orders/' + this.order.order_hash + '/review', query: { title: 'Review your return' } })\n    },\n    handleAccordionSelection ({ id }) {\n      const option = this.returnMethods.find((m) => m.id === id)\n      this.openAccordionCarrier = option\n      this.setSelectedCarrier(option)\n      this.openPanel(option)\n    },\n    getReturnMethodType: function () {\n      return this.selectedCarrier.type\n    }\n  },\n  data () {\n    return {\n      openAccordionCarrier: null,\n      openedPanel: null,\n      customerSupportContact: '',\n      CHOOSE_METHOD: RETURN_METHOD_SCREENS.CHOOSE_METHOD,\n      DIALOG_SEARCH_LOCATION,\n      DIALOG_CARRIER_INFORMATION,\n      DIALOG_CARRIER_LOCATIONS,\n      EXPLORE_LOCATIONS: RETURN_METHOD_DIALOGS.EXPLORE_LOCATIONS\n    }\n  },\n  mounted () {\n    this.customerSupportContact = this.policies?.retailer_info?.customer_support_contact\n  }\n}\n</script>\n\n<style scoped>\n</style>\n"]}]}